#	etc.printcap
#
#  printcap on network host.  Uses remote printing service.
#
#  Change all "ajfcal" to the hostname of the print server

lp|ajfcal postscript printer:\
	:lp=:rm=ajfcal:sd=/usr/spool/lpd/lp:lf=/usr/adm/lpd-errs: \
	:rp=lp

2up|postscript 2 up printer:\
	:lp=:rm=ajfcal:sd=/usr/spool/lpd/lp:lf=/usr/adm/lpd-errs: \
	:rp=2up

4up|postscript 4 up printer:\
	:lp=:rm=ajfcal:sd=/usr/spool/lpd/lp:lf=/usr/adm/lpd-errs: \
	:rp=4up

132|landscape, 132 col 60 lin:\
	:lp=:rm=ajfcal:sd=/usr/spool/lpd/lp:lf=/usr/adm/lpd-errs: \
	:rp=132

acct|general accounting page layout:\
	:lp=:rm=ajfcal:sd=/usr/spool/lpd/lp:lf=/usr/adm/lpd-errs: \
	:rp=acct

src|programme source listings:\
	:lp=:rm=ajfcal:sd=/usr/spool/lpd/lp:lf=/usr/adm/lpd-errs: \
	:rp=src

post|postscript source listings:\
	:lp=:rm=ajfcal:sd=/usr/spool/lpd/lp:lf=/usr/adm/lpd-errs: \
	:rp=post

legal|standard legal paper:\
	:lp=:rm=ajfcal:sd=/usr/spool/lpd/lp:lf=/usr/adm/lpd-errs: \
	:rp=legal

sheet|large spreadsheets, legal paper:\
	:lp=:rm=ajfcal:sd=/usr/spool/lpd/lp:lf=/usr/adm/lpd-errs: \
	:rp=sheet
