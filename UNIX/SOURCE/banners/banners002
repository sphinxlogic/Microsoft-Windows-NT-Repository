X" @@@@@   @@@@  @@@@@   @@@@   @@@@@ @    @ @    @ @    @",
X" @    @ @    @ @    @ @         @   @    @ @    @ @    @",
X" @    @ @    @ @    @  @@@@     @   @    @ @    @ @    @",
X" @@@@@  @  @ @ @@@@@       @    @   @    @ @    @ @ @@ @",
X" @      @   @  @   @  @    @    @   @    @  @  @  @@  @@",
X" @       @@@ @ @    @  @@@@     @    @@@@    @@   @    @",
X 
X"                       @@@     @     @@@   @@    @ @ @ @",
X" @    @  @   @ @@@@@@ @        @        @ @  @  @ @ @ @ ",
X"  @  @    @ @      @  @        @        @     @@ @ @ @ @",
X"   @@      @      @  @@                 @@        @ @ @ ",
X"   @@      @     @    @        @        @        @ @ @ @",
X"  @  @     @    @     @        @        @         @ @ @ ",
X" @    @    @   @@@@@@  @@@     @     @@@         @ @ @ @"};
X
X
main(argc,argv)
X     int argc;
X     char *argv[];
X{
X  int a,b,c,len,ind;
X  char line[80];
X
X  for(argv++;--argc;argv++){
X    len=strlen(*argv);
X    if(len>10)
X      len=10;
X    for(a=0;a<7;a++){
X      for(b=0;b<len;b++){
X        if((ind=(*argv)[b]-' ')<0)
X          ind=0;
X        for(c=0;c<7;c++){
X          line[b*8+c] = glyphs[(ind/8*7)+a][(ind%8*7)+c];
X        }
X        line[b*8+7] = ' ';
X      }
X      for(b=len*8-1;b>=0;b--){
X        if(line[b]!=' ')
X          break;
X        line[b]='\0';
X      }
X      puts(line);
X    }
X    puts("");
X  }
X}
END_OF_FILE
