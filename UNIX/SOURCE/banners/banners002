exec_compress( i, max, end)
int i,max,end;
X{
X	int j,k;
X
X	for(j=0 ; j<=max ; j++ ){
X		for(k=i; k<end-1; k++ )
X			board[j][k]=board[j][k+1];
X		board[j][k] = 0 ;
X		}
