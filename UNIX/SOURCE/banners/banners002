space_check(max)
X{
X	int i,j;
X
X	for(i=0;i<=max ; i++ )
X		for( j=0; j< (CW*2+1)*MC-1; j++)
X			if(board[i][j] == 'A')  board[i][j]=' ';
X}
board_compress( max )
int max;
X{
X	int i,j;
X	int end;
X	int sw = 0;
