X		tablet[j] = 0;
X		if( strClen(tablet) ==0 ){ 
X			sw++; 
X			if (sw==3){
X				exec_compress(i,max,end);
X				sw--;
X				end--;
X				i--;  } /* if */
X				}  /* if */
X		else sw=0;
X		}  /* for i */
X}
