X				putchar(' ');
X		for (sp = msg; *sp; sp++) {
X			if((t = *sp) == '\\') {
X				for(t=0,j=0;j<3 && *++sp>='0' && *sp<='7'; j++)
X					t = t*8 + *sp - '0';
X				if(j == 0) {
X					t = *sp;
X					if(t == 'h') {
X						hebrew = 1;
X						continue;
X					} else if(t == 'e') {
X						hebrew = 0;
X						continue;
X					}
X				} else if(j < 3)
X					sp--;
X			} else
X				t = *sp;
X			if(hebrew) {
X				if(shadow && i<7)
X					t1 = htab[t & 037][i+1];
X				t = htab[t & 037][i];
X			} else {
X				if(shadow && i<7)
X					t1 = chrtab[t & 0177][i+1];
X				t = chrtab[t & 0177][i];
X			}
X			if(shadow) {
X				for (j1=0, j=0200; (j|j1)!=0; j1=j, j >>= 1)
X					putchar((j&t) ? (j&t1) ? ' ' : '_'
X					 : (j1&t) ? '/' : ' ');
X			} else {
X				for (j=0200; j!=0; j >>= 1)
X					putchar((j&t) ? '#' : ' ');
X			}
X		}
X		putchar('\n');
X	}
X}
