X/* leb : Large English Banner
X *
X * Who the heck wrote original Pascal version?
X *
X * C version by Han, Yun-Su
X * 880505 KAIST undergraduate Dept of Life Science
X */
X
X#include <stdio.h>
X
main(argc, argv)
int argc;
char **argv;
X{
X	char line[256];
X
X	if (argc < 2)	{
X		fprintf(stderr, "&:^)\n");
X		exit(1);
X		}
X	getline(line, argc, argv);
X	lowerline(line);
X	putline(line);
X}
X
getline(s, n, v)
char *s;
int n;
char *v[];
X{
X	int i, j;
X
X	strcpy(s, "");
X	for (i = 1; i < n; i++, s++)	{
X		for (j = 0; v[i][j] != '\0'; j++, s++)
X			*s = v[i][j];
X		*s = ' ';
X		}
X	s--;
X	*s = '\0';
X}
X
lowerline(s)
char *s;
X{
X	char *p;
X
X	for (p = s; *p != '\0'; p++)
X		if (*p >= 'A' && *p <= 'Z')
X			*p = *p - 'A' + 'a';
X}
X
X#define	Height		6	/* Height of 3D Font */
X#define	Blank		"     "
putline(s)
char *s;
X{
X	static char *data[] = {
X		"     __      ",
X		"    /  \\     ",
