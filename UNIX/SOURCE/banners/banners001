X		" ><",
X		"   ",
X		"    ",
X		"    ",
X		" \\_/",
X		" _/ ",
X		"   ",
X		" _ ",
X		" /_",
X		"   ",
X		"   ",
X		""
X		};
X	int i;
X	char *p;
X
X	for (i = 0; i < Height; i++)	{
X		for (p = s; *p != '\0'; p++)
X			if (*p >= 'a' && *p <= 'z')
X				printf("%s", lower[(*p - 'a') * Height + i]);
X			else if (*p >= 'A' && *p <= 'Z')
X				printf("%s", upper[(*p - 'A') * Height + i]);
X			else
X				printf(Blank);
X		printf("\n");
X		}
X}
