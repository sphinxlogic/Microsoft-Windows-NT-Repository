X		exit(1);
X		}
X	getline(line, argc, argv);
X	putline(line);
X}
X
getline(s, n, v)
char *s;
int n;
char *v[];
X{
X	int i, j;
X
X	strcpy(s, "");
X	for (i = 1; i < n; i++, s++)	{
X		for (j = 0; v[i][j] != '\0'; j++, s++)
X			*s = v[i][j];
X		*s = ' ';
X		}
X	s--;
X	*s = '\0';
X}
X
X#define	Height		4	/* Height of Font */
X#define	Blank		"     "
putline(s)
char *s;
X{
X	static char *upper[] = {
X		"     ",
