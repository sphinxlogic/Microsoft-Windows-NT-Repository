DEFINES= $(JPEG) $(JPEG_INCLUDES) $(TIFF) $(TIFF_INCLUDES)
LOCALDIR=/opt/bin
SYS_LIBRARIES= $(XLIB) $(JPEG_LIBRARIES) $(TIFF_LIBRARIES) -lm

EXTRA_INCLUDES= -I..

MagickObjects= ../X.o ../image.o ../shear.o ../quantize.o ../colors.o \
  ../signature.o ../compress.o ../alien.o ../PreRvIcccm.o

COMBINEObjects= combine.o $(MagickObjects)
CONVERTObjects= convert.o $(MagickObjects)
MOGRIFYObjects= mogrify.o $(MagickObjects)
MIFFtoSTEREOSources= MIFFtoSTEREO.c
MIFFtoSTEREOObjects= MIFFtoSTEREO.o $(MagickObjects)

PROGRAMS= mogrify convert combine MIFFtoSTEREO

all:: $(PROGRAMS)

mogrify: $(MOGRIFYObjects)
	$(RM) $@
	$(CC) -o $@ $(MOGRIFYObjects) $(LDOPTIONS)   $(LDLIBS)   $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) mogrify

install:: mogrify
	@if [ -d $(DESTDIR)$(LOCALDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LOCALDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  mogrify $(DESTDIR)$(LOCALDIR)

install.man:: mogrify.man
	@if [ -d $(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(MANDIR)); fi
	$(NROFF) -man mogrify.man > $(MANDIR)/mogrify.$(MANSUFFIX)
	chmod $(MANFLAGS) $(MANDIR)/mogrify.$(MANSUFFIX)

convert: $(CONVERTObjects)
	$(RM) $@
	$(CC) -o $@ $(CONVERTObjects) $(LDOPTIONS)   $(LDLIBS)   $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) convert

install:: convert
	@if [ -d $(DESTDIR)$(LOCALDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LOCALDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  convert $(DESTDIR)$(LOCALDIR)

install.man:: convert.man
	@if [ -d $(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(MANDIR)); fi
	$(NROFF) -man convert.man > $(MANDIR)/convert.$(MANSUFFIX)
	chmod $(MANFLAGS) $(MANDIR)/convert.$(MANSUFFIX)

combine: $(COMBINEObjects)
	$(RM) $@
	$(CC) -o $@ $(COMBINEObjects) $(LDOPTIONS)   $(LDLIBS)   $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) combine

install:: combine
	@if [ -d $(DESTDIR)$(LOCALDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LOCALDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  combine $(DESTDIR)$(LOCALDIR)

install.man:: combine.man
	@if [ -d $(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(MANDIR)); fi
	$(NROFF) -man combine.man > $(MANDIR)/combine.$(MANSUFFIX)
	chmod $(MANFLAGS) $(MANDIR)/combine.$(MANSUFFIX)

MIFFtoSTEREO: $(MIFFtoSTEREOObjects)
	$(RM) $@
	$(CC) -o $@ $(MIFFtoSTEREOObjects) $(LDOPTIONS)   $(LDLIBS)   $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) MIFFtoSTEREO

install:: MIFFtoSTEREO
	@if [ -d $(DESTDIR)$(LOCALDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LOCALDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  MIFFtoSTEREO $(DESTDIR)$(LOCALDIR)

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

