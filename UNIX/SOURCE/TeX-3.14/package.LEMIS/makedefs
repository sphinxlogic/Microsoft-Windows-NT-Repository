xmkmf=
depend=
domake=make
doinstall="make~install make~install-formats make~install-bases make~install-manpages make~install-dviutil make~install-dvialw make~install-dvibit make~install-dvican make~install-dvie72 make~install-dvieps make~install-dvigd make~install-dviimp make~install-dvijep make~install-dvijet make~install-dvil3p make~install-dvil75 make~install-dvim72 make~install-dvimac make~install-dvimpi make~install-dvio72 make~install-dvioki make~install-dviprx make~install-dvitos"
doclean="make~clean"
TREES="/opt"
STRIP=yes
export STRIP
