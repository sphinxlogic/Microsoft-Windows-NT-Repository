# Complete installation of xfig
# Greg Lehey, LEMIS, 14 May 1993
cp xfig.pm /usr/X/desktop/PackageMgr/xfig
cp xfig.cdb /usr/X/lib/classdb/Xfig
nroff -man Doc/xfig.man | compress > /opt/man/cat1/xfig.1.Z
chmod 644 /usr/X/desktop/PackageMgr/xfig /usr/X/lib/classdb/Xfig /opt/man/cat1/xfig.1.Z
chown bin /usr/X/desktop/PackageMgr/xfig /usr/X/lib/classdb/Xfig /opt/man/cat1/xfig.1.Z
chgrp bin /usr/X/desktop/PackageMgr/xfig /usr/X/lib/classdb/Xfig /opt/man/cat1/xfig.1.Z
