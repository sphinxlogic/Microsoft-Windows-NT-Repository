#
# xbomb is done by hand
#
XBOMB_SRCS = main.c map.c applications.c actions.c graphics.c solvers.c score.c
XBOMB_OBJS = main.o map.o applications.o actions.o graphics.o solvers.o score.o

all:: xbomb

xbomb: $(XBOMB_OBJS)
	$(RM) $@
	$(CC) -o $@ $(XBOMB_OBJS) $(LDOPTIONS)  $(LDLIBS) $(XLIB) $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) xbomb

install:: xbomb
	@if [ -d $(DESTDIR)$(EXEDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(EXEDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS) $(XBOMBFLAGS) xbomb $(DESTDIR)$(EXEDIR)

install.man:: xbomb.man
	@if [ -d $(MANSOURCEPATH)$(GAMESMANSUFFIX) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(MANSOURCEPATH)$(GAMESMANSUFFIX)); fi
	$(INSTALL) -c $(INSTMANFLAGS) xbomb.man $(MANSOURCEPATH)$(GAMESMANSUFFIX)/xbomb.$(GAMESMANSUFFIX)

clean::
	$(RM) xbomb

