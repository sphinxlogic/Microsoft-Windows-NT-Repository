! These are some of the resources you can fiddle with for xmris
! The defaults are shown in the man page.
! colour stuff in is Xmris-color.ad

Xmris.up:			apostrophe
Xmris.down:			slash
Xmris.left:			z
Xmris.right:			x
Xmris.throw:			space
Xmris.pause:			p
Xmris.quit:			q
Xmris.iconize:			i
Xmris.keyboard:			k
Xmris.dir:			/var/tmp
Xmris.swap:			no
Xmris.mono:			no
Xmris.leavepause:		yes
Xmris.gender:			he
Xmris.font:			-*-courier-*-r-*-*-18-*-*-*-*-*-*-*
