.c.o:
	$(RM) $@
	$(CC) -c $(CFLAGS) $(_NOOP_) $*.c

all:: libdix.a

libdix.a: $(OBJS)
	$(RM) $@
	$(AR) $@ $(OBJS)

lintlib:: llib-ldix.ln

llib-ldix.ln: $(SRCS)
	$(RM) $@
	$(LINT) $(LINTLIBFLAG)dix $(LINTFLAGS) $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

all:: $(OBJ3) $(OBJ4)

globals.o:  globals.c $(ICONFIGFILES)
	$(RM) $@
	$(CC) -c $(CFLAGS)  $(SITE_DEFINES) $*.c

main.o:  main.c $(ICONFIGFILES)
	$(RM) $@
	$(CC) -c $(CFLAGS)  $(VENDOR_DEFINES) $*.c

dixfonts_m.c: dixfonts.c
	$(RM) $@
	$(LN) $? $@

dixfonts_m.o: dixfonts_m.c
	$(RM) $@
	$(CC) -c $(CFLAGS) -DX386MONOVGA -DBANKEDMONOVGA $*.c

depend:: dixfonts_m.c

clean::
	$(RM) dixfonts_m.c

install.linkkit:: libdix.a
	@if [ -d $(DESTDIR)$(LINKKITDIR)/lib ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LINKKITDIR)/lib); fi
	$(INSTALL) -c $(INSTLIBFLAGS) libdix.a $(DESTDIR)$(LINKKITDIR)/lib

install.linkkit:: dixfonts.o
	@if [ -d $(DESTDIR)$(LINKKITDIR)/lib ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LINKKITDIR)/lib); fi
	$(INSTALL) -c $(INSTDATFLAGS) dixfonts.o $(DESTDIR)$(LINKKITDIR)/lib

install.linkkit:: dixfonts_m.o
	@if [ -d $(DESTDIR)$(LINKKITDIR)/lib ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LINKKITDIR)/lib); fi
	$(INSTALL) -c $(INSTDATFLAGS) dixfonts_m.o $(DESTDIR)$(LINKKITDIR)/lib

depend:: $(DEPEND)

$(DEPEND):
	@echo "checking $@ over in $(DEPENDSRC) first..."; \
	cd $(DEPENDSRC); $(MAKE); \
	echo "okay, continuing in $(CURRENT_DIR)"

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

# -------------------------------------------------------------------------
# common rules for all Makefiles - do not edit

emptyrule::

clean::
	$(RM_CMD) "#"*

Makefile:: $(IMAKE)

$(IMAKE):
	@(cd $(IMAKESRC); if [ -f Makefile ]; then \
	echo "checking $@ in $(IMAKESRC) first..."; $(MAKE) all; else \
	echo "bootstrapping $@ from Makefile.ini in $(IMAKESRC) first..."; \
	$(MAKE) -f Makefile.ini BOOTSTRAPCFLAGS="$(BOOTSTRAPCFLAGS)"; fi; \
	echo "okay, continuing in $(CURRENT_DIR)")

Makefile::
	-@if [ -f Makefile ]; then set -x; \
	$(RM) Makefile.bak; $(MV) Makefile Makefile.bak; \
	else exit 0; fi
	$(IMAKE_CMD) -DTOPDIR=$(TOP) -DCURDIR=$(CURRENT_DIR)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

# -------------------------------------------------------------------------
# empty rules for directories that do not have SUBDIRS - do not edit

install::
	@echo "install in $(CURRENT_DIR) done"

install.man::
	@echo "install.man in $(CURRENT_DIR) done"

install.linkkit::
	@echo "install.linkkit in $(CURRENT_DIR) done"

