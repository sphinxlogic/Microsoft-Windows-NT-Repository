CLASS xfig_exec
BEGIN
	_CLASSNAME	'xfig executable';
	_FILETYPE	EXEC;
	_PATTERN	"xfig";
	_ICONFILE	draw2.icon;
	_DFLTICONFILE	exec.icon;
	MENU _Open	'exec "%F" &';
	_DROP		'exec "%F" "%S" &';
END

CLASS xfig_data
BEGIN
	_CLASSNAME	'xfig data';
	_FILETYPE	"DATA";
	_PATTERN	"*.[fF][iI][gG]";
	_ICONFILE	drawfile2.icon;
	_DFLTICONFILE	datafile.icon;
	MENU _Open	'exec /usr/X/bin/xfig "%F" &';
END
