10 frame off
   clear
   declare integer i
   midpoint = 12
   scalex   = 1/12
   scaley   = 4

   print at midpoint, 1: repeat$('-', 80);

   for i=1 to 80
      print at sin(i * scalex) * scaley + midpoint, i: '.';
      print at cos(i * scalex) * scaley + midpoint, i: '=';
      print at tan(i * scalex) * scaley + midpoint, i: '*';
   next i

   print at 1,1:
