DEFINE VERB NEWMAIL
	IMAGE	"SYS$DISK:[]NEWMAIL"
	NOPARAMETERS
	QUALIFIER    FULL
			NEGATABLE
	QUALIFIER    OUTPUT
			NEGATABLE
			VALUE(TYPE=$FILE, DEFAULT="SYS$OUTPUT")
	QUALIFIER    SAVE_COUNT
			NEGATABLE
			VALUE(TYPE=$OUTSYM,DEFAULT="NEWMAIL$COUNT")
	QUALIFIER    USER
			VALUE(REQUIRED)
			NONNEGATABLE
	DISALLOW  FULL AND USER
