SUBDIRS= magick

all::
	@case '${MFLAGS}' in *[ik]*) set +e;; esac; \
	for i in $(SUBDIRS) ;\
	do \
		(cd $$i ; echo "making" all "in $(CURRENT_DIR)/$$i..."; \
			$(MAKE) $(MFLAGS) CCOPTIONS='$(CCOPTIONS)' all); \
	done

depend::
	@case '${MFLAGS}' in *[ik]*) set +e;; esac; \
	for i in $(SUBDIRS) ;\
	do \
		(cd $$i ; echo "depending" "in $(CURRENT_DIR)/$$i..."; \
			$(MAKE) $(MFLAGS)  depend); \
	done

all:: $(PROGRAMS)

X.h:  magick/X.h
	 $(RM) $@
	$(LN) $? $@

includes:: X.h

depend:: X.h

clean::
	 $(RM) X.h

display: display.o
	  $(RM) $@
	 $(CCLINK) -o $@ $(LDOPTIONS) display.o $(LOCAL_LIBRARIES) $(LDLIBS) $(XLIB) -lm $(EXTRA_LOAD_FLAGS)

clean::
	 $(RM) display

lint:
	$(LINT) $(LINTFLAGS) $(LINTSRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

install:: display
	 @if [ -d $(DESTDIR)$(LOCALDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(LOCALDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  display $(DESTDIR)$(LOCALDIR)/display

