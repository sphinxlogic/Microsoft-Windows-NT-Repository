10 message "This program invents Alien names"
20 dim a$(5), b$(19)
30 for a=1 to 5: read a$(a)
40 for a=1 to 19: read b$(a)
50 do
60   print b$(rnd(19)) + a$(rnd(5)) + b$(rnd(19)) + &
           b$(rnd(19)) + a$(rnd(5)) + b$(rnd(19)) + ' ';
70 loop

80 data 'a', 'e', 'i', 'o', 'u', 'b', 'c', 'd', 'f', 'g', 'k', 'l', 'm'
90 data 'n', 'p', 'r', 's', 't', 'v', 'x', 'z', 'ch', 'sh', 'th'
