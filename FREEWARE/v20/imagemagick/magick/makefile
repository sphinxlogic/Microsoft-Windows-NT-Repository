	 $(_NULLCMD_)
	 $(_NULLCMD_)
	 $(_NULLCMD_)

.c.o:
	 $(_NULLCMD_)
	 $(_NULLCMD_)
	  $(RM) $@ $*.os \
	&& $(CC) -c $(_NOOP_) $(CFLAGS) $*.c \
	&& $(LD) $(LDSTRIPFLAGS) -r $@ -o $*.os \
	&& $(MV) $*.os $@
	 $(_NULLCMD_)

clean::
	 $(_NULLCMD_)
	 $(_NULLCMD_)
	 $(_NULLCMD_)

all:: lib$(LIBNAME).a

lib$(LIBNAME).a: $(OBJS)
	 $(RM) $@
	 $(AR) $@ $(OBJS)
	 $(_NULLCMD_)
	  $(_NULLCMD_)

install:: lib$(LIBNAME).a
	 @if [ -d $(DESTDIR)$(USRLIBDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(USRLIBDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTLIBFLAGS) lib$(LIBNAME).a $(DESTDIR)$(USRLIBDIR)
	 $(_NULLCMD_)

lintlib:: llib-l$(LIBNAME).ln

llib-l$(LIBNAME).ln: $(SRCS)
	 $(RM) $@
	$(LINT) $(LINTLIBFLAG)$(LIBNAME) $(LINTFLAGS) $(SRCS)

install.ln:: llib-l$(LIBNAME).ln
	 @if [ -d $(DESTDIR)$(LINTLIBDIR) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(LINTLIBDIR)); fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTLIBFLAGS) llib-l$(LIBNAME).ln $(DESTDIR)$(LINTLIBDIR)/llib-l$(LIBNAME).ln

includes::
	 @if [ -d $(BUILDINCDIR)/magick ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(BUILDINCDIR)/magick); fi
	@(set -x; cd $(BUILDINCDIR)/magick; for i in $(HEADERS); do \
		 $(RM) $$i; \
		$(LN) $(BUILDINCTOP)/../$(CURRENT_DIR)/$$i .; \
	done)

install:: $(HEADERS)
	 @if [ -d $(DESTDIR)$(INCDIR)/magick ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(INCDIR)/magick); fi
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(HEADERS); do \
		(set -x; $(INSTALL) $(INSTALLFLAGS) $(INSTINCFLAGS) $$i $(DESTDIR)$(INCDIR)/magick); \
	done

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

install:: $(HEADERS)
	 @if [ -d $(DESTDIR)$(LOCALINC) ]; then set +x; \
		else (set -x; $(MKDIRHIER) $(DESTDIR)$(LOCALINC)); fi
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(HEADERS); do \
		(set -x; $(INSTALL) $(INSTALLFLAGS) $(INSTDATFLAGS) $$i $(DESTDIR)$(LOCALINC)); \
	done

depend::
	 $(DEPEND) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

