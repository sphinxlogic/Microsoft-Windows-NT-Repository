  ss=3\
  sss=2\
  $bs\
  |h{\
    $bs\
    |v{\
      orientLabel\
      $sss\
      orientButton\
      $sss\
      forientLabel\
      $sss\
      forientButton\
      $sss\
      scalesLabel\
      $sss\
      scalesButton\
      (2*$bs)\
      swapLandscape<+[1]*>\
      $sss\
      autoResize<+[1]*>\
      $sss\
      watch<+[1]*>\
    }\
    $ss\
    |v{\
      mediaLabel\
      $sss\
      mediaButton\
      $sss\
      fmediaLabel\
      $sss\
      fmediaButton\
      $sss\
      scaleBaseLabel\
      $sss\
      scaleBaseButton\
      (2*$bs)\
      respectDSC<+[1]*>\
      $sss\
      eof<+[1]*>\
      $sss\
      antialias<+[1]*>\
    }\
    $bs\
  }\
  (2*$bs)\
  |h{\
    (3*$bs)<-90%>\
    apply\
    $bs<+[1]-100%>\
    save\
    $bs<+[1]-100%>\
    dismiss\
    (3*$bs)<-90%>\
  }\
  $bs\
}

