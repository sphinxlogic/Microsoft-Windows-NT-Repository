% Define if not already defined
/ndf % /key value ndf --
{1 index where {pop pop pop} {dup xcheck {bind} if def} ifelse} def
%
% cmyk color fallback uses Ned Batchelder's algorithm
/setcmykcolor {
  1 sub 4 1 roll %(k-1)
  1 sub   3 index mul 3 1 roll % (c-1)(k-1)
  1 sub   3 index mul 3 1 roll % (m-1)(k-1)
  1 sub 4 -1 roll mul 3 1 roll % (y-1)(k-1)
  setrgbcolor
} ndf
%
% Convert document spot color to PrintSpec process color
EDMS_retro_dict/EDMS_EnableColor get not
{ /setcustomcolor   % [object] tint setcustomcolor -
  { exch aload pop                    % tint c m y k (name)
    dup EDMS_spotcmyk exch known      % check name
    {EDMS_spotcmyk exch get aload     % get PrintSpec cmyk
      9 -4 roll 5{pop} repeat         % blow away original cmyk
      4 {4 index mul 4 1 roll} repeat % apply tint
      setcmykcolor pop }
    {(Un-bound spot color = ) print print (\n) print quit}
    ifelse
  } def
  /setoverprint {pop} def
}if
%
% Convert spot color to gray tint when color is suppressed
/DEC$EDMS_SUPPRESS_COLOR where
{ pop
  /setcustomcolor   % [object] tint setcustomcolor -
  { 1 exch sub setgray pop }
  def
  /setoverprint {pop} def
}if
%
% rgb to cmyk conversion using a light amount of black generation
/rgbtocmyk   % red green blue rgbtocmyk cyan magenta yellow black
{   EDMS_retro_dict begin
% get gray component as an integer index
	3 copy
	2 copy lt {exch} if pop
	2 copy lt {exch} if pop
	255 mul cvi
% put under rgb values
	4 1 roll
% compute Cyan value
	3 -1 roll			% additive color value
	EDMS_Cyan 4 index get		% color correction
	add				% corrected color value
	1 exch sub			% subtractive color value
	dup 0 lt {pop 0} if 		% minimum is zero
% compute Magenta value
	3 -1 roll			% additive color value
	EDMS_Magenta 4 index get	% color correction
	add				% corrected color value
	1 exch sub			% subtractive color value
	dup 0 lt {pop 0} if 		% minimum is zero
% compute Yellow value
	3 -1 roll			% additive color value
	EDMS_Yellow 4 index get		% color correction
	add				% corrected color value
	1 exch sub			% subtractive color value
	dup 0 lt {pop 0} if 		% minimum is zero
% compute black generation value
	EDMS_Black 5 -1 roll get
	end %EDMS_retro_dict
} def
