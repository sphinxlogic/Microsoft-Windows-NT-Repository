%%BeginResource: procset EDM_retro
% For EDM V3.3a
% This must follow an EDM Setup area
% By R. Anderson 19-May-1992
EDMS_retro_dict begin
% shift the entire document to make room for crop and reg marks
/DEC$EDMS_MAKE_FILM where {pop 54 54 translate } if
%
% Shift EPSF files
%EDMS_XLL neg EDMS_YLL neg translate
%
% save definitions at start of job
/EDMS_currentmatrix /currentmatrix load def
/EDMS_s_matrix matrix currentmatrix def
/EDMS_showpage /showpage load def
/Bleed 9 def
%
/EDMS_property (This film is the property of Digital Equipment Corporation) def
EDMS_retro_dict/EDMS_EnableColor % set true if separating color
  /DEC$EDMS_SEPARATE_COLORS where {pop EDMS_colorprolog} {false} ifelse put
/EDMS_RV_flag false def
/EDMS_str 10 string def
%
% disable showpage for included document
userdict/showpage {initgraphics} put
%
/DEC$EDMS_POSITION_REGMARKS dup where
 {pop DEC$EDMS_POSITION_REGMARKS}{0}ifelse 9 sub def
%
/DEC$EDMS_ENABLE_RECTOVERSO where
{ pop DEC$EDMS_ENABLE_RECTOVERSO 0 }{ 0 0 }ifelse
dtransform /RV_dy exch def /RV_dx exch def
%
% RV translation is done by re-defining
% setmatrix, currentmatrix, and initgraphics
%
/EDMS_setmatrix /setmatrix load def
userdict /setmatrix
{ EDMS_retro_dict begin
  EDMS_setmatrix
% RV shift usually occurs on even-numbered (left-hand) pages
  EDMS_RV_flag { RV_dx RV_dy idtransform translate } if
  end %EDMS_retro_dict
} put
%
userdict /currentmatrix
{ gsave
  EDMS_retro_dict begin
% undo RV shift
  EDMS_RV_flag { RV_dx RV_dy idtransform exch neg exch neg translate } if
  EDMS_currentmatrix
  end %EDMS_retro_dict
  grestore
} put
%
/EDMS_initgraphics /initgraphics load def
userdict /initgraphics
{ EDMS_retro_dict begin
  EDMS_initgraphics
% RV shift usually occurs on even-numbered (left-hand) pages
  EDMS_RV_flag { RV_dx RV_dy idtransform translate } if
  end %EDMS_retro_dict
} put
%
% Create dictionary that maps document spot colors to CMYK values
/num 0 def EDMS_colorbinding {length /num exch num add def pop} forall
  userdict/EDMS_spotcmyk num dict put
EDMS_spotcmyk begin
EDMS_colorbinding
{ exch /num exch store % save color number
  { EDMS_cmykcolor num get def
  } forall % for each color name
} forall   % for each color number
end %EDMS_spotcmyk
%
% Proc executed before page 1
userdict/EDMS_Setup  % -- EDMS_Setup --
  {EDMS_retro_dict/EDMS_p_matrix matrix currentmatrix put} put
%
% Proc executed at start of page
userdict/EDMS_BeginPage  % RV_flag EDMS_BeginPage --
{ EDMS_retro_dict begin
  /EDMS_save save def
% save arguments passed on stack  
  /EDMS_RV_flag exch def    % used by matrix commands
% verify color defined for film mode
  /DEC$EDMS_MAKE_FILM where
  { pop
    /DEC$EDMS_SEPARATE_COLORS where
    { pop }
    { (ERROR - DEC$EDMS_MAKE_FILM requires DEC$EDMS_SEPARATE_COLORS) == quit }
    ifelse
  } if
% verify color # defined in name dictionary
  /DEC$EDMS_SEPARATE_COLORS where
  { pop
    EDMS_colornames DEC$EDMS_SEPARATE_COLORS known not
    {
      (ERROR - incorrect value for DEC$EDMS_SEPARATE_COLORS: ) print
      DEC$EDMS_SEPARATE_COLORS == quit
    } if
  } if
% enable color separation
EDMS_EnableColor {
% set screen for this separation type
% Index for screen values
  /# DEC$EDMS_SEPARATE_COLORS 1 ge { 1 }{ DEC$EDMS_SEPARATE_COLORS }ifelse def
  /EDMS_screen where
  {pop EDMS_screen # get exec}
  {currentscreen 3 1 roll pop EDMS_angle # get 3 -1 roll setscreen}
  ifelse 
% create Adobe color-separation array
  /_beginseparation [
  EDMS_septype # get      % Separation type
  EDMS_colorbinding DEC$EDMS_SEPARATE_COLORS get % Spot colors on this sep
  1						% setgray value for nullimage
  ] setvar
  /_endseparation [ ] setvar
% Turn on Adobe color separation
  Adobe_separation_vars /_beginseparation get
  Adobe_separation /beginseparation get exec
  Adobe_separation /beginpage get exec
} if
% clip to bounding box
  initclip
  EDMS_s_matrix setmatrix   % set coord system from start of job plus RV
  newpath
  EDMS_XLL Bleed sub EDMS_YLL Bleed sub moveto
  EDMS_XLL Bleed sub EDMS_YUR Bleed add lineto
  EDMS_XUR Bleed add EDMS_YUR Bleed add lineto
  EDMS_XUR Bleed add EDMS_YLL Bleed sub lineto
  closepath
  clip
  newpath
% use coord system from first page
  EDMS_p_matrix setmatrix  % adds RV correction
  end  %EDMS_retro_dict
} put %EDMS_BeginPage
%
% arrays accessed by EDMS_BitVect
/EDMS_Bleed_array [0 0 1 1] def % 1 => call EDMS_Bleed
/EDMS_Blank_array [0 1 0 0] def % 1 => check DEC$EDMS_SUPRESS_COLOR
%
% Proc executed at end of page
userdict/EDMS_EndPage  % Page# bit_vector EDMS_EndPage --
{ EDMS_retro_dict/EDMS_EnableColor get
% turn off Adobe separation code
  { Adobe_separation /endpage get exec
    Adobe_separation_vars /_endseparation get
    Adobe_separation /endseparation get exec
  } if
% save args
  EDMS_retro_dict begin
  /EDMS_BitVect exch def
  /EDMPageNumber exch def
% do bleeds
  EDMS_initgraphics
  EDMS_s_matrix setmatrix   % get coord system from start of job plus RV
  EDMS_Bleed_array EDMS_BitVect get 1 eq
  { EDMS_RV_flag EDMS_XLL EDMS_YLL EDMS_XUR EDMS_YUR EDMS_Bleed }if
% do film annotation
  /DEC$EDMS_MAKE_FILM where
  { pop
    /Helvetica findfont 10 scalefont setfont 
    0 setgray 
% process ink name
    gsave  
    EDMS_XLL -20 add EDMS_YLL 10 add moveto 
    90 rotate 
    (Ink:  ) show
	-1 -1 -3
	{
	  dup DEC$EDMS_SEPARATE_COLORS eq {0}{1} ifelse setgray
	  EDMS_colornames exch 2 copy known {get show(  ) show}{pop pop} ifelse
	}for
% spot ink name
	0 1 10
	{
	  dup DEC$EDMS_SEPARATE_COLORS eq {0}{1} ifelse setgray
	  EDMS_colornames exch 2 copy known {get show(  ) show}{pop pop} ifelse
	}for
    grestore
% Screen frequency and angle
    currentscreen pop exch
    gsave  
    EDMS_XLL -20 add EDMS_YLL EDMS_YUR add 2 div 20 add moveto 
    90 rotate 
    (Halftone frequency:  ) show
    EDMS_str cvs show
    (  angle:  ) show
    EDMS_str cvs show
	grestore
% document title
    gsave
    EDMS_XLL 10 add EDMS_YUR 20 add moveto		 
    (Document: ) show
    EDMS_title show 
    grestore
% ordinal page number
    gsave
    EDMS_XUR EDMS_XLL add .5 mul 24 add EDMS_YUR 20 add moveto
    (Page: ) show	
    EDMPageNumber EDMS_str cvs show 
    ( of ) show
    EDMS_TOTAL_PAGES EDMS_str cvs show 
    grestore
% property notice
    gsave
    EDMS_XUR 24 add EDMS_YUR EDMS_YLL sub .75 mul EDMS_YLL add
	EDMS_property stringwidth pop .5 mul sub moveto 
    90 rotate
    EDMS_property show stroke 
    grestore
% grayscale calibration bar
	0.0
	EDMS_XUR EDMS_XLL sub .25 mul EDMS_XLL add
	EDMS_YLL 22.0 sub
	graystrip2
% process color bar
    DEC$EDMS_SEPARATE_COLORS 0 le
    { /Helvetica 180
	  EDMS_XUR EDMS_XLL sub .75 mul EDMS_XLL add
	  EDMS_YLL 22.0 sub
	  colorstrip2
    } if
% add crop marks
    /mask 15 /DEC$EDMS_SUPPRESS_CROPMARKS where
      { pop DEC$EDMS_SUPPRESS_CROPMARKS xor }if def 
%          
    mask 1 and 1 eq
    {   0 EDMS_XUR EDMS_YUR cropmark
      -90 EDMS_XUR EDMS_YUR cropmark
    }if
    mask 2 and 2 eq
    { -90 EDMS_XUR EDMS_YLL cropmark
      180 EDMS_XUR EDMS_YLL cropmark
    }if
   mask 4 and 4 eq
    { 180 EDMS_XLL EDMS_YLL cropmark
       90 EDMS_XLL EDMS_YLL cropmark
    }if
    mask 8 and 8 eq
    {  90 EDMS_XLL EDMS_YUR cropmark
        0 EDMS_XLL EDMS_YUR cropmark
    }if
%
% add registration marks
    /mask 15 /DEC$EDMS_SUPPRESS_REGMARKS where
      { pop DEC$EDMS_SUPPRESS_REGMARKS xor } if def 
%
    mask 1 and 1 eq
    { EDMS_XUR 2 div
      EDMS_YUR DEC$EDMS_POSITION_REGMARKS sub
      regmark 
    } if 
    mask 2 and 2 eq
    { EDMS_XUR DEC$EDMS_POSITION_REGMARKS sub
      EDMS_YUR 2 div
      regmark
    } if 
    mask 4 and 4 eq
    { EDMS_XUR 2 div 
      DEC$EDMS_POSITION_REGMARKS
      regmark
    } if 
    mask 8 and 8 eq
    { DEC$EDMS_POSITION_REGMARKS
      EDMS_YUR 2 div
      regmark
    } if
  } if    % end of film annotation
% test for blank page suppression and leave boolean on stack
  EDMS_Blank_array EDMS_BitVect get 1 ne dup not
   { /DEC$EDMS_SUPPRESS_BLANKPAGES where {pop false} {true} ifelse } if
% Blow away VM used by this page
  EDMS_save restore
% execute showpage if not suppressed
  gsave
  {EDMS_showpage}{erasepage} ifelse
  grestore
  end %EDMS_retro_dict
} put  % EDMS_endpage 
%
% Proc for end of the document, before EDMS index
userdict/EDMS_EndDocument  % -- EDMS_EndDocument --
{ EDMS_retro_dict/EDMS_EnableColor get
% Terminate Adobe color separation, i.e. remove dictionaries
  { end  %userdict
    Adobe_separation /terminate get exec
    Adobe_separate /terminate get exec
    Adobe_gsproperty /terminate get exec
    Adobe_cmykcolor /terminate get exec
  } if
} put
%
% draw crop mark in +Y direction
/cropmark  % rot x y cropmark --
{ gsave
  translate
  rotate
  newpath
  0.3 setlinewidth
  0 6 moveto
  0 30 lineto
  stroke
  grestore
} def
%
% draw registration mark
/regmark  % x y regmark --
{ gsave
  translate
  0.3 setlinewidth
  0 setlinecap
  0 setlinejoin
  newpath
  1 setgray
  10 0 moveto 0 0 10 0 360 arc
  fill
  0 setgray 
  0 0 7 0 360 arc
  -10 0 moveto 10 0 lineto
  0 -10 moveto 0 10 lineto
  stroke
  0 0 4 0 360 arc
  fill
  1 setgray
  -4 0 moveto 4 0 lineto
  0 -4 moveto 0 4 lineto
  stroke
  grestore
} def
%
% grayscale calibration bar
/graystrip2			% angle tx ty graystrip2 --
{ gsave
  translate
  rotate
  -75 -7.5 translate
  0 1 9
  { newpath 0 0 moveto 15 0 rlineto 0 15 rlineto -15 0 rlineto closepath
    gsave
    10 div setgray
    fill
    grestore
    gsave
    1 setgray
    .5 setlinewidth
    stroke
    grestore
    15 0 translate
  } for
  grestore
} def
%
% process color bar
/colorstrip2			% font angle tx ty colorstrip2 --
{ gsave
  translate
  rotate
  -60 -7.5 translate
  findfont 15 3 div scalefont setfont
  /# DEC$EDMS_SEPARATE_COLORS def
  (C) # -1 eq {0} {1} ifelse
  (M) # -2 eq {0} {1} ifelse
  (Y) # -3 eq {0} {1} ifelse
  (CM) # -1 eq # -2 eq or {0} {1} ifelse
  (MY) # -2 eq # -3 eq or {0} {1} ifelse
  (CY) # -1 eq # -3 eq or {0} {1} ifelse
  (CMY) # -1 eq # -2 eq or # -3 eq or {0} {1} ifelse
  (K) # 0 eq {0} {1} ifelse
  0 1 7
  { pop % for loop index
    newpath 0 0 moveto 15 0 rlineto 0 15 rlineto -15 0 rlineto closepath
    gsave
    setgray
    fill
    grestore
    gsave
    0 setgray
    .5 setlinewidth
    stroke
    grestore
    gsave
    -90 rotate 
    dup stringwidth pop neg 15 sub 2 div 5 
    moveto
    show
    grestore
    15 0 translate
  } for
  grestore
} def
end %EDMS_retro_dict
