%%BeginResource: procset EDM_PRINTSPEC
%%Copyright: 1993 by Digital Equipment Corporation, Maynard, MA, All rights reserved.
%%Creator: R. Anderson
%%CreationDate: (08/09/93)
%%Version: 1.0 22
EDMSdict begin
% This procset calibrates spot color to PrintSpec definitions 
% This must be loaded whenever using spot color
% Define if not already defined
/ndf % /key value ndf --
{1 index where {pop pop pop} {dup xcheck {bind} if userdict 3 1 roll put} ifelse} def
% cmyk color fallback uses Ned Batchelder's algorithm
/@setcmykcolor /setcmykcolor where
{ /setcmykcolor get def }
{
  {
    1 sub 4 1 roll %(k-1)
    1 sub   3 index mul 3 1 roll % (c-1)(k-1)
    1 sub   3 index mul 3 1 roll % (m-1)(k-1)
    1 sub 4 -1 roll mul 3 1 roll % (y-1)(k-1)
    setrgbcolor
  } bdf
} ifelse
% Print spot as process using PrintSpec values
% [c m y k /name] tint setprocess --
/setprocess
{
  exch
  aload pop pop
  4 index 1.0 ne { 4 {4 index mul 4 1 roll} repeat} if
  @setcmykcolor pop
} bdf
% Get PrintSpec-equivalent spot color
% [c m y k /oldname] getspot [c m y k /newname]
/getspot
{
  PrintSpec 1 index 4 get known
  {PrintSpec exch 4 get get} % PrintSpec replacement
  {
    EDMSdict/BadSpot known not {/BadSpot 20 dict def} if
    BadSpot 1 index 4 get known not
    {
      (Warning: Unknown spot color = ) print dup 4 get =string cvs print (\n) print flush
	  BadSpot 1 index aload pop 5 1 roll /Process 5 array astore put
    } if
    BadSpot exch 4 get get % BadSpot replacement
  } ifelse
} bdf
% Convert document spot color to printspec spot color
% [c m y k /name] tint setcustomcolor -
/setcustomcolor dup where
{
   pop
   EDMSdict /@customcolor /setcustomcolor load put
  {
    EDMSdict begin
    exch
    getspot % get PrintSpec spot color
    dup 4 get /Process eq % check if bound to process
    {exch setprocess}
    {exch @customcolor} ifelse
    end % EDMSdict
  }
}
{{
    EDMSdict begin
    exch
    getspot % get PrintSpec spot color
    exch setprocess
    end % EDMSdict
}} ifelse
udf
% These two are needed in addition to setcustomcolor for Adobe Illustrator
/setoverprint { pop } ndf
/findcmykcustomcolor {5 array astore} ndf
% Update EDM color binding with EDMatic color binding
/color_update
{
  EDMSdict /EDMatic_color_binding known
  {
    PrintSpec begin
    PrintSpec % look at every EDM color binding
    {
      4 get % EDM PrintSpec color
      dup EDMatic_color_binding exch known
      {
        EDMatic_color_binding exch get store % bind to EDMatic color
      }
      {
        pop pop 
      } ifelse
    }forall
    end
  }if
} bdf
end % EDMSdict
