%%BeginResource: procset FULL_BLEED
%
% Full Bleed for VAX DOCUMENT V2.1-1
%
% EDM will insert this code at the beginning of the master file. The 'DoBleed'
% procedure expects arguments on the stack:
%
% RV      => boolean specifying recto-verso shift (left-pages)
% XLL,YLL => Lower-left corner of page
% XUR,YUR => Upper-right corner of page
%
/BleedDict 20 dict def
BleedDict begin
/bars 1 def             % number of bleed bars
/width .375 72 mul def  % 3/8 inch
/lap .125 72 mul def	% 1/8 inch
/total width lap add def% sum
/offset width lap sub 2 div def % distance from paper edge
end % BleedDict
%
% EDMS_Bleed routine for all bleed types
%
/EDMS_Bleed                    % RV XLL YLL XUR YUR EDMS_Bleed --
{
  BleedDict begin
% save args
  /YUR exch def
  /XUR exch def
  /YLL exch def
  /XLL exch def
  /RV exch def
% bleed unless non-black separation
  /EDMS_color# where             % If separating colors
  { pop EDMS_color# 0 eq }       % and color# is non-zero, set false
  { true }
  ifelse
% do bleed if true
  {
    gsave
% set position and size
    3600 72 div dup scale
    XLL YLL translate
    /PaperHeight YUR YLL sub def
    /PaperWidth XUR XLL sub def
    0 setgray
    0 setlinecap
    total setlinewidth
% tranlate to center of line
    RV {offset}{PaperWidth offset sub} ifelse 0 translate
% draw a full bleed
    bars 0 gt {0 lap neg moveto 0 PaperHeight lap add lineto stroke} if
% knock (bars-1) holes out of bleed
    bars 1 gt
    {
	  1 setgray
      0 0 moveto
      /len PaperHeight bars dup 1 sub add div def  % len = PaperHeight/(n * (n-1))
      2 1 bars {
	  pop 0 len rmoveto 0 len rlineto} for
	  stroke
    } if
    grestore
  } if
  end % BleedDict
} def
%%EndResource
