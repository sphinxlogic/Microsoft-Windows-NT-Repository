EXES=cat\cat.exe chmod\chmod.exe chown\chown.exe cp\cp.exe \
elvis\vi.exe find\find.exe grep\grep.exe ln\ln.exe ls\ls.exe \
mkdir\mkdir.exe mv\mv.exe rm\rm.exe rmdir\rmdir.exe sh\sh\sh.exe \
touch\touch.exe wc\wc.exe

all:$(EXES)

ar\ar.exe:
	cd ar
	nmake
	cd ..

