X# $Id: test0.sh,v 1.5 1993/12/12 01:39:09 genek Exp $ 
X
XSIGGEN=$1
XTESTDIR=$2
XTEMPFILE=$3
XTEST0KEY=$4
XME=$0
X
Xcat << GHK
X=== $ME: DESCRIPTION
X
X    This shell script exercises all the signature routines included in
Xthe Tripwire distribution.  This suite is run on a series of files
Xcreated by the authors of the signature routines.
X
XGHK
X
Xecho "=== $ME: BEGIN ==="
X
X./createfiles $TESTDIR
Xrm -rf $TESTDIR/CVS
X$SIGGEN -h $TESTDIR/* > $TEMPFILE
Xdiff $TEMPFILE $TEST0KEY 
Xif [ $? -eq 0 ] 
Xthen
X    touch OKSIGS && rm $TEMPFILE 
X    echo "=== $ME: PASS ==="
X    exit 0
Xelse
X    echo Signatures do not match!  File $TEMPFILE should match $TEST0KEY.  Aborting... 
X    echo "=== $ME: FAIL ==="
X    exit 1
Xfi
X
END_OF_FILE
