X * MANIFEST: minimal stddef.h substitute
X */
X
X#ifndef _STDDEF_H
X#define _STDDEF_H
X
X#ifndef NULL
X#define NULL 0
X#endif
X
X#ifndef _PTRDIFF_T
X#define _PTRDIFF_T
Xtypedef long ptrdiff_t;
X#endif
X
X#ifndef _SIZE_T
X#define _SIZE_T
Xtypedef unsigned long size_t;
X#endif
X
X#ifndef ofsetof
X#define offsetof(type, identifier) \
X	(size_t)((char *)&((type *)0)->identifier - (char *)0)
X#endif
X
X#endif /* _STDDEF_H */
END_OF_FILE
