X#
Xcase $# in
X1)
X	;;
X*)
X	echo "usage: $0 somewhere/man1" 1>&2
X	exit 1
X	;;
Xesac
X
Xset -e
Xcd man1
Xfor file in *.1
Xdo
X	echo "soelim $file > $1/$file"
X	soelim $file > $1/$file
