X	scanner.o section.o table.o tangle.o texhead.o weave.o writfile.o
X
fw: $(OBJ)
X	$(CC) -o fw $(OBJ)
X
clean:
X	rm -f $(OBJ) fw
X	rm -f *.BAK *.CKP *~ #*#
X	rm -f core a.out trace.dump
X
install: fw
X	install -s -c fw $(DESTBIN)/fw
