X@$@<Five body parts@>+=@{Walrus@}
X@$@<Five body parts@>+=@{@<Three body parts@>@}
X@$@<Five body parts@>+=@{Aardvaark@}
X@$@<Five body parts@>+=@{@<Two body parts@>@}
X
X
