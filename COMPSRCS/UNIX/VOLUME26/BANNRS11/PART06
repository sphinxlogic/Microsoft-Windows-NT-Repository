X
int i,j;
int width = DWIDTH;	/* -w option: scrunch letters to 80 columns */
int debug;
int trace;
char line[DWIDTH];
char print[DWIDTH];
char message[MAXMSG];
int nchars;
int linen;
int x,y;
int term;
int pc;
int max;
X
main(argc, argv)
X	int argc;
X	char **argv;
X{ 
X	extern char *optarg;
X	extern int optind;
X	int ch;
X	char *strcpy(), *strcat();
X
X	while ((ch = getopt(argc, argv, "w:td")) != EOF)
X		switch((char)ch) {
X		case 'w':
X			width = atoi(optarg);
X			if (width <= 0)
X				width = 80;
X			break;
X		case 'd':
X			debug = 1;
X			break;
X		case 't':
X			trace = 1;
X			break;
X		case '?':
X		default:
X			fprintf(stderr, "usage: banner [-w width]\n");
X			exit(1);
X		}
X	argc -= optind;
X	argv += optind;
X
X	for (i=0; i<width; i++) {
X		j = i * 132 / width;
X		print[j] = 1;
X	}
X
X#ifdef notdef
X	{
X#define dir(f) "/e1/mrh/ucb/lib/f"
X#define INDTBL dir(ban.dat.indtbl)
X#define OBJECT dir(ban.dat.object)
X		FILE *fd;
X
X		fd = fopen(INDTBL, "r");
X		for (i=0; i<NCHARS; i++) {
X			fscanf(fd, "%d", &asc_ptr[i]);
X		}
X		fclose(fd);
X
X		fd = fopen(OBJECT, "r");
X		fread(data_table, 1, NBYTES, fd);
X		fclose(fd);
X	}
X#endif
X
X	/* Have now read in the data. Next get the message to be printed. */
X	if (*argv) {
X		strcpy(message, *argv);
X		while (*++argv) {
X			strcat(message, " ");
X			strcat(message, *argv);
X		}
X		nchars = strlen(message);
X	} else {
X		fprintf(stderr,"Message: ");
X		(void)fgets(message, sizeof(message), stdin);
X		nchars = strlen(message);
X		message[nchars--] = '\0';	/* get rid of newline */
X	}
X
X	/* some debugging print statements */
X	if (debug) {
X		printf("int asc_ptr[128] = {\n");
X		for (i=0; i<128; i++) {
X			printf("%4d,   ",asc_ptr[i]);
X			if ((i+1) % 8 == 0)
X				printf("\n");
X		}
X		printf("};\nchar data_table[NBYTES] = {\n");
X		printf("  /*   ");
X		for (i=0; i<10; i++) printf(" %3d  ",i);
X		printf("   */\n");
X		for (i=0; i<NBYTES; i += 10) {
X			printf("/* %4d */  ",i);
X			for (j=i; j<i+10; j++) { 
X				x = data_table[j] & 0377;
X				printf(" %3d, ",x);
X			}
X			putchar('\n');
X		}
X		printf("};\n");
X	}
X
X	/* check message to make sure it's legal */
X	j = 0;
X	for (i=0; i<nchars; i++)
X		if (asc_ptr[message[i]] == 0) {
X			printf("The character '%c' is not in my character set.\n",message[i]);
X			j++;
X		}
X	if (j) exit(1);
X
X	if (trace)
X		printf("Message '%s' is OK\n",message);
X	/* Now have message. Print it one character at a time.  */
X
X	for (i=0; i<nchars; i++) {
X		if (trace)
X			printf("Char #%d: %c\n", i, message[i]);
X		for (j=0; j<DWIDTH; j++) line[j] = ' ';
X		pc = asc_ptr[message[i]];
X		term = 0;
X		max = 0;
X		linen = 0;
X		while ( !term ) {
X			if (pc<0 || pc > NBYTES) {
X				printf("bad pc: %d\n",pc);
X				exit(1);
X			}
X			x = data_table[pc] & 0377;
X			if (trace)
X				printf("pc=%d, term=%d, max=%d, linen=%d, x=%d\n",pc,term,max,linen,x);
X			if (x >= 128) {
X				if (x>192) term++;
X				x = x & 63;
X				while (x--) {
X					if (print[linen++]) {
X						for (j=0; j<=max; j++)
X							if (print[j])
X								putchar(line[j]);
X						putchar('\n');
X					}
X				}
X				for (j=0; j<DWIDTH; j++) line[j] = ' ';
X				pc++;
X			}
X			else {
X				y = data_table[pc+1];
X				/* compensate for narrow teminals */
X#ifdef notdef
X				x = (x*width + (DWIDTH/2)) / DWIDTH;
X				y = (y*width + (DWIDTH/2)) / DWIDTH;
X#endif
X				max = x+y;
X				while (x < max) line[x++] = '#';
X				pc += 2;
X				if (trace)
X					printf("x=%d, y=%d, max=%d\n",x,y,max);
X			}
X		}
X	}
X
X	exit(0);
X}
END_OF_FILE
if test 74009 -ne `wc -c <'banner-05/vban.c'`; then
    echo shar: \"'banner-05/vban.c'\" unpacked with wrong size!
fi
# end of 'banner-05/vban.c'
fi
echo shar: End of archive 6 \(of 7\).
cp /dev/null ark6isdone
MISSING=""
for I in 1 2 3 4 5 6 7 ; do
    if test ! -f ark${I}isdone ; then
	MISSING="${MISSING} ${I}"
    fi
done
if test "${MISSING}" = "" ; then
    echo You have unpacked all 7 archives.
    rm -f ark[1-9]isdone
else
    echo You still need to unpack the following archives:
    echo "        " ${MISSING}
fi
##  End of shell archive.
exit 0
