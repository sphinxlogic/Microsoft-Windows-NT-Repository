#!/bin/sh
X
XXMIN=-4.0
XXMAX=4.0
YMIN=-3.0
YMAX=3.0
WIDTH=400
HEIGHT=300
ITER=100
SCALE=2
CMAP="-cmap ../cmaps/hermes"
# CMAP=
# DITHER=
# DITHER=-dither
X
mbs $XMIN $XMAX $YMIN $YMAX $WIDTH $HEIGHT $ITER | ../pixmon -scale $SCALE -dx $WIDTH -dy $HEIGHT $CMAP $DITHER
X
X
SHAR_EOF
