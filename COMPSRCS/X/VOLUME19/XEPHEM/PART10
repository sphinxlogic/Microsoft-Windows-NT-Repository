if test -f 'helpmenu.c' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'helpmenu.c'\"
else
  echo shar: Extracting \"'helpmenu.c'\" \(8808 characters\)
  sed "s/^X//" >'helpmenu.c' <<'END_OF_FILE'
X/* this file contains the code to put up help messages.
X * the messages come from a file or, if no file is found or there is no
X * help entry for the requested subject, a small default text is provided.
X */
X
X#include <stdio.h>
X#include <ctype.h>
X#include <math.h>
X#if defined(__STDC__)
X#include <stdlib.h>
X#endif
X#include <X11/Xlib.h>
X#include <Xm/Xm.h>
X#include <Xm/Form.h>
X#include <Xm/PushB.h>
X#include <Xm/Text.h>
X
X#if defined(__STDC__) || defined(__cplusplus)
X#define P_(s) s
X#else
X#define P_(s) ()
X#endif
X
Xextern void prompt_map_cb P_((Widget w, XtPointer client, XtPointer call));
Xextern void set_something P_((Widget w, char *resource, char *value));
Xextern void xe_msg P_((char *msg, int app_modal));
X
Xvoid hlp_dialog P_((char *tag, char *deflt[], int ndeflt));
Xvoid references_help P_((void));
Xvoid intro_help P_((void));
Xvoid config_help P_((void));
Xvoid mainmenu_help P_((void));
Xvoid datetime_help P_((void));
Xvoid operation_help P_((void));
Xvoid notes_help P_((void));
Xstatic Widget hlp_create_dialog P_((char *tag));
Xstatic void hlp_ok_cb P_((Widget w, XtPointer client, XtPointer call));
Xstatic FILE *hlp_openfile P_((char *tag));
Xstatic hlp_fillfromfile P_((char *tag, Widget txt_w, int l));
Xstatic void hlp_fillfromstrings P_((char *msg[], int nmsg, Widget txt_w));
X
X#undef P_
X
X
Xextern Widget toplevel_w;
X#define	XtD XtDisplay(toplevel_w)
Xextern char *myclass;
X
X#define	MAXLINE		128	/* longest allowable help file line */
X#define	HLP_TAG		'@'	/* help file tag marker */
X#define	HLP_NEST	'+'	/* help file nested tag marker */
X
Xstatic char hlpfdef[] = "xephem.hlp";	/* default help file name */
X
X/* put up a help dialog. it contains a scrolled text area and an Ok button.
X * make a new dialog each time so we can have several up at once.
X * this means we need an explicit callback on the Ok button to destroy it
X * again (rather than being able to use the autoUnmanage feature).
X * if can't find any help, say so.
X */
Xvoid
Xhlp_dialog (tag, deflt, ndeflt)
Xchar *tag;	/* tag to look for in help file - also dialog title */
Xchar *deflt[];	/* help text to use if tag not found */
Xint ndeflt;	/* number of strings in deflt[] */
X{
X	Widget txt_w, form_w;
X
X	txt_w = hlp_create_dialog (tag);
X	form_w = XtParent(XtParent(txt_w));
X
X	if (hlp_fillfromfile(tag, txt_w, 0) < 0) {
X	    if (!deflt || ndeflt == 0) {
X		char buf[MAXLINE];
X		(void) sprintf (buf, "No help for %s", tag);
X		xe_msg (buf, 0);
X		XtDestroyWidget (form_w);
X		return;
X	    } else
X		hlp_fillfromstrings(deflt, ndeflt, txt_w);
X	}
X
X	XtManageChild (form_w);
X	/* everything gets destroyed when the Ok button is selected */
X}
X
Xvoid
Xreferences_help()
X{
X	static char *msg[] = {
X"Many formulas and tables are based, with permission, on material found in",
X"\"Astronomy with your Personal Computer\" by Dr. Peter Duffett-Smith,",
X"Cambridge University Press, (c) 1985."
X};
X
X	hlp_dialog ("Credits", msg, XtNumber(msg));
X}
X
Xvoid
Xintro_help()
X{
X	static char *msg[] = {
X"Xephem computes all sorts of information about the planets, some moons and",
X"two user defined objects. The program displays this in a variety of tabular",
X"and graphical formats. It can also solve a user defined function built from",
X"this data and save values in plot and listing files for later display.",
X"",
X"Many dialogs have further \"Help\" of their own as well."
X};
X
X	hlp_dialog ("Intro", msg, XtNumber(msg));
X}
X
Xvoid
Xconfig_help()
X{
X	static char *msg[] = {
X"When xephem starts up it looks for a file named ephem.cfg. The name may be",
X"changed with the -c option or the EPHEMCFG environ variable. The file",
X"contains initialization information in the form of keyword=value pairs.",
X"The same keyword=value pairs can be used from the command line as well."
X};
X
X	hlp_dialog ("Initialization", msg, XtNumber(msg));
X}
X
Xvoid
Xmainmenu_help()
X{
X	static char *msg[] = {
X"The Main xephem menu displays the current circumstances and controls",
X"runtime behavior.",
X"See the Help for Operation for more information."
X};
X
X	hlp_dialog ("MainMenu", msg, XtNumber(msg));
X}
X
Xvoid
Xdatetime_help()
X{
X	static char *msg[] = {
X"Dates formats depend on the DATE_FORMAT resource. But regardless you need",
X"only specify what changes. Dates can also be given as decimal years.",
X"Times are hh:mm:ss - again, only specify what changes."
X};
X
X	hlp_dialog ("Date/time", msg, XtNumber(msg));
X}
X
Xvoid
Xoperation_help()
X{
X	static char *msg[] = {
X"Set location details, starting date and time, step size and number of steps,",
X"select desired views, then select \"Update\" to run.",
X};
X
X	hlp_dialog ("Operation", msg, XtNumber(msg));
X}
X
Xvoid
Xnotes_help()
X{
X	static char *msg[] = {
X"floating point errors are not trapped gracefully. especially be careful not",
X"  to turn on Objx/y before they are properly defined.",
X"",
X"planet magnitudes are not very accurate."
X};
X
X	hlp_dialog ("Notes", msg, XtNumber(msg));
X}
X
X/* create the help dialog with a scrolled text area and an Ok button.
X * return the text area widget; when ready to view, manage its parent's parent:
X *   dialog -> form -> scrolled_window -> text.
X * pass the dialog widget as the client parameter of the ok activate callback
X *   so it can destroy it.
X */
Xstatic Widget
Xhlp_create_dialog (tag)
Xchar *tag;
X{
X	Widget hlp_w;
X	Widget t_w, cb_w;
X	Arg args[20];
X	char title[MAXLINE];
X	int n;
X
X	/* make the help shell form-dialog widget */
X
X	n = 0;
X	XtSetArg (args[n], XmNdefaultPosition, False); n++;
X	XtSetArg (args[n], XmNautoUnmanage, False); n++;
X	XtSetArg (args[n], XmNfractionBase, 9); n++;
X	hlp_w = XmCreateFormDialog (toplevel_w, "Help", args, n);
X	XtAddCallback (hlp_w, XmNmapCallback, prompt_map_cb, NULL);
X
X	(void) sprintf (title, "xephem Help on %s", tag);
X	set_something (XtParent(hlp_w), XmNtitle, title);
X
X	/* make the Ok button */
X
X	n = 0;
X	XtSetArg (args[n], XmNbottomAttachment, XmATTACH_FORM); n++;
X	XtSetArg (args[n], XmNleftAttachment, XmATTACH_POSITION); n++;
X	XtSetArg (args[n], XmNleftPosition, 3); n++;
X	XtSetArg (args[n], XmNrightAttachment, XmATTACH_POSITION); n++;
X	XtSetArg (args[n], XmNrightPosition, 6); n++;
X	cb_w = XmCreatePushButton (hlp_w, "Ok", args, n);
X	XtAddCallback (cb_w, XmNactivateCallback, hlp_ok_cb,
X							(XtPointer)hlp_w);
X	XtManageChild (cb_w);
X
X	/* make the scrolled text area to help the help text */
X
X	n = 0;
X	XtSetArg (args[n], XmNtopAttachment, XmATTACH_FORM); n++;
X	XtSetArg (args[n], XmNbottomAttachment, XmATTACH_WIDGET); n++;
X	XtSetArg (args[n], XmNbottomWidget, cb_w); n++;
X	XtSetArg (args[n], XmNleftAttachment, XmATTACH_FORM); n++;
X	XtSetArg (args[n], XmNrightAttachment, XmATTACH_FORM); n++;
X	XtSetArg (args[n], XmNeditMode, XmMULTI_LINE_EDIT); n++;
X	XtSetArg (args[n], XmNeditable, False); n++;
X	XtSetArg (args[n], XmNcursorPositionVisible, False); n++;
X	t_w = XmCreateScrolledText (hlp_w, "ScrolledText", args, n);
X	XtManageChild (t_w);
X
X	return (t_w);
X}
X
X/* ARGSUSED */
Xstatic void
Xhlp_ok_cb (w, client, call)
XWidget w;
XXtPointer client;
XXtPointer call;
X{
X	Widget d_w = (Widget) client;
X
X	XtDestroyWidget (d_w);
X}
X
X/* open the help file and position at first line after we see "@tag\n".
X * use HELPFILE resource or hlpfdef.
X * if successfull return a FILE *, else return 0.
X */
Xstatic FILE *
Xhlp_openfile (tag)
Xchar *tag;
X{
X	static char *fn;
X	char buf[MAXLINE];
X	char tagline[MAXLINE];
X	FILE *fp;
X
X	if (!fn) {
X	    fn = XGetDefault (XtD, myclass, "HELPFILE");
X	    if (!fn)
X		fn = hlpfdef;
X	}
X
X	fp = fopen (fn, "r");
X	if (!fp)
X	    return ((FILE *)0);
X
X	(void) sprintf (tagline, "%c%s\n", HLP_TAG, tag);
X	while (fgets (buf, sizeof(buf), fp))
X	    if (strcmp (buf, tagline) == 0)
X		return (fp);
X
X	(void) fclose (fp);
X	return ((FILE *)0);
X}
X
X/* search help file for tag entry, then copy that entry into txt_w.
X * l is the number of chars already in txt_w.
X * also recursively follow any NESTed entries found.
X * return new length of txt_w, else -1 if error.
X */
Xstatic
Xhlp_fillfromfile(tag, txt_w, l)
Xchar *tag;
XWidget txt_w;
Xint l;
X{
X	FILE *fp;
X	char buf[MAXLINE];
X	
X	fp = hlp_openfile (tag);
X	if (!fp)
X	    return (-1);
X
X	while (fgets (buf, sizeof(buf), fp)) {
X	    if (buf[0] == HLP_TAG)
X		break;
X	    else if (buf[0] == HLP_NEST) {
X		int newl;
X		buf[strlen(buf)-1] = '\0';	/* remove trailing \n */
X		newl = hlp_fillfromfile (buf+1, txt_w, l);
X		if (newl > l)
X		    l = newl;
X	    } else {
X		/* buf already includes a trailing \n */
X		XmTextReplace (txt_w, l, l, buf);
X		l += strlen (buf);
X	    }
X	}
X
X	(void) fclose (fp);
X	return (l);
X}
X
Xstatic void
Xhlp_fillfromstrings(msg, nmsg, txt_w)
Xchar *msg[];
Xint nmsg;
XWidget txt_w;
X{
X	static char nohelpwarn[] = 
X	    "No HELP file found. Set XEphem.HELPFILE. Minimal Help only:\n\n";
X	int i, l;
X
X	l = 0;
X
X	XmTextReplace (txt_w, l, l, nohelpwarn);
X	l += strlen (nohelpwarn);
X
X	for (i = 0; i < nmsg; i++) {
X	    XmTextReplace (txt_w, l, l, msg[i]);
X	    l += strlen (msg[i]);
X	    XmTextReplace (txt_w, l, l, "\n");
X	    l += 1;
X	}
X}
END_OF_FILE
