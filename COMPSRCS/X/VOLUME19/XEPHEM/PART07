#! /bin/sh
# This is a shell archive.  Remove anything before this line, then feed it
# into a shell via "sh file" or similar.  To overwrite existing files,
# type "sh file -c".
# The tool that generated this appeared in the comp.sources.unix newsgroup;
# send mail to comp-sources-unix@uunet.uu.net if you want that tool.
# Contents:  calmenu.c ephem.db.Z.uu.4 time.c
# Wrapped by chris@nova on Mon May 10 16:41:44 1993
PATH=/bin:/usr/bin:/usr/ucb ; export PATH
echo If this archive is complete, you will see the following message:
echo '          "shar: End of archive 7 (of 21)."'
if test -f 'calmenu.c' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'calmenu.c'\"
else
  echo shar: Extracting \"'calmenu.c'\" \(10895 characters\)
  sed "s/^X//" >'calmenu.c' <<'END_OF_FILE'
X/* stuff to control the calendar in the main menu.
X */
X
X#include <stdio.h>
X#include <ctype.h>
X#include <math.h>
X#if defined(__STDC__)
X#include <stdlib.h>
X#endif
X#include <X11/Xlib.h>
X#include <Xm/Xm.h>
X#include <Xm/Form.h>
X#include <Xm/Label.h>
X#include <Xm/PushB.h>
X#include <Xm/CascadeB.h>
X#include <Xm/RowColumn.h>
X#include "astro.h"
X#include "circum.h"
X
X#if defined(__STDC__) || defined(__cplusplus)
X#define P_(s) s
X#else
X#define P_(s) ()
X#endif
X
Xextern Now *mm_get_now P_((void));
Xextern double mjd_day P_((double jd));
Xextern double mjd_hr P_((double jd));
Xextern void cal_mjd P_((int mn, double dy, int yr, double *Mjd));
Xextern void f_showit P_((Widget w, char *s));
Xextern void get_something P_((Widget w, char *resource, char *value));
Xextern void mjd_cal P_((double Mjd, int *mn, double *dy, int *yr));
Xextern void mjd_dow P_((double Mjd, int *dow));
Xextern void mjd_dpm P_((double Mjd, int *ndays));
Xextern void mm_newcaldate P_((double newmjd));
Xextern void moonnf P_((double Mjd, double *mjdn, double *mjdf));
Xextern void set_something P_((Widget w, char *resource, char *value));
Xextern void time_fromsys P_((Now *np));
X
XWidget calm_create P_((Widget parent));
Xvoid calm_set P_((Now *np));
Xstatic void today_cb P_((Widget w, XtPointer client, XtPointer call));
Xstatic void date_changed_cb P_((Widget w, XtPointer client, XtPointer call));
Xstatic void mm_nfmoon P_((double jd, int m, int f));
X
X#undef P_
X
X#define	CAL_ROWS	6		/* rows in the date matrix */
X#define	CAL_COLS	7		/* columns in the date matrix */
X#define	NYEARS		12		/* num entries in the year pulldown */
X#define	NMONTHS		12		/* num entries in the month pulldown */
Xstatic Widget m_w, mmenu_w[NMONTHS];	/* month cascade btn and pulldwn menu */
Xstatic Widget y_w, ymenu_w[NYEARS];	/* year cascade btn and pulldwn menu */
Xstatic Widget d_w[CAL_ROWS*CAL_COLS];	/* pushbtns in the date matrix */
X
X/* must all be fixed-width since XmMENU_BAR RowColumns don't allow for resizing.
X */
Xstatic char mnames[][10] = {
X    "January ", "February ", "March    ", "April    ", "May      ", "June     ",
X    "July    ", "August   ", "September", "October  ", "November ", "December "
X};
Xstatic char dnames[CAL_COLS][3] = {
X    "Su", "Mo", "Tu", "We", "Th", "Fr", "Sa"
X};
X
Xenum {DAY, MONTH, YEAR};
X
Xstatic Pixel fg_pix, bg_pix;		/* used to reverse colors "today" */
X
X#define	NO_DATE	(999)			/* anything outside -31..31 */
X
X/* create a calendar from parent and return the outtermost widget.
X */
XWidget
Xcalm_create (parent)
XWidget parent;
X{
X	Arg args[20];
X	Widget form_w;
X	Widget rc_w;
X	Widget menu_w;
X	Widget w;
X	int n;
X	int i;
X
X	/* create the outter form */
X
X	n = 0;
X	form_w = XmCreateForm (parent, "CalForm", args, n);
X
X	/* create the Year cascade button/pulldown menu */
X
X	n = 0;
X	XtSetArg (args[n], XmNtopAttachment, XmATTACH_FORM); n++;
X	XtSetArg (args[n], XmNtopOffset, 2); n++;
X	XtSetArg (args[n], XmNrightAttachment, XmATTACH_FORM); n++;
X	XtSetArg (args[n], XmNrightOffset, 3); n++;
X	XtSetArg (args[n], XmNrowColumnType, XmMENU_BAR); n++;
X	XtSetArg (args[n], XmNmarginHeight, 1); n++;
X	XtSetArg (args[n], XmNmarginWidth, 1); n++;
X	XtSetArg (args[n], XmNshadowThickness, 1); n++;
X	rc_w = XmCreateRowColumn (form_w, "YearRC", args, n);
X	XtManageChild (rc_w);
X
X	n = 0;
X	menu_w = XmCreatePulldownMenu (form_w, "YearPD", args, n);
X	/* managed by the CascadeButton */
X
X	    for (i = 0; i < NYEARS; i++) {
X		n = 0;
X		w = XmCreatePushButton (menu_w, "YearPB", args, n);
X		XtAddCallback (w, XmNactivateCallback, date_changed_cb,
X							    (XtPointer)YEAR);
X		XtManageChild (w);
X		ymenu_w[i] = w;
X	    }
X
X	n = 0;
X	XtSetArg (args[n], XmNsubMenuId, menu_w); n++;
X	y_w = XmCreateCascadeButton (rc_w, "YearCB", args, n);
X	XtManageChild (y_w);
X
X	/* create the Month cascade button/pulldown menu.
X	 */
X
X	n = 0;
X	XtSetArg (args[n], XmNtopAttachment, XmATTACH_FORM); n++;
X	XtSetArg (args[n], XmNtopOffset, 2); n++;
X	XtSetArg (args[n], XmNleftAttachment, XmATTACH_FORM); n++;
X	XtSetArg (args[n], XmNleftOffset, 3); n++;
X	XtSetArg (args[n], XmNrightAttachment, XmATTACH_WIDGET); n++;
X	XtSetArg (args[n], XmNrightWidget, rc_w); n++;
X	XtSetArg (args[n], XmNrowColumnType, XmMENU_BAR); n++;
X	XtSetArg (args[n], XmNmarginHeight, 1); n++;
X	XtSetArg (args[n], XmNmarginWidth, 1); n++;
X	XtSetArg (args[n], XmNshadowThickness, 1); n++;
X	rc_w = XmCreateRowColumn (form_w, "MonthRC", args, n);
X	XtManageChild (rc_w);
X
X	n = 0;
X	menu_w = XmCreatePulldownMenu (form_w, "MonthPD", args, n);
X	/* managed by the CascadeButton */
X
X	    for (i = 0; i < NMONTHS; i++) {
X		n = 0;
X		XtSetArg (args[n], XmNuserData, i+1); n++;
X		w = XmCreatePushButton (menu_w, mnames[i], args, n);
X		XtAddCallback (w, XmNactivateCallback, date_changed_cb,
X							    (XtPointer)MONTH);
X		XtManageChild (w);
X		mmenu_w[i] = w;
X	    }
X
X	n = 0;
X	XtSetArg (args[n], XmNsubMenuId, menu_w); n++;
X	m_w = XmCreateCascadeButton (rc_w, "MonthCB", args, n);
X	XtManageChild (m_w);
X
X	/* create the rowcol for the main date matrix */
X
X	n = 0;
X	XtSetArg (args[n], XmNtopAttachment, XmATTACH_WIDGET); n++;
X	XtSetArg (args[n], XmNtopWidget, rc_w); n++;
X	XtSetArg (args[n], XmNleftAttachment, XmATTACH_FORM); n++;
X	XtSetArg (args[n], XmNrightAttachment, XmATTACH_FORM); n++;
X	XtSetArg (args[n], XmNnumColumns, CAL_ROWS+1); n++; /* +1 for dnames*/
X	XtSetArg (args[n], XmNpacking, XmPACK_COLUMN); n++;
X	XtSetArg (args[n], XmNorientation, XmHORIZONTAL); n++;
X	XtSetArg (args[n], XmNentryAlignment, XmALIGNMENT_END); n++;
X	XtSetArg (args[n], XmNadjustMargin, False); n++;
X	XtSetArg (args[n], XmNspacing, 0); n++;
X	rc_w = XmCreateRowColumn (form_w, "CalRC", args, n);
X	XtManageChild (rc_w);
X
X	/* add the fixed day abbreviations */
X
X	for (i = 0; i < XtNumber(dnames); i++) {
X	    n = 0;
X	    XtSetArg (args[n], XmNmarginLeft, 1); n++;	/* allow for buttons' */
X	    XtSetArg (args[n], XmNmarginRight, 1); n++;	/* allow for buttons' */
X	    w = XmCreateLabel (rc_w, dnames[i], args, n);
X	    XtManageChild (w);
X	}
X
X	/* add the calendar entries proper */
X
X	for (i = 0; i < CAL_ROWS*CAL_COLS; i++) {
X	    n = 0;
X	    XtSetArg (args[n], XmNrecomputeSize, False); n++;
X	    XtSetArg (args[n], XmNshadowThickness, 1); n++;
X	    d_w[i] = XmCreatePushButton (rc_w, "CD", args, n);
X	    XtAddCallback (d_w[i], XmNactivateCallback, date_changed_cb,
X								(XtPointer)DAY);
X	    XtManageChild (d_w[i]);
X	}
X
X	/* add the Today button */
X
X	n = 0;
X	XtSetArg (args[n], XmNtopAttachment, XmATTACH_WIDGET); n++;
X	XtSetArg (args[n], XmNtopOffset, 3); n++;
X	XtSetArg (args[n], XmNtopWidget, rc_w); n++;
X	XtSetArg (args[n], XmNleftAttachment, XmATTACH_FORM); n++;
X	XtSetArg (args[n], XmNrightAttachment, XmATTACH_FORM); n++;
X	w = XmCreatePushButton (form_w, "Today", args, n);
X	XtManageChild (w);
X	XtAddCallback (w, XmNactivateCallback, today_cb, NULL);
X
X	get_something (d_w[0], XmNforeground, (char *)&fg_pix);
X	get_something (d_w[0], XmNbackground, (char *)&bg_pix);
X
X	return (form_w);
X}
X
X/* set the calendar to the UT time of the given Now *np.
X * don't bother if still same day as before.
X */
Xvoid
Xcalm_set (np)
XNow *np;
X{
X	static double last_mjdday;
X	static int rv_date;
X	double mjdday;
X	char buf[32];
X	int m, y;
X	double d;
X	int f;		/* day of week of first day of month Sun=0 */
X	int nd;		/* number of days in this month */
X	double jd0;	/* mjd of first day of the month */
X	int i;
X
X	/* don't bother if it's still the same day */
X	mjdday = mjd_day (mjd);
X	if (last_mjdday == mjdday)
X	    return;
X	last_mjdday = mjdday;
X
X	/* get today's month, day, year */
X	mjd_cal (mjd, &m, &d, &y);
X
X	/* find day of week of first day of month */
X	cal_mjd (m, 1.0, y, &jd0);
X	mjd_dow (jd0, &f);
X
X	/* undo the current reverse video date */
X	set_something (d_w[rv_date], XmNforeground, (char *)fg_pix);
X	set_something (d_w[rv_date], XmNbackground, (char *)bg_pix);
X
X	/* label the month */
X	f_showit (m_w, mnames[m-1]);
X
X	/* label the year and set the menu entries around it */
X	(void) sprintf (buf, "%d", y);
X	f_showit (y_w, buf);
X	for (i = 0; i < NYEARS; i++) {
X	    int ty = (y - NYEARS/3) + i; /* current year about 1/3 down list */
X	    (void) sprintf (buf, "%d", ty);
X	    f_showit (ymenu_w[i], buf);
X	    set_something (ymenu_w[i], XmNuserData, (char *)ty);
X	}
X
X
X	if (f < 0) {
X	    /* can't figure it out - too hard before Gregorian */
X	    for (i = 0; i < CAL_ROWS*CAL_COLS; i++) {
X		set_something (d_w[i], XmNuserData, (char *)NO_DATE);
X		f_showit (d_w[i], "  ");
X	    }
X	    return;
X	}
X
X	/* find number of days in this month */
X	mjd_dpm (jd0, &nd);
X
X	/* print the calendar.
X	 * set userData to the day of the month.
X	 * use reverse colors for today.
X	 */
X	for (i = 0; i < CAL_ROWS*CAL_COLS; i++) {
X	    if (i == f + (int)d - 1) {
X		set_something (d_w[i], XmNforeground, (char *)bg_pix);
X		set_something (d_w[i], XmNbackground, (char *)fg_pix);
X		rv_date = i;
X	    }
X	    if (i < f || i > f + nd - 1) {
X		f_showit (d_w[i], "  ");
X	    } else {
X		(void) sprintf (buf, "%2d", i-f+1);
X		f_showit (d_w[i], buf);
X	    }
X	    set_something (d_w[i], XmNuserData, (char *)(i-f+1));
X	}
X
X	/* over print the new and full moons for this month.
X	 * TODO: don't really know which dates to use here (see moonnf())
X	 *   so try several to be fairly safe. have to go back to 4/29/1988
X	 *   to find the full moon on 5/1 for example.
X	 */
X	mm_nfmoon (jd0-3, m, f);
X	mm_nfmoon (jd0+15, m, f);
X}
X
X/* called when Today is pressed.
X * set m/d/y but retain any partion day.
X */
X/* ARGSUSED */
Xstatic void
Xtoday_cb (w, client, call)
XWidget w;
XXtPointer client;
XXtPointer call;
X{
X	Now *np = mm_get_now();
X	double newmjd;
X	int m, y;
X	double d;
X
X	mjd_cal (mjd, &m, &d, &y);
X	time_fromsys (np);
X	newmjd = mjd_day(mjd) + (d - floor(d));
X	mm_newcaldate (newmjd);
X}
X
X/* called when any of the calendar pushbuttons is activated.
X * client is one of DAY, MONTH or YEAR.
X */
X/* ARGSUSED */
Xstatic void
Xdate_changed_cb (w, client, call)
XWidget w;
XXtPointer client;
XXtPointer call;
X{
X	Now *np = mm_get_now();
X	int code = (int)client;
X	double newmjd;
X	int x;
X	int m, y;
X	double d;
X
X	get_something (w, XmNuserData, (char *)&x);
X	if (x == NO_DATE)
X	    return;
X
X	mjd_cal (mjd, &m, &d, &y);
X
X	switch (code) {
X	case MONTH:
X	    cal_mjd (x, d, y, &newmjd);
X	    break;
X	case DAY:
X	    d = x + mjd_hr(mjd)/24.0;	/* preserve any partial day */
X	    cal_mjd (m, d, y, &newmjd);
X	    break;
X	case YEAR:
X	    cal_mjd (m, d, x, &newmjd);
X	    break;
X	}
X
X	mm_newcaldate (newmjd);
X}
X
X/* print the new and full moons for the month contained mjd jd, where
X * m is the month code for the current month and f is the day-of-the-week
X * of the first day of the month.
X */
Xstatic void
Xmm_nfmoon (jd, m, f)
Xdouble jd;
Xint m, f;
X{
X	static char nms[] = "NM", fms[] = "FM";
X	int mm, ym;
X	double dm;
X	double jdn, jdf;	/* mjd of new and full moons near jd */
X	int di;
X
X	moonnf (jd, &jdn, &jdf);
X	mjd_cal (jdn, &mm, &dm, &ym);
X	if (m == mm) {
X	    di = dm + f - 1;
X	    f_showit (d_w[di], nms);
X	}
X	mjd_cal (jdf, &mm, &dm, &ym);
X	if (m == mm) {
X	    di = dm + f - 1;
X	    f_showit (d_w[di], fms);
X	}
X}
END_OF_FILE
  if test 10895 -ne `wc -c <'calmenu.c'`; then
    echo shar: \"'calmenu.c'\" unpacked with wrong size!
  fi
  # end of 'calmenu.c'
