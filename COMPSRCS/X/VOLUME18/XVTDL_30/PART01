X	0xC000,0x0660,0x0000,0x1800,
X	0xC000,0x0780,0x0000,0x1800,
X	0xC000,0x0E00,0x0000,0x1800,
SHAR_EOF
true || echo 'restore of bitmaps/tdl.pr failed'
fi
echo 'End of  part 1'
echo 'File bitmaps/tdl.pr is continued in part 2'
echo 2 > _shar_seq_.tmp
exit 0
