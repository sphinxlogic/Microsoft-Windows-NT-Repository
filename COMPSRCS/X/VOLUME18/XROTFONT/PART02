chmod 0644 hersheyfonts/romant ||
echo 'restore of hersheyfonts/romant failed'
Wc_c="`wc -c < 'hersheyfonts/romant'`"
test 9196 -eq "$Wc_c" ||
	echo 'hersheyfonts/romant: original size 9196, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
# ============= hersheyfonts/scriptc ==============
