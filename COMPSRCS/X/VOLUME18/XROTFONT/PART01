chmod 0644 hersheyfonts/gothgrt ||
echo 'restore of hersheyfonts/gothgrt failed'
Wc_c="`wc -c < 'hersheyfonts/gothgrt'`"
test 10430 -eq "$Wc_c" ||
	echo 'hersheyfonts/gothgrt: original size 10430, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
# ============= hersheyfonts/gothitt ==============
