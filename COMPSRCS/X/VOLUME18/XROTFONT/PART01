chmod 0644 hersheyfonts/gothitt ||
echo 'restore of hersheyfonts/gothitt failed'
Wc_c="`wc -c < 'hersheyfonts/gothitt'`"
test 8481 -eq "$Wc_c" ||
	echo 'hersheyfonts/gothitt: original size 8481, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
