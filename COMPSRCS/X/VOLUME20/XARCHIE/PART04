#!/bin/sh
# this is Part.04 (part 4 of xarchie-2.0.6)
# do not concatenate these parts, unpack them in order with /bin/sh
# file xarchie-2.0.6/Xarchie.ad continued
#
if test ! -r _shar_seq_.tmp; then
	echo 'Please unpack part 1 first!'
	exit 1
fi
(read Scheck
 if test "$Scheck" != 4; then
	echo Please unpack part "$Scheck" next!
	exit 1
 else
	exit 0
 fi
) < _shar_seq_.tmp || exit 1
if test ! -f _shar_wnt_.tmp; then
	echo 'x - still skipping xarchie-2.0.6/Xarchie.ad'
else
echo 'x - continuing file xarchie-2.0.6/Xarchie.ad'
sed 's/^X//' << 'SHAR_EOF' >> 'xarchie-2.0.6/Xarchie.ad' &&
XXarchie*settingsForm*setHostWeightsLabel.horizDistance:	100
XXarchie*settingsForm*setHostWeightsLabel.mappedWhenManaged: False
XXarchie*settingsForm*setHostWeightsText.borderWidth:	1
XXarchie*settingsForm*setHostWeightsText.fromVert:	setHostWeightsLabel
XXarchie*settingsForm*setHostWeightsText.fromHoriz:	setSearchButton
XXarchie*settingsForm*setHostWeightsText.horizDistance:	100
XXarchie*settingsForm*setHostWeightsText.mappedWhenManaged: False
XXarchie*settingsForm*setHostWeightsText.height:		60
XXarchie*settingsForm*setHostWeightsText.width:		205
XXarchie*settingsForm*setHostWeightsText.scrollVertical:	always
XXarchie*settingsForm*setHostWeightsText.scrollHorizontal: whenNeeded
XXarchie*settingsForm*setHostWeightsText.translations:#override\n\
X	<Key>Return:	newline() \n
X
!	-	-	-	-	-	-	-	-	-
! File panel (for save, load, and write)
