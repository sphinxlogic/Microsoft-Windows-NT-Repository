SHAR_EOF
echo 'File xarchie-2.0.6/Xarchie.ad is complete' &&
chmod 0644 xarchie-2.0.6/Xarchie.ad ||
echo 'restore of xarchie-2.0.6/Xarchie.ad failed'
Wc_c="`wc -c < 'xarchie-2.0.6/Xarchie.ad'`"
test 36627 -eq "$Wc_c" ||
	echo 'xarchie-2.0.6/Xarchie.ad: original size 36627, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
# ============= xarchie-2.0.6/Xarchie.ad.h ==============
if test -f 'xarchie-2.0.6/Xarchie.ad.h' -a X"$1" != X"-c"; then
	echo 'x - skipping xarchie-2.0.6/Xarchie.ad.h (File already exists)'
	rm -f _shar_wnt_.tmp
else
> _shar_wnt_.tmp
echo 'x - extracting xarchie-2.0.6/Xarchie.ad.h (Text)'
sed 's/^X//' << 'SHAR_EOF' > 'xarchie-2.0.6/Xarchie.ad.h' &&
"Xarchie*archieHost:	archie.sura.net(1526)",
"Xarchie*searchType:	exact",
"Xarchie*sortType:	name",
"Xarchie*maxHits:	99",
"Xarchie*offset:		0",
"Xarchie*timeout:	4",
"Xarchie*retries:	3",
"Xarchie*niceLevel:	0",
"Xarchie*ftpMailAddress:	%s@%s",
"Xarchie*ftpType:	binary",
"Xarchie*ftpPrompt:	True",
"Xarchie*ftpTrace:	False",
"Xarchie*ftpStrip:	True",
"Xarchie*fileWriteOnePerLine:	False",
"Xarchie*autoScroll:	True",
"Xarchie*pasteBuffer:	True",
"Xarchie*debugLevel:	0",
"Xarchie*xarchieFont:		*-fixed-medium-r-*-*-13-*-*-*-*-*-iso8859-*",
