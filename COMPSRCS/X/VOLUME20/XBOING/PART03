#! /bin/sh
# This is a shell archive.  Remove anything before this line, then feed it
# into a shell via "sh file" or similar.  To overwrite existing files,
# type "sh file -c".
# Contents:  audio/SUNaudio.c sounds/intro.au.uue
# Wrapped by chris@sparky on Fri Sep  3 07:14:41 1993
PATH=/bin:/usr/bin:/usr/ucb:/usr/local/bin:/usr/lbin ; export PATH
echo If this archive is complete, you will see the following message:
echo '          "shar: End of archive 3 (of 26)."'
if test -f 'audio/SUNaudio.c' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'audio/SUNaudio.c'\"
else
  echo shar: Extracting \"'audio/SUNaudio.c'\" \(9176 characters\)
  sed "s/^X//" >'audio/SUNaudio.c' <<'END_OF_FILE'
X#include "copyright.h"
X
X/* SUN Audio format - original code from play.c by Sun */
X
X/*
X *  Include file dependencies:
X */
X
X#include <stdio.h>
X#include <stdlib.h>
X#include <stddef.h>
X#include <ctype.h>
X#include <errno.h>
X#include <sys/stat.h>
X#include <sys/fcntl.h>
X#include <sys/signal.h>
X
X#include <libaudio.h>
X#include <audio_device.h>
X
X#include "error.h"
X
X#include "audio.h"
X
X/*
X *  Internal macro definitions:
X */
X
X#define	MAX_GAIN				(100)		/* maximum gain */
X#define	SAMPLE_RATE_THRESHOLD	(.01)
X#define BUFFER_SIZE				(1024 * 128)	/* 128K yup */
X
X/*
X *  Internal type declarations:
X */
X
X#if NeedFunctionPrototypes
Xstatic void flushAudioDevice(void);
Xstatic int reconfig(void);
Xstatic void sigalarm_handler(void);
X#else
Xstatic void sigalarm_handler();
Xstatic void flushAudioDevice();
Xstatic int reconfig();
X#endif
X
X/*
X *  Internal variable declarations:
X */
X
XAudio_hdr		Dev_hdr;			/* audio header for device */
XAudio_hdr		File_hdr;			/* audio header for file */
X
Xunsigned char	buf[BUFFER_SIZE];	/* size should depend on sample_rate */
Xdouble			Savevol;			/* saved volume level */
Xdouble			maxVolume;			/* maximum volume level */
Xchar			*Audio_dev = "/dev/audio";
Xint				Audio_fd = -1;		/* file descriptor for audio device */
Xint 			err;
Xstruct stat     st;
Xdouble          vol;
Xint             cnt;
Xchar			errorString[255];
X
X#if NeedFunctionPrototypes
Xint SetUpAudioSystem(Display *display)
X#else
Xint SetUpAudioSystem(display)
X	Display *display;
X#endif
X{
X	/* Validate and open the audio device */
X	if (stat(Audio_dev, &st) < 0)
X	{
X		/* No audio device so barf */
X		sprintf(errorString, 
X			"Error: Cannot stat audio device %s.", Audio_dev);
X		ErrorMessage(errorString);
X		return False;
X	}
X
X	/* Check that the audio device is a character device */
X	if (!S_ISCHR(st.st_mode)) 
X	{
X		/* Not a character device which is not right */
X		sprintf(errorString, "Error: %s is not an audio device.", Audio_dev);
X		ErrorMessage(errorString);
X		return False;
X	}
X
X	/*
X	 * We open the device without the O_NDELAY flag so that someone else
X	 * who has the device can release it to us.  If we use the O_NDELAY
X	 * flag, the open will fail even if the process that has the device is
X	 * willing to release it.  Unfortunately, an ill-behaved process may not
X	 * be willing to give up the device.  In this case, the open will hang.
X	 * To prevent this, we wrap an alarm around the open.  If the open hangs
X	 * and the alarm goes off, the open will return with errno = EINTR.
X	 * suggestion by: jra@hrcms.jazz.att.com (Jeffry R. Abramson)
X	 */
X	if (signal(SIGALRM, sigalarm_handler) < 0) 
X	{
X		ErrorMessage("Error: signal(SIGALRM) failed.");
X		return False;
X	}
X
X	alarm(2);
X	/* Try to open audio device */
X	Audio_fd = open(Audio_dev, O_WRONLY);
X	alarm(0);
X
X	if ((Audio_fd < 0) && (errno == EBUSY || errno == EINTR)) 
X	{
X		/* The audio is in use so barf */
X		sprintf(errorString, "Error: %s audio device is busy.", Audio_dev);
X		ErrorMessage(errorString);
X		return False;
X	}
X
X	/* Ok so we cannot open it for some reason */
X	if (Audio_fd < 0) 
X	{
X		sprintf(errorString, "Error: Cannot open audio device %s.", Audio_dev);
X		ErrorMessage(errorString);
X		return False;
X	}
X
X	/* Get the device output encoding configuration */
X	if (audio_get_play_config(Audio_fd, &Dev_hdr) != AUDIO_SUCCESS) 
X	{
X		sprintf(errorString, "Error: %s is not an audio device.", Audio_dev);
X		ErrorMessage(errorString);
X		return False;
X	}
X
X	/* Get the previous volume so we can reset it later */
X	(void) audio_get_play_gain(Audio_fd, &Savevol);
X
X	/* Clear any active audio for the new sound */
X	flushAudioDevice();
X
X	/* Success in opening audio device */
X	return True;
X}
X
X#if NeedFunctionPrototypes
Xvoid FreeAudioSystem(void)
X#else
Xvoid FreeAudioSystem()
X#endif
X{
X	/* Clear any active audio for the new sound */
X	flushAudioDevice();
X
X	/*
X	 * Though drain is implicit on close(), it's performed here
X	 * for the sake of completeness, and to ensure that the volume
X	 * is reset after all output is complete.
X	 */
X	(void) audio_drain(Audio_fd, FALSE);
X
X	/* Reset the volume to the saved volume */
X	(void) audio_set_play_gain(Audio_fd, &Savevol);
X	
X	/* Close the audio device thanks */
X	(void) close(Audio_fd);
X}
X
X#if NeedFunctionPrototypes
Xstatic void flushAudioDevice(void)
X#else
Xstatic void flushAudioDevice()
X#endif
X{
X	/* Flush any audio activity */
X	(void) audio_flush_play(Audio_fd);
X}
X
X#if NeedFunctionPrototypes
Xvoid SetMaximumVolume(int Volume)
X#else
Xvoid SetMaximumVolume(Volume)
X	int Volume;
X#endif
X{
X	/* Set the maximum volume for the audio system */
X	maxVolume = (double) (Volume / 100);
X}
X
X#if NeedFunctionPrototypes
Xstatic void setNewVolume(unsigned int Volume)
X#else
Xstatic void setNewVolume(Volume)
X	unsigned int Volume;
X#endif
X{
X	double oldVol, modVol;
X
X	/* Use the saved volume as maximum or use the maxvolume set by user */
X	if (maxVolume == 0.0)
X		oldVol = (Savevol * MAX_GAIN) / 100;
X	else
X		oldVol = (maxVolume * MAX_GAIN) / 100;
X
X	modVol = Volume * oldVol;
X
X	/* volume is double value between 0 and 1 */
X	vol = modVol / (double) MAX_GAIN;
X	err = audio_set_play_gain(Audio_fd, &vol);
X
X	/* Check if the volume was set correctly */
X	if (err != AUDIO_SUCCESS) 
X	{
X		/* Nup - issue an error message */
X		sprintf(errorString, "Error: Unable to set output volume for %s\n",
X		    Audio_dev);
X		ErrorMessage(errorString);
X	}
X}
X
X#if NeedFunctionPrototypes
Xvoid playSoundFile(char *filename, int volume)
X#else
Xvoid playSoundFile(filename, volume)
X	char *filename;
X	int volume;
X#endif
X{
X	int ifd;
X	char soundfile[1024];
X	char *str;
X
X	/* Clear any active audio for the new sound */
X	flushAudioDevice();
X
X	/* Set to the required volume */
X 	setNewVolume((unsigned) volume);
X
X	/* Construct the sounds file path and use env var if exists */
X    if ((str = getenv("XBOING_SOUND_DIR")) != NULL)
X    	sprintf(soundfile, "%s/%s.au", str, filename);
X    else            
X        sprintf(soundfile, "%s/%s.au", SOUNDS_DIR, filename);
X
X	/* Open the sound file for reading */
X	if ((ifd = open(soundfile, O_RDONLY, 0)) < 0) 
X	{
X		/* Issue an error about not opening sound file */
X		sprintf(errorString, 
X			"Warning: Unable to open sound file %s.", soundfile);
X		ErrorMessage(errorString);
X		return;
X	}
X
X	/* Check to make sure this is an audio file */
X	err = audio_read_filehdr(ifd, &File_hdr, (char *)NULL, 0);
X	if (err != AUDIO_SUCCESS) 
X	{
X		/* Cannot understand the sound file so close file and return */
X		sprintf(errorString, 
X			"Warning: Unable to parse sound file %s.", soundfile);
X		ErrorMessage(errorString);
X		goto closeinput;
X	}
X
X	/* Check the device configuration */
X	if (audio_cmp_hdr(&Dev_hdr, &File_hdr) != 0) 
X	{
X		/*
X		 * The device does not match the input file.
X		 * Wait for any old output to drain, then attempt
X		 * to reconfigure the audio device to match the
X		 * input data.
X		 */
X		if (audio_drain(Audio_fd, FALSE) != AUDIO_SUCCESS) 
X		{
X			/* Cannot drain the audio device so barf */
X			sprintf(errorString, "Error: Unable to drain the audio device.");
X			ErrorMessage(errorString);
X			return;
X		}
X
X		if (!reconfig()) goto closeinput;
X	}
X
X	/* At this point, we're all ready to copy the data. */
X	while ((cnt = read(ifd, (char *)buf, BUFFER_SIZE)) >= 0) 
X	{
X		/* If input EOF, write an eof marker */
X		err = write(Audio_fd, (char *)buf, cnt);
X		if (err != cnt) 
X		{
X			/* Did we succeed in writing all the sound out */
X			sprintf(errorString, 
X				"Warning: Problem while writing to audio device.");
X			ErrorMessage(errorString);
X			break;
X		}
X
X		if (cnt == 0) break;
X	}
X
X	if (cnt < 0) 
X	{
X		/* Some error - while reading - notify user */
X		sprintf(errorString, 
X			"Warning: Problem while reading soundfile %s", soundfile);
X		ErrorMessage(errorString);
X	}
X
Xcloseinput:
X	/* Close the sound file */
X	(void) close(ifd);
X}
X
X#if NeedFunctionPrototypes
Xvoid audioDeviceEvents(void)
X#else
Xvoid audioDeviceEvents()
X#endif
X{
X	/* None to do */
X}
X
X#if NeedFunctionPrototypes
Xstatic int reconfig(void)
X#else
Xstatic int reconfig()
X#endif
X{
X	/*
X	 * Try to reconfigure the audio device to match the file encoding.
X	 * If this fails, we should attempt to make the input data match the
X	 * device encoding.  For now, we give up on this file.
X	 *
X	 * Returns TRUE if successful.  Returns FALSE if not.
X	 */
X
X	char	msg[AUDIO_MAX_ENCODE_INFO];
X
X	Dev_hdr = File_hdr;
X	err = audio_set_play_config(Audio_fd, &Dev_hdr);
X
X	switch (err) 
X	{
X		case AUDIO_SUCCESS:
X			return (TRUE);
X
X		case AUDIO_ERR_NOEFFECT:
X			/*
X			 * Couldn't change the device.
X			 * Check to see if we're nearly compatible.
X			 * audio_cmp_hdr() returns >0 if only sample rate difference.
X			 */
X			if (audio_cmp_hdr(&Dev_hdr, &File_hdr) > 0) 
X			{
X				double	ratio;
X	
X				ratio = (double) abs((int)
X				    (Dev_hdr.sample_rate - File_hdr.sample_rate)) /
X				    (double) File_hdr.sample_rate;
X	
X				if (ratio <= SAMPLE_RATE_THRESHOLD) 
X					return (TRUE);
X	
X				return (FALSE);
X			}
X	
X			(void) audio_enc_to_str(&File_hdr, msg);
X	
X			return (FALSE);
X	
X		default:
X			sprintf(errorString, 
X				"Error: I/O error (set config) audio device.");
X			ErrorMessage(errorString);
X	}
X}
X
X#if NeedFunctionPrototypes
Xstatic void sigalarm_handler(void)
X#else
Xstatic void sigalarm_handler()
X#endif
X{
X	/* Used to fix problem when opening the audio device */
X	return;
X}
END_OF_FILE
  if test 9176 -ne `wc -c <'audio/SUNaudio.c'`; then
    echo shar: \"'audio/SUNaudio.c'\" unpacked with wrong size!
  fi
  # end of 'audio/SUNaudio.c'
fi
if test -f 'sounds/intro.au.uue' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'sounds/intro.au.uue'\"
else
  echo shar: Extracting \"'sounds/intro.au.uue'\" \(42645 characters\)
  sed "s/^X//" >'sounds/intro.au.uue' <<'END_OF_FILE'
