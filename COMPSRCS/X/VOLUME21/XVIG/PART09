# into a shell via "sh file" or similar.  To overwrite existing files,
# type "sh file -c".
# Contents:  version_1.1/examples/Makefile.std
#   version_1.1/examples/colortest.c version_1.1/lib/EMPTY
#   version_1.1/man/xvig.ps.5 version_1.1/src/empty.xbm
# Wrapped by chris@sparky on Tue Nov 23 11:17:59 1993
PATH=/bin:/usr/bin:/usr/ucb:/usr/local/bin:/usr/lbin:$PATH ; export PATH
echo If this archive is complete, you will see the following message:
echo '          "shar: End of archive 9 (of 10)."'
if test -f 'version_1.1/examples/Makefile.std' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'version_1.1/examples/Makefile.std'\"
else
  echo shar: Extracting \"'version_1.1/examples/Makefile.std'\" \(843 characters\)
  sed "s/^X//" >'version_1.1/examples/Makefile.std' <<'END_OF_FILE'
X### ANSI-C compiler for HP-UX and load-flags
XCC = /bin/cc -Aa
XLDFLAGS = -L/usr/lib/X11R5
X
X### ANSI-C compiler for DecStation and load-flags
X#CC = /usr/bin/c89
X#LDFLAGS =
X
X### ANSI-C compiler for SUN-SPARC and load-flags
X#CC = /usr/lang/acc
X#LDFLAGS =
X
X
X
XIDIR = $(XVIGDIR)/include
XLIB  = $(XVIGDIR)/lib/xvig.a
X
Xall: colortest cursortest sensetest texttest
X
Xcolortest: colortest.c $(IDIR)/xvig.h
X	$(CC) colortest.c -I$(IDIR) $(LIB) $(LDFLAGS) -lX11 -o colortest
X
Xcursortest: cursortest.c $(IDIR)/xvig.h
X	$(CC) cursortest.c -I$(IDIR) $(LIB) $(LDFLAGS) -lX11 -o cursortest
X
Xsensetest: sensetest.c $(IDIR)/xvig.h
X	$(CC) sensetest.c -I$(IDIR) $(LIB) $(LDFLAGS) -lX11 -lm -o sensetest
X
Xtexttest: texttest.c $(IDIR)/xvig.h
X	$(CC) texttest.c -I$(IDIR) $(LIB) $(LDFLAGS) -lX11 -lm -o texttest
X
Xclean:
X	/bin/rm -f colortest cursortest sensetest texttest
END_OF_FILE
  if test 843 -ne `wc -c <'version_1.1/examples/Makefile.std'`; then
    echo shar: \"'version_1.1/examples/Makefile.std'\" unpacked with wrong size!
  fi
  # end of 'version_1.1/examples/Makefile.std'
fi
if test -f 'version_1.1/examples/colortest.c' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'version_1.1/examples/colortest.c'\"
else
  echo shar: Extracting \"'version_1.1/examples/colortest.c'\" \(919 characters\)
  sed "s/^X//" >'version_1.1/examples/colortest.c' <<'END_OF_FILE'
X#include <stdlib.h>
X#include <stdio.h>
X#include "xvig.h"
X
Xmain()
X{
X  char win_name[] = "Colors";
X  unsigned int window_width = 700, window_height = 400;
X  int x, x2, y, i, chr;
X
X  /* -- Initialize -- */
X
X  if (!XviG_Init("ColorTest", NULL, 0))
X    exit(1);
X
X  /* -- Open the window -- */
X
X  if (!XviG_OpenWindow(win_name, -1, -1, &window_width, &window_height))
X  {
X    XviG_Exit();
X    exit(1);
X  }
X
X  /* -- Draw the colors -- */
X
X  for (x = 0, i = 0; i < XviG_NR_OF_COLORS; i++)
X  {
X    x2 = window_width * (i + 1) / XviG_NR_OF_COLORS;
X    XviG_SetColor(i);
X    XviG_FillRectangle(x, 0, x2, window_height);
X    x = x2;
X  }
X
X  /* -- Ask for a 'q' or 'Q' keypress in the window -- */
X
X  while (((chr = XviG_GetCursor(XviG_KEY, &x, &y)) != 'q') && (chr != 'Q'));
X
X  /* -- Close the window -- */
X
X  if (!XviG_CloseWindow(win_name))
X  {
X    XviG_Exit();
X    exit(1);
X  }
X
X  /* -- Exit -- */
X
X  XviG_Exit();
X
X  return 0;
X}
END_OF_FILE
  if test 919 -ne `wc -c <'version_1.1/examples/colortest.c'`; then
    echo shar: \"'version_1.1/examples/colortest.c'\" unpacked with wrong size!
  fi
  # end of 'version_1.1/examples/colortest.c'
fi
