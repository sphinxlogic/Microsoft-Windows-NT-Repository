X-- Comment  :
X--
X-- Review   :
X--
X*/
X
X
X#ifndef __COMMONDATA_H
X#define __COMMONDATA_H
X
X
X/*
X-- The version string
X*/
X
X#define XviG_VERSION  "Version_1.1.0"
