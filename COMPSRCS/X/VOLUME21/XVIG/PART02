X#define MESSAGE_QUIT          "_XVIG_MESSAGE_QUIT"
X
X
X/*
X-- The data for the ClientMessage events
X*/
X
X#define DATA_SENSEKBD_YES  ((char) 1)
X#define DATA_SENSEKBD_NO   ((char) 0)
X#define DATA_CURSOR_ARROW  0L
