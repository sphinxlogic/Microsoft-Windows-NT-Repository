X
X/*
X-- The border width for the windows
X*/
X
X#define BORDER_WIDTH  4
X
X
X/*
X-- The Atom strings for the ClientMessage events
X*/
X
X#define MESSAGE_INIT          "_XVIG_MESSAGE_INIT"
X#define MESSAGE_KEY           "_XVIG_MESSAGE_KEY"
X#define MESSAGE_BUTTON        "_XVIG_MESSAGE_BUTTON"
X#define MESSAGE_KEY_BUTTON    "_XVIG_MESSAGE_KEY_BUTTON"
