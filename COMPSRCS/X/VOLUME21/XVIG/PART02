    echo shar: \"'version_1.1/man/xvig.ps.9'\" unpacked with wrong size!
  fi
  # end of 'version_1.1/man/xvig.ps.9'
fi
if test -f 'version_1.1/src/commondata.h' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'version_1.1/src/commondata.h'\"
else
  echo shar: Extracting \"'version_1.1/src/commondata.h'\" \(1392 characters\)
  sed "s/^X//" >'version_1.1/src/commondata.h' <<'END_OF_FILE'
X/* File>>> commondata.h
