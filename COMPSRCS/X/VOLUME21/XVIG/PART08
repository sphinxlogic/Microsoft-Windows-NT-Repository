X-- Global include files
X------------------------------------------------------------------------------*/
X
X#include <stdlib.h>
X#include <stdio.h>
X#include <string.h>
X#include <X11/Xlib.h>
X
X/*------------------------------------------------------------------------------
X-- Local include files
X------------------------------------------------------------------------------*/
X
X#include "xviglocal.h"
