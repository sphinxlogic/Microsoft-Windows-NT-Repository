END_OF_FILE
  if test 2736 -ne `wc -c <'README'`; then
    echo shar: \"'README'\" unpacked with wrong size!
  fi
  # end of 'README'
fi
if test -f 'Imakefile' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'Imakefile'\"
else
  echo shar: Extracting \"'Imakefile'\" \(552 characters\)
  sed "s/^X//" >'Imakefile' <<'END_OF_FILE'
X#ifndef FontObj
X/* support X11R4 */
X#define FontObj(basename) basename.snf
X#define SnfFonts
X#endif
X
XOBJS = FontObj (wntml10-100) FontObj (wntml10-150) \
X       FontObj (wntml12-100) FontObj (wntml12-150)
X
XFONTINSTDIR = $(FONTDIR)/tamil
X
XMakeFonts ()
X
XFontTarget (wntml10-100)
XFontTarget (wntml10-150)
XFontTarget (wntml12-100)
XFontTarget (wntml12-150)
X
XInstallFonts ($(FONTINSTDIR))
XInstallFontAliases ($(FONTINSTDIR))
X
X#ifdef SnfFonts
Xfontdir:
X	@if [ -d $(FONTINSTDIR) ]; then set +x; \
X		else (set -x; mkdir $(FONTINSTDIR)); fi
X#else
Xfontdir:
X
X#endif
END_OF_FILE
  if test 552 -ne `wc -c <'Imakefile'`; then
    echo shar: \"'Imakefile'\" unpacked with wrong size!
  fi
  # end of 'Imakefile'
fi
if test -f 'wntml12-150.bdf' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'wntml12-150.bdf'\"
else
  echo shar: Extracting \"'wntml12-150.bdf'\" \(39888 characters\)
  sed "s/^X//" >'wntml12-150.bdf' <<'END_OF_FILE'
