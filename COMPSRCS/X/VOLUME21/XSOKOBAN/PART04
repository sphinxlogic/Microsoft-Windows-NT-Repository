PATH=/bin:/usr/bin:/usr/ucb:/usr/local/bin:/usr/lbin:$PATH ; export PATH
echo If this archive is complete, you will see the following message:
echo '          "shar: End of archive 4 (of 4)."'
if test -f 'xsokoban-3.0/Imakefile' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'xsokoban-3.0/Imakefile'\"
else
  echo shar: Extracting \"'xsokoban-3.0/Imakefile'\" \(244 characters\)
  sed "s/^X//" >'xsokoban-3.0/Imakefile' <<'END_OF_FILE'
XEXTRA_DEFINES=	-DNDEBUG
XCC=gcc
X
XSYS_LIBRARIES=	$(XLIB)
XSRCS=		display.c main.c resources.c play.c score.c screen.c save.c
XOBJS=		display.o main.o resources.o play.o score.o screen.o save.o
X
XComplexProgramTarget(xsokoban)
X
Xinstall:: install.man
END_OF_FILE
  if test 244 -ne `wc -c <'xsokoban-3.0/Imakefile'`; then
    echo shar: \"'xsokoban-3.0/Imakefile'\" unpacked with wrong size!
  fi
  # end of 'xsokoban-3.0/Imakefile'
fi
