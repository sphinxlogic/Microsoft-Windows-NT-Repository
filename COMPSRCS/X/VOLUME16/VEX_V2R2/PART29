X/*
X *	pip_mmap.h:	Memory mapping object definition.
X *
X *		Copyright(c) 1991, RasterOps, Inc.
X */
X#ifndef pipMmap_INCLUDED
X#define pipMmap_INCLUDED
X
X#include <pixrect/pr_planegroups.h>		/* Plane group indices. */
X
Xtypedef struct Mmap_Obj {						/* Memory Map Object: */
X	caddr_t	device_pip;							/* ... TV: pip registers. */
X	caddr_t	device_ramdac;						/* ... TV: ramdac registers. */
X	caddr_t	device_regs;						/* ... TCL: Device registers. */
X	caddr_t	device_venus;						/* ... TV: venus registers. */
X	caddr_t	eight_bit;							/* ... Eight bit buffer. */
X	int		fd;									/* ... File descriptor for device. */
X	int		model;								/* ... Model being supported. */
X	caddr_t	monochrome;							/* ... Monochrome buffer. */
X	u_char	pg_available[PIXPG_LAST_PLUS_ONE];	/* ... Plane groups present. */
X	caddr_t	selection;							/* ... Selection memory. */
X	caddr_t	test_slot_base;						/* ... Base for testing purposes. */
X	caddr_t	true_color;							/* ... True color buffer. */
X	caddr_t	video_enable;						/* ... Video enable memory. */
X}	Mmap_Obj;
X
X#define MODEL_TCL	1
X#define MODEL_TCS	2
X#define MODEL_TV	3
X#define MODEL_TEST  99
X
XMmap_Obj *mmap_init();		/* Memory map the whole card. */
XMmap_Obj *mmap_pip();		/* Memory map regions suitable for Xv usage. */
X
X#endif  pipMmap_INCLUDED
