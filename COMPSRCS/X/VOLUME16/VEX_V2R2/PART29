if test -f 'extensions/xv/server/ddx/decrop/libpip.h' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'extensions/xv/server/ddx/decrop/libpip.h'\"
else
echo shar: Extracting \"'extensions/xv/server/ddx/decrop/libpip.h'\" \(763 characters\)
sed "s/^X//" >'extensions/xv/server/ddx/decrop/libpip.h' <<'END_OF_FILE'
X#ifndef LIBPIP_H
X#define LIBPIP_H
X/*
X** File:
X**
X**   libpip.h --- libpip.c header file
X**
X** Author:
X** 
X**   David Carver (Digital Workstation Engineering/Project Athena)
X**
X** Revisions:
X**
X**   29.08.91 Carver
X**     - added support for gray scaled video
X**     - changed all interfaces to take RopPtr as first argument
X**       RopPtr is defined in rop.h
X**
X**   Carver --- 04.06.91
X**     - original
X**
X*/
X
X#define PIP_NTSC      (1L<<0)
X#define PIP_PAL       (1L<<1)
X#define PIP_SECAM     (1L<<2)
X#define PIP_COMPOSITE (1L<<3)
X#define PIP_SVIDEO    (1L<<4)
X#define PIP_RGB       (1L<<5)
X#define PIP_GRAY      (1L<<6)
X
X#define PIP_SOURCE_TIMING (PIP_NTSC | PIP_PAL | PIP_SECAM)
X#define PIP_SOURCE_TYPE (PIP_COMPOSITE | PIP_SVIDEO | PIP_RGB)
X
X#endif LIBPIP_H
END_OF_FILE
