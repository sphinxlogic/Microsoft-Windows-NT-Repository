if test -f 'extensions/xv/lib/Imakefile' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'extensions/xv/lib/Imakefile'\"
else
echo shar: Extracting \"'extensions/xv/lib/Imakefile'\" \(1274 characters\)
sed "s/^X//" >'extensions/xv/lib/Imakefile' <<'END_OF_FILE'
XIMAKE_DEFINES = ExtensionDefines
X
X TD_DEFINES = LibraryDefines ExtensionDefines
XCDEBUGFLAGS = LibraryCDebugFlags
X   INCLUDES = -I$(TOP) -I../include -I$(EXTENSIONSRC)/include -I$(EXTENSIONSRC)/include/xv -I$(INCLUDESRC) -I$(XLIBSRC)
X       SRCS = Xv.c
X       OBJS = Xv.o
X     LINTLIBS = $(LINTXLIB)
X
X#if DebugLibXext && ProfileLibXext
XDebuggedAndProfiledLibraryObjectRule()
X#else
X# if DebugLibXext
XDebuggedLibraryObjectRule()
X# else
X#  if ProfileLibXext
XProfiledLibraryObjectRule()
X#  else
XNormalLibraryObjectRule()
X#  endif
X# endif
X#endif
X
XNormalLibraryTarget(Xv,$(OBJS))
XLintLibraryTarget(Xv,$(SRCS))
XInstallLibrary(Xv,$(USRLIBDIR))
XInstallLintLibrary(Xv,$(LINTLIBDIR))
X
X#if DoInstallExtensionsIntoXlib
XMergeIntoInstalledLibrary($(USRLIBDIR)/libX11.a,libXv.a)
X#endif
X
X#if ProfileLibXv
XProfiledLibraryTarget(Xv,$(OBJS))
XInstallLibrary(Xv_p,$(USRLIBDIR))
X#if DoInstallExtensionsIntoXlib
XMergeIntoInstalledLibrary($(USRLIBDIR)/libX11_p.a,libXv_p.a)
X#endif
X#endif
X
X#if DebugLibXv
XDebuggedLibraryTarget(Xv,$(OBJS))
XInstallLibrary(Xv_d,$(USRLIBDIR))
X#if DoInstallExtensionsIntoXlib
XMergeIntoInstalledLibrary($(USRLIBDIR)/libX11_d.a,libXv_d.a)
X#endif
X#endif
X
X#ifdef IHaveSubdirs
XMakeSubdirs($(SUBDIRS))
XDependSubdirs($(SUBDIRS))
X#endif
X
XDependTarget()
X
XNormalLintTarget($(SRCS))
END_OF_FILE
