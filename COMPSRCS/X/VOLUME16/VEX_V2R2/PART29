  echo shar: Will not clobber existing file \"'extensions/xv/clients/Makefile'\"
else
echo shar: Extracting \"'extensions/xv/clients/Makefile'\" \(239 characters\)
sed "s/^X//" >'extensions/xv/clients/Makefile' <<'END_OF_FILE'
X
XCFLAGS = $(CDEBUGFLAGS) $(INCPATH)
X
XINCPATH = -I../../..
XLIBPATH = -L../../../lib/X -L../lib
X
XALL = vset
X
Xall:: $(ALL)
X
Xvset:  vset.o
X	rm -f $@
X	cc -o $@ $(CFLAGS) $(LDFLAGS) $(LIBPATH) $@.o  -lXv -lX11
X
Xclean::
X	rm *.o core #* *~ $(ALL)
END_OF_FILE
