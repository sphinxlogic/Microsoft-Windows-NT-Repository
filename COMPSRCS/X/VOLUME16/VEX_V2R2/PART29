sed "s/^X//" >'install/clean.sh' <<'END_OF_FILE'
X#!/bin/sh
X#set -x
Xpwd=`pwd`
Xi=0
X
Xecho ""
Xecho -n "> Enter target X11 (top) directory: "
Xread top
Xecho ""
Xecho "> Clean $top directory of all, but the first, Xv backup files"
Xecho -n "> Continue? (y,[n]) "
Xread yesno
Xif [ x$yesno != xy -a x$yesno != xY ]
Xthen
X	echo
X	echo "  Aborting Cleaning"
X	exit
Xfi
X
Xif [ ! -d $top/extensions ]
Xthen
X	if [ ! -d $top/mit/extensions ]
X	then
X		echo ""
X		echo "> Error: Couldn't find extensions subdirectory"
X		exit
X	else
X		top=$top/mit
X	fi
Xfi
X
Xcd $top
Xtop=`pwd`
Xcd $pwd
X
Xrm -f $top/XVINSTBAK
Xrm -rf $top/extensions/xv.xvbak[2-9]*
Xrm -f $top/extensions/Imakefile.xvbak[2-9]*
Xrm -f $top/server/Imakefile.xvbak[2-9]*
Xrm -f $top/server/mi/miinitext.c.xvbak[2-9]*
Xrm -f $top/config/ultrix.cf.xvbak[2-9]*
Xrm -f $top/config/sun.cf.xvbak[2-9]*
Xrm -rf $top/server/ddx/dec/tx.xvbak[2-9]*
Xrm -f $top/server/ddx/dec/ws/Imakefile.xvbak[2-9]*
Xrm -f $top/server/ddx/dec/ws/acc_none.c.xvbak[2-9]*
Xrm -f $top/server/ddx/dec/ws/init.c.xvbak[2-9]*
Xrm -rf $top/server/ddx/cfb32.xvbak[2-9]*
Xrm -f $top/server/ddx/sun/Imakefile.xvbak[2-9]*
Xrm -f $top/server/ddx/sun/sunInit.c.xvbak[2-9]*
Xecho "1" > $top/XVINSTBAK
X
Xecho ""
Xecho "  Cleaning complete"
END_OF_FILE
