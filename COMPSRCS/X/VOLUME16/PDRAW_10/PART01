# Contents:  Imakefile pdraw.c.Z.uu pdraw.man
# Wrapped by dcmartin@fascet on Thu Feb  6 07:36:49 1992
PATH=/bin:/usr/bin:/usr/ucb ; export PATH
if test -f 'Imakefile' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'Imakefile'\"
else
echo shar: Extracting \"'Imakefile'\" \(1532 characters\)
sed "s/^X//" >'Imakefile' <<'END_OF_FILE'
X/**/#	"@(#)Imakefile	1.1 Stardent 91/12/13"
X/**/#	This file is under sccs control at Stardent in:
X/**/#	@uranus/sccs1.p/cmd/grafix/X11/demos/pdraw/s.Imakefile
X/**/#	
X/*
X *			Copyright (c) 1991 by
X *			Stardent Computer Inc.
X *			All Rights Reserved
X *
X * Permission to use, copy, modify, and distribute this software and its
X * documentation for any purpose and without fee is hereby granted,
X * provided that the above copyright notice appear in all copies and that
X * both that copyright notice and this permission notice appear in
X * supporting documentation, and that the name of Stardent Computer not be
X * used in advertising or publicity pertaining to distribution of the
X * software without specific, written prior permission. Stardent Computer
X * makes no representations about the suitability of this software for any
X * purpose.  It is provided "as is" without express or implied warranty.
X *
X * Fri, 06 Dec 91 From: Greg Stiehl  
X * I've cleaned up the Imakefile.  Thought you might want it:
X *************************************************************************
X *
X *                         P D R A W
X *
X *  A PEX/PHIGS drawing program. (or the baby CAD program)
X *
X *
X *  Jan "YON" Hardenbergh, fall 1991
X */
XLOCAL_LIBRARIES = $(MRESOURCELIB) $(XMLIB) $(XTOOLLIB) $(XMULIB) $(PHIGSLIB) $(XLIB)
XDEFINES = PexClientDefines
XINCLUDES = -I. -I$(UILSRC) -I$(MTOOLKITSRC) -I$(LIBSRC) PhigsInclude
XCCOPTIONS = PexCCOptions
X
XSRCS=	pdraw.c
X
XOBJS=	pdraw.o
X
XSimpleProgramTarget(pdraw)
XMSimpleUidTarget(pdraw)
X
XDependTarget()
END_OF_FILE
if test 1532 -ne `wc -c <'Imakefile'`; then
    echo shar: \"'Imakefile'\" unpacked with wrong size!
fi
# end of 'Imakefile'
fi
if test -f 'pdraw.c.Z.uu' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'pdraw.c.Z.uu'\"
else
echo shar: Extracting \"'pdraw.c.Z.uu'\" \(40554 characters\)
