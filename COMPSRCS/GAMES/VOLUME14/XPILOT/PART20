X	if (BIT(Players[i]->disp_type, DT_HAVE_COLOR)) {	\
X	    XSetForeground(d, gc, bg);				\
X	    XDrawString(d, w, gc, x+1, y+1, str, strlen(str));	\
X	}							\
X	XSetForeground(d, gc, fg);				\
X	XDrawString(d, w, gc, x-1, y-1, str, strlen(str));	\
X}
