X	} if
X	/setoverprint where {
X		pop
X		false setoverprint
X	} if
X	newpath
X	/CNT countdictstack def
X	/showpage {} def
X} bind def
X
X/PEND {
X	clear
X	countdictstack CNT sub { end } repeat
X	level1 restore
X} bind def
