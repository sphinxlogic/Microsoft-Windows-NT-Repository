echo shar: Extracting \"'intro.c'\" \(6542 characters\)
sed "s/^X//" >'intro.c' <<'END_OF_FILE'
X/*
X**
X**	X11 Jewel By David Cooper and Jose Guterman 05/92
X**
X*/
X
X#ifdef VMS
X#include <decw$include/Xlib.h>
X#include <decw$include/Xutil.h>
X#include <decw$include/Xos.h>
X#else
X#include <X11/Xlib.h>
X#include <X11/Xutil.h>
X#include <X11/Xos.h>
X#endif
X#include <stdlib.h>
X
X#include "general.h"
X#include "logic.h"
X#include "jewel.h"
X#include "xhscore.h"
X#include "xw.h"
X
Xstatic enum IStates { PRESENT, LOGO, BY, SHINE, FINI} IntroState;
X
X
X#define PRES_FONT "-*-*-bold-o-*-*-24-*-*-*-p-*-iso8859-1"
X#define PRESENT_LOC_X 100
X#define PRESENT_LOC_Y 100
Xstatic char *PresentString="Presenting...";
X#define THANK_LOC_Y 500
Xstatic char *ThankString="Originally by Yoshihiro Satoh of hp";
X#define PHASE_LOC_X ((SCREEN_X-biglogo_width)/2)
X#define PHASE_LOC_Y 300
X
X
X#define NUM_SHINE 4
X#define STEP 2
X#define YELSTEP 20
X#define SHINESTEP 10
X#include "bitmaps/biglogo.xbm"
Xstatic GC GreenGC, YelGC, PresGC;
Xstatic XFontStruct *PresFont;
Xstatic Pixmap BigLogoPM;
Xstatic int count=0;
Xstatic int ShineX=0;
Xstatic BOOL dir_forward=0;
X#define LM (biglogo_width/2)
X#define XM (PHASE_LOC_X + LM)
X#define GWIDTH ((biglogo_width/STEP)+1)
X#define GHEIGHT ((biglogo_height/STEP)+1)
X
Xvoid Start_Intro()
X	{
X	if (JewelState != NULL_STATE)
X	    { XClearWindow(xw_display, xw_window); }
X
X	IntroState=PRESENT;
X	JewelState=INTRO;
X
X	count=0;
X	xw_set_timer(200L);
X	}
X
Xvoid Intro_Timeout()
X	{
X	switch(IntroState)
X		{
X		case PRESENT:
X			/* start logo */
X			count++;
X			if (count <= strlen(PresentString))
X				{
X				XDrawString(xw_display, xw_window, PresGC, PRESENT_LOC_X,
X					PRESENT_LOC_Y+((PresFont->ascent)*3/2),
X					PresentString, count);
X				XFlush(xw_display);
X				}
X			else
X				{
X				IntroState=LOGO;
X				/* set up */
X				count=0;
X				}
X			xw_set_timer(200L);
X			break;
X		case LOGO:
X			/* put more logo up */
X			XCopyPlane(xw_display, BigLogoPM, xw_window, GreenGC,
X				(LM-count*STEP), 0, STEP, biglogo_height,
X				(XM-count*STEP), PHASE_LOC_Y, 1L);
X			
X			XCopyPlane(xw_display, BigLogoPM, xw_window, GreenGC,
X				(LM+count*STEP), 0, STEP, biglogo_height,
X				(XM+count*STEP), PHASE_LOC_Y, 1L);
X			
X			XCopyPlane(xw_display, BigLogoPM, xw_window, YelGC,
X				(LM-count*STEP)-YELSTEP, 0, YELSTEP, biglogo_height,
X				(XM-count*STEP)-YELSTEP, PHASE_LOC_Y, 1L);
X
X			XCopyPlane(xw_display, BigLogoPM, xw_window, YelGC,
X				(LM+count*STEP)+STEP, 0, YELSTEP, biglogo_height,
X				(XM+count*STEP)+STEP, PHASE_LOC_Y, 1L);
X			XFlush(xw_display);
X
X			count++;
X
X			XFlush(xw_display);
X			if (count < (GWIDTH/2))
X				{ xw_set_timer(2L); }
X			else
X				{
X				IntroState=BY;
X				xw_set_timer(200L);
X				}
X			break;
X		case BY:
X			{
X			XCharStruct Sizes;
X			int dir, asc, dsc;
X
X			XTextExtents(VerFont,ThankString, strlen(ThankString),
X				&dir,&asc,&dsc,&Sizes);
X			XDrawImageString(xw_display, xw_window, VerGC,
X				(SCREEN_X - Sizes.width)/2,
X				THANK_LOC_Y+((VerFont->ascent)*3/2),
X				ThankString, strlen(ThankString));
X
X			XTextExtents(VerFont,StartString, strlen(StartString),
X				&dir,&asc,&dsc,&Sizes);
X			XDrawImageString(xw_display, xw_window, VerGC,
X				(SCREEN_X - Sizes.width)/2,
X				START_LOC_Y+((VerFont->ascent)*3/2),
X				StartString, strlen(StartString));
X
X			XDrawImageString(xw_display, xw_window, VerGC, VER_LOC_X,
X				VER_LOC_Y+((VerFont->ascent)*3/2),
X				VerString, strlen(VerString));
X			XFlush(xw_display);
X			count=0;
X			ShineX=0;
X			dir_forward=TRUE;
X			IntroState=SHINE;
X			xw_set_timer(200L);
X			}
X			break;
X		case SHINE:
X			XCopyPlane(xw_display, BigLogoPM, xw_window, GreenGC,
X				(ShineX+ ((dir_forward)?0:SHINESTEP)), 0,
X				STEP, biglogo_height,
X				(ShineX+PHASE_LOC_X + ((dir_forward)?0:SHINESTEP)),
X				PHASE_LOC_Y, 1L);
X			
X			XCopyPlane(xw_display, BigLogoPM, xw_window, YelGC,
X				(ShineX+((dir_forward)?STEP:0)), 0,
X				SHINESTEP, biglogo_height,
X				(ShineX+PHASE_LOC_X+((dir_forward)?STEP:0)),
X				PHASE_LOC_Y, 1L);
X
X			XFlush(xw_display);
X
X			ShineX+=((dir_forward)?(STEP):(-STEP));
X			xw_set_timer(2L);
X			if (ShineX>biglogo_width)
X				{
X				ShineX=biglogo_width;
X				dir_forward=FALSE;
X				}
X			if (ShineX<0)
X				{
X				ShineX=0;
X				dir_forward=TRUE;
X				count++;
X				if (count>NUM_SHINE)
X					{
X					IntroState=FINI;
X					xw_set_timer(1000L);
X					}
X				}
X			break;
X		case FINI:
X			Set_State_High_Score();
X			JewelState=HIGHSCORE;
X			break;
X		}
X	}
X
Xvoid Init_Intro()
X	{
X	XGCValues gcv;
X	unsigned long gcvm;
X	
X	gcvm=(GCGraphicsExposures | GCForeground | GCBackground | GCPlaneMask);
X
X	gcv.graphics_exposures=False;
X	gcv.plane_mask=(green | yellow | black);
X	gcv.foreground=yellow;
X	gcv.background=black;
X	YelGC=XCreateGC(xw_display, xw_window, gcvm, &gcv);
X	gcv.foreground=green;
X	GreenGC=XCreateGC(xw_display, xw_window, gcvm, &gcv);
X
X	BigLogoPM=XCreateBitmapFromData(xw_display, xw_window,
X				biglogo_bits, biglogo_width, biglogo_height);
X
X	if ( (PresFont=XLoadQueryFont(xw_display,PRES_FONT)) == NULL)
X		{ xw_fatal("Cannot load PRESENT font.\n",__LINE__,__FILE__); }
X	gcvm|=GCFont;
X	gcv.font=PresFont->fid;
X	gcv.foreground=yellow;
X	PresGC=XCreateGC(xw_display, xw_window, gcvm, &gcv);
X	}
X
Xvoid Expose_Intro()
X	{
X	switch(IntroState)
X		{
X		case PRESENT:
X			XDrawString(xw_display, xw_window, PresGC, PRESENT_LOC_X,
X				PRESENT_LOC_Y+((PresFont->ascent)*3/2),
X				PresentString, count);
X			break;
X		case LOGO:
X			XDrawString(xw_display, xw_window, PresGC, PRESENT_LOC_X,
X				PRESENT_LOC_Y+((PresFont->ascent)*3/2),
X				PresentString, strlen(PresentString));
X			XCopyPlane(xw_display, BigLogoPM, xw_window, GreenGC,
X				0, 0, biglogo_width, biglogo_height,
X				PHASE_LOC_X, PHASE_LOC_Y, 1L);
X			break;
X		case BY:
X		case SHINE:
X			{
X			XCharStruct Sizes;
X			int dir, asc, dsc;
X
X			XDrawString(xw_display, xw_window, PresGC, PRESENT_LOC_X,
X				PRESENT_LOC_Y+((PresFont->ascent)*3/2),
X				PresentString, strlen(PresentString));
X
X			XCopyPlane(xw_display, BigLogoPM, xw_window, GreenGC,
X				0, 0, biglogo_width, biglogo_height,
X				PHASE_LOC_X, PHASE_LOC_Y, 1L);
X
X			XTextExtents(VerFont,ThankString, strlen(ThankString),
X				&dir,&asc,&dsc,&Sizes);
X			XDrawImageString(xw_display, xw_window, VerGC,
X				(SCREEN_X - Sizes.width)/2,
X				THANK_LOC_Y+((VerFont->ascent)*3/2),
X				ThankString, strlen(ThankString));
X
X			XTextExtents(VerFont,StartString, strlen(StartString),
X				&dir,&asc,&dsc,&Sizes);
X			XDrawImageString(xw_display, xw_window, VerGC,
X				(SCREEN_X - Sizes.width)/2,
X				START_LOC_Y+((VerFont->ascent)*3/2),
X				StartString, strlen(StartString));
X
X			XDrawImageString(xw_display, xw_window, VerGC, VER_LOC_X,
X				VER_LOC_Y+((VerFont->ascent)*3/2),
X				VerString, strlen(VerString));
X			}
X			break;
X		}
X	XFlush(xw_display);
X	}
X
END_OF_FILE
