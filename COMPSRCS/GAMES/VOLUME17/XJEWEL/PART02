X	{
X		1 index /FID ne { def } { pop pop } ifelse
X	} forall
X	/Encoding exch def
X	dup /FontName exch def
X	currentdict end definefont pop
X} bind def
X
X/DEFS 0 def
X
X% hpos vpos EBEGIN -
X
X/EBEGIN {
X	moveto
X	DEFS begin
X} bind def
X
X/EEND /end load def
X
X/CNT 0 def
X/level1 0 def
X
X% llx lly newwid wid newht ht newllx newlly PBEGIN -
X
X/PBEGIN {
X	/level1 save def
X	translate
X	div 3 1 roll div exch scale
X	neg exch neg exch translate
X	% set the graphics state to default values
X	0 setgray
X	0 setlinecap
X	1 setlinewidth
X	0 setlinejoin
X	10 setmiterlimit
X	[] 0 setdash
X	/setstrokeadjust where {
X		pop
