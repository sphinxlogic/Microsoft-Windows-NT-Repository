#! /bin/sh
# This is a shell archive.  Remove anything before this line, then unpack
# it by saving it into a file and typing "sh file".  To overwrite existing
# files, type "sh file -c".  You can also feed this as standard input via
# unshar, or by typing "sh <file", e.g..  If this archive is complete, you
# will see the following message at the end:
#		"End of archive 3 (of 7)."
# Contents:  bitmaps/sw2.bm execute2.c.uu stupid.c
# Wrapped by billr@saab on Wed Jul 14 12:32:50 1993
PATH=/bin:/usr/bin:/usr/ucb ; export PATH
if test -f 'bitmaps/sw2.bm' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'bitmaps/sw2.bm'\"
else
echo shar: Extracting \"'bitmaps/sw2.bm'\" \(276 characters\)
sed "s/^X//" >'bitmaps/sw2.bm' <<'END_OF_FILE'
X#define sw2_width 16
X#define sw2_height 16
X#define sw2_x_hot -1
X#define sw2_y_hot -1
Xstatic char sw2_bits[] = {
X0x00,0x00,0x00,0x00,0x00,0x00,0x60,0x06,0x30,0x0c,0x3c,0x3c,
X0x1e,0x78,0x0f,0xf0,0x07,0xe0,0x07,0xe0,0x1f,0xf8,0x3e,0x7c,
X0x7c,0x3e,0x78,0x1e,0xf8,0x1f,0xfc,0x3f};
END_OF_FILE
if test 276 -ne `wc -c <'bitmaps/sw2.bm'`; then
    echo shar: \"'bitmaps/sw2.bm'\" unpacked with wrong size!
fi
# end of 'bitmaps/sw2.bm'
fi
if test -f 'execute2.c.uu' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'execute2.c.uu'\"
