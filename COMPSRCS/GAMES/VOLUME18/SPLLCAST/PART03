if test -f 'stupid.c' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'stupid.c'\"
else
echo shar: Extracting \"'stupid.c'\" \(12884 characters\)
sed "s/^X//" >'stupid.c' <<'END_OF_FILE'
X#include <stdio.h>
X#include <string.h>
X
X#include "handwave.h"
X#include "spelllist.h"
X
Xstruct maingame {
X    int ignorestuff;
X};
X
Xstatic void PrintMsg(msg, gameval, rock)
Xchar *msg;
Xgame *gameval;
Xstruct maingame *rock;
X{
X    if (msg) {
X	printf("*: ");
X	fputs(msg, stdout);
X    }
X}
X
Xstatic void PrintMsg2(person1, msg1, msgelse, gameval, rock)
Xint person1;
Xchar *msg1, *msgelse;
Xgame *gameval;
Xstruct maingame *rock;
X{
X    if (msg1) {
X	printf("%d: ", person1);
X	fputs(msg1, stdout);
X    }
X    if (msgelse) {
X	printf("~%d: ", person1);
X	fputs(msgelse, stdout);
X    }
X}
X
Xstatic void PrintMsg3(person1, person2, msg1, msg2, msgelse, gameval, rock)
Xint person1, person2;
Xchar *msg1, *msg2, *msgelse;
Xgame *gameval;
Xstruct maingame *rock;
X{
X    if (msg1) {
X	printf("%d: ", person1);
X	fputs(msg1, stdout);
X    }
X    if (msg2) {
X	printf("%d: ", person2);
X	fputs(msg2, stdout);
X    }
X    if (msgelse) {
X	printf("~%d/~%d: ", person1, person2);
X	fputs(msgelse, stdout);
X    }
X}
X
Xstatic void Queries(numqueries, qlist, gameval, rock)
Xint numqueries;
Xstruct query qlist[];
Xgame *gameval;
Xstruct maingame *rock;
X{
X    int ix, jx, attnum, spellnum;
X    int res;
X    char inbuf[256];
X    char outbuf[512];
X    static int targetlist[256];
X    static char *targetnamelist[256];
X    int numtargs, targnum;
X
X    int *hlist;
X    char *ques, *handage;
X
X    for (ix=0; ix<numqueries; ix++) {
X	switch (qlist[ix].qtype) {
X	    case Qu_NoQuery:
X		/*printf("ERROR: got NoQuery\n");*/
X		break;
X	    case Qu_SetOffDelay:
X		printf(
X"%d: Do you want to release the %s from the Delayed Effect?\n (0:no, 1:yes): ",
X				qlist[ix].player,
X				spelllist[(int)qlist[ix].rock].name);
X		while (1) {
X		    fgets(inbuf, 256, stdin);
X		    res = atol(inbuf);
X		    if (res!=0 && res!=1) {
X			printf(" [Please enter 0 or 1]: ");
X		    }
X		    else
X			break;
X		}
X		qlist[ix].answer = res;
X		break;
X	    case Qu_LeftHand:
X	    case Qu_RightHand:
X		if (qlist[ix].qtype==Qu_LeftHand)
X		    ques = "What spell do you want to cast with your left hand?";
X		else
X		    ques = "What spell do you want to cast with your right hand?";
X		hlist = (int *)qlist[ix].rock;
X		printf("%d: %s\n (", qlist[ix].player, ques);
X		for (jx=0; jx<hlist[0]; jx++) {
X		    spellnum = hlist[jx+1];
X		    printf("%d: %s%s%s", jx, spelllist[spellnum & (~QuVal_Hand_Both)].name,
X			   ((spellnum & QuVal_Hand_Both) ? " [both hands]" : ""),
X			   (jx+1==hlist[0]) ? "): " : ", ");
X		}
X		while (1) {
X		    fgets(inbuf, 256, stdin);
X		    res = atol(inbuf);
X		    if (res<0 || res>=hlist[0]) {
X			printf(" [Please enter a number from 0 to %d]: ", hlist[0]-1);
X		    }
X		    else
X			break;
X		}
X		qlist[ix].answer = res;
X		break;
X	    case Qu_ElementalType:
X		printf(
X"%d: Which type of elemental do you want to summon?\n (0:fire, 1:ice): ",
X				qlist[ix].player);
X		while (1) {
X		    fgets(inbuf, 256, stdin);
X		    res = atol(inbuf);
X		    if (res!=0 && res!=1) {
X			printf(" [Please enter 0 or 1]: ");
X		    }
X		    else
X			break;
X		}
X		qlist[ix].answer = res;
X		break;
X	    case Qu_ParalysisHand:
X		printf(
X"%d: Which of %s's hands do you want to paralyze?\n (0:left, 1:right): ",
X				qlist[ix].player,
X				NameOfBeing(gameval, QuVal_Target_Wizard,
X				qlist[ix].rock));
X		while (1) {
X		    fgets(inbuf, 256, stdin);
X		    res = atol(inbuf);
X		    if (res!=0 && res!=1) {
X			printf(" [Please enter 0 or 1]: ");
X		    }
X		    else
X			break;
X		}
X		qlist[ix].answer = res;
X		break;
X	    case Qu_CharmHand:
X		printf(
X"%d: Which of %s's hands do you want to charm?\n (0:left, 1:right): ",
X				qlist[ix].player,
X				NameOfBeing(gameval, QuVal_Target_Wizard,
X				qlist[ix].rock));
X		while (1) {
X		    fgets(inbuf, 256, stdin);
X		    res = atol(inbuf);
X		    if (res!=0 && res!=1) {
X			printf(" [Please enter 0 or 1]: ");
X		    }
X		    else
X			break;
X		}
X		qlist[ix].answer = res;
X		break;
X	    case Qu_CharmGesture:
X		jx = (int)(qlist[ix].rock);
X		if (jx >= 128) {
X		    spellnum = jx - 128;
X		    jx = 1;
X		}
X		else {
X		    spellnum = jx;
X		    jx = 0;
X		}
X		printf(
X"%d: What gesture do you want %s's %s hand to make?\n (enter a letter): ",
X				qlist[ix].player,
X				NameOfBeing(gameval, QuVal_Target_Wizard,
X				spellnum), (jx?"right":"left"));
X		while (1) {
X		    fgets(inbuf, 256, stdin);
X		    if (inbuf[0]=='\0' || inbuf[0]=='\n') {
X			printf(" [Please enter a letter]: ");
X		    }
X		    else {
X			res = cheap_translate(inbuf[0]);
X			break;
X		    }
X		}
X		qlist[ix].answer = res;
X		break;
X	    case Qu_MonsterTarget:
X		jx = (int)(qlist[ix].rock);
X		attnum = jx / 256;
X		spellnum = jx % 256;
X		switch (attnum) {
X		    case 1:
X			sprintf(outbuf, "Whom do you want %s to attack?",
X					NameOfBeing(gameval,
X					QuVal_Target_Creature, spellnum));
X			break;
X		    case 2:
X			sprintf(outbuf, "Whom do you want %s's first attack to be at?",
X					NameOfBeing(gameval,
X					QuVal_Target_Creature, spellnum));
X			break;
X		    case 3:
X			sprintf(outbuf, "Whom do you want %s's second attack to be at?",
X					NameOfBeing(gameval,
X					QuVal_Target_Creature, spellnum));
X			break;
X		    default:
X			sprintf(outbuf, "ERROR: Query about %d (%d)", spellnum, attnum);
X			break;
X		}
X		printf("%d: %s\n", qlist[ix].player, outbuf);
X		targnum = 0;
X		targetlist[targnum] = 0;
X		targetnamelist[targnum] = "nobody";
X		targnum++;
X		numtargs = NumberOfTargets(gameval, QuVal_Target_Wizard);
X		for (jx=0; jx<numtargs; jx++) {
X		    targetlist[targnum] = jx | QuVal_Target_Wizard;
X		    targetnamelist[targnum] = NameOfTarget(gameval,
X				QuVal_Target_Wizard, jx);
X		    targnum++;
X		}
X		numtargs = NumberOfTargets(gameval, QuVal_Target_Creature);
X		for (jx=0; jx<numtargs; jx++) {
X		    targetlist[targnum] = jx | QuVal_Target_Creature;
X		    targetnamelist[targnum] = NameOfTarget(gameval,
X				QuVal_Target_Creature, jx);
X		    targnum++;
X		}
X		numtargs = targnum;
X		if (numtargs==0)
X		    printf("ERROR: No targets available!\n");
X		else {
X		    printf(" [");
X		    for (jx=0; jx<numtargs; jx++) {
X			printf("%d: %s%s", jx, targetnamelist[jx],
X					((jx<numtargs-1)?", ":""));
X		    }
X		    printf("]: ");
X		    while (1) {
X			fgets(inbuf, 256, stdin);
X			res = atol(inbuf);
X			if (res<0 || res>=numtargs) {
X			    printf(" [Please enter a number from 0 to %d]: ",
X					numtargs-1);
X			}
X			else
X			    break;
X		    }
X		    qlist[ix].answer = targetlist[res];
X		}
X		break;
X	    case Qu_WhichToDelay:
X	    case Qu_WhichToPerm:
X		if (qlist[ix].qtype == Qu_WhichToDelay)
X		    printf("%d: Which spell do you want to delay?\n",
X				qlist[ix].player);
X		else
X		    printf("%d: Which spell do you want to make permanent?\n",
X				qlist[ix].player);
X		
X		hlist = (int *)qlist[ix].rock;
X		printf(" [");
X		for (jx=0; hlist[jx]!=(-1); jx++) {
X		    printf("%s%d: %s", ((jx)?", ":""), jx, spelllist[hlist[jx]].name);
X		}
X		numtargs = jx;
X		printf("]: ");
X		while (1) {
X		    fgets(inbuf, 256, stdin);
X		    res = atol(inbuf);
X		    if (res<0 || res>=numtargs) {
X			printf(" [Please enter a number from 0 to %d]: ", numtargs-1);
X		    }
X		    else
X			break;
X		}
X		qlist[ix].answer = res;
X		break;
X	    case Qu_TargetBeing:
X	    case Qu_TargetBeingNone:
X	    case Qu_TargetWizard:
X	    case Qu_TargetWizardNone:
X	    case Qu_TargetRaiseDead:
X		jx = (int)(qlist[ix].rock);
X		spellnum = jx & (~QuVal_Hand_MASK);
X		if (jx & QuVal_Hand_Left)
X		    handage = "with your left hand";
X		else if (jx & QuVal_Hand_Right)
X		    handage = "with your right hand";
X		else if (jx & QuVal_Hand_Both)
X		    handage = "with both hands";
X		else
X		    handage = "from the Delayed Effect";
X		if (spellnum==SP__STAB)
X		    sprintf(outbuf, "Who do you want to stab at (%s)?", handage);
X		else
X		    sprintf(outbuf, "Who do you want to cast %s at (%s)?",
X				spelllist[spellnum].name, handage);
X		printf("%d: %s\n", qlist[ix].player, outbuf);
X		targnum = 0;
X		if (qlist[ix].qtype==Qu_TargetBeingNone || 
X				qlist[ix].qtype==Qu_TargetWizardNone) {
X		    targetlist[targnum] = 0;
X		    targetnamelist[targnum] = "nobody";
X		    targnum++;
X		}
X		numtargs = NumberOfTargets(gameval, QuVal_Target_Wizard);
X		for (jx=0; jx<numtargs; jx++) {
X		    targetlist[targnum] = jx | QuVal_Target_Wizard;
X		    targetnamelist[targnum] = NameOfTarget(gameval,
X				QuVal_Target_Wizard, jx);
X		    targnum++;
X		}
X		if (qlist[ix].qtype==Qu_TargetBeing ||
X				qlist[ix].qtype==Qu_TargetBeingNone || 
X				qlist[ix].qtype==Qu_TargetRaiseDead) {
X		    numtargs = NumberOfTargets(gameval, QuVal_Target_Creature);
X		    for (jx=0; jx<numtargs; jx++) {
X			targetlist[targnum] = jx | QuVal_Target_Creature;
X			targetnamelist[targnum] = NameOfTarget(gameval,
X					QuVal_Target_Creature, jx);
X			targnum++;
X		    }
X		}
X		if (qlist[ix].qtype==Qu_TargetRaiseDead) {
X		    numtargs = NumberOfTargets(gameval, QuVal_Target_Corpse);
X		    for (jx=0; jx<numtargs; jx++) {
X			targetlist[targnum] = jx | QuVal_Target_Corpse;
X			targetnamelist[targnum] = NameOfTarget(gameval,
X					QuVal_Target_Corpse, jx);
X			targnum++;
X		    }
X		}
X		numtargs = targnum;
X		if (numtargs==0)
X		    printf("ERROR: No targets available!\n");
X		else {
X		    printf(" [");
X		    for (jx=0; jx<numtargs; jx++) {
X			printf("%d: %s%s", jx, targetnamelist[jx], ((jx<numtargs-1)?",
X					":""));
X		    }
X		    printf("]: ");
X		    while (1) {
X			fgets(inbuf, 256, stdin);
X			res = atol(inbuf);
X			if (res<0 || res>=numtargs) {
X			    printf(" [Please enter a number from 0 to %d]: ",
X					numtargs-1);
X			}
X			else
X			    break;
X		    }
X		    qlist[ix].answer = targetlist[res];
X		}
X		break;
X
X	    default:
X		printf("ERROR: Unknown query type %d!\n", qlist[ix].qtype);
X		break;
X	}
X    }
X}
X
Xint cheap_translate(ch)
Xchar ch;
X{
X    switch (ch) {
X	case 'p':
X	case 'P':
X	    return Gesture_PALM;
X	case 'f':
X	case 'F':
X	    return Gesture_FINGERS;
X	case 'd':
X	case 'D':
X	    return Gesture_DIGIT;
X	case 's':
X	case 'S':
X	    return Gesture_SNAP;
X	case 'c':
X	case 'C':
X	    return Gesture_CLAPHALF;
X	case 'w':
X	case 'W':
X	    return Gesture_WAVE;
X	case 'k':
X	case 'K':
X	    return Gesture_KNIFE;
X	case ' ':
X	case 'n':
X	case 'N':
X	default:
X	    return Gesture_NOTHING;
X    }
X}
X
Xvoid show_beings(gameval)
Xgame *gameval;
X{
X    int ix, max, hp;
X
X    printf("\n");
X    max = NumberOfBeings(gameval, QuVal_Target_Wizard);
X    for (ix=0; ix<max; ix++) {
X	hp = HitPointsOfBeing(gameval, QuVal_Target_Wizard, ix);
X	if (hp>=0)
X	    printf("%s (%d); ", NameOfBeing(gameval, QuVal_Target_Wizard, ix), hp);
X    }
X    max = NumberOfBeings(gameval, QuVal_Target_Creature);
X    for (ix=0; ix<max; ix++) {
X	hp = HitPointsOfBeing(gameval, QuVal_Target_Creature, ix);
X	if (hp>=0)
X	    printf("%s (%d); ", NameOfBeing(gameval, QuVal_Target_Creature, ix), hp);
X    }
X    printf("\n");
X}
X
Xmain(argc, argv)
Xint argc;
Xchar *argv[];
X{
X    struct interface procs;
X    struct maingame ignorerock;
X    game *gameval;
X    static char *namelist[MAXPLAYERS] = {"Arnold", "Barbara", "Crash"};
X    static int genderlist[MAXPLAYERS] = {Gender_MALE, Gender_FEMALE, Gender_NEUTER};
X    static int movelist[MAXPLAYERS*2];
X    static char inbuf[256];
X    int numplayers, whowon;
X    int ix, val;
X
X    procs.proc_PrintMsg = PrintMsg;
X    procs.proc_PrintMsg2 = PrintMsg2;
X    procs.proc_PrintMsg3 = PrintMsg3;
X    procs.proc_Queries = Queries;
X
X    numplayers = 2;
X    if (argc==2 && !strcmp(argv[1], "-3")) {
X	numplayers = 3;
X    }
X    else if (argc>=2) {
X	numplayers = 0;
X	for (ix=1; ix<argc; ix++) {
X	    char *tmp;
X	    namelist[numplayers] = argv[ix];
X	    tmp = index(namelist[numplayers], ':');
X	    if (!tmp)
X		genderlist[numplayers] = Gender_MALE;
X	    else {
X		switch (*(tmp+1)) {
X		    case 'f':
X		    case 'F':
X			genderlist[numplayers] = Gender_FEMALE;
X			break;
X		    case 'n':
X		    case 'N':
X			genderlist[numplayers] = Gender_NEUTER;
X			break;
X		    case 'x':
X		    case 'X':
X			genderlist[numplayers] = Gender_NONE;
X			break;
X		    case 'm':
X		    case 'M':
X		    default:
X			genderlist[numplayers] = Gender_MALE;
X			break;
X		}
X		*tmp = '\0';
X	    }
X	    numplayers++;
X	}
X    }
X
X    if (numplayers < 2) {
X	printf("You have listed only %d players!\n", numplayers);
X	exit(12);
X    }
X    if (numplayers > MAXPLAYERS) {
X	printf("There is a maximum of %d players!\n", MAXPLAYERS);
X	exit(12);
X    }
X
X    gameval = BeginGame(numplayers, namelist, genderlist, &procs, &ignorerock);
X
X    do {
X	show_beings(gameval);
X	val = 0;
X	switch (TurnType(gameval)) {
X	    case Turn_HASTE:
X		printf("(hasted) ");
X		break;
X	    case Turn_TIMESTOP:
X		printf("(timestopped) ");
X		break;
X	    case Turn_NORMAL:
X	    default:
X		break;
X	}
X	for (ix=0; ix<MAXPLAYERS; ix++)
X	    if (TurnPlayerActive(gameval, ix)) {
X		val++;
X		printf("%d", ix);
X	    }
X	printf(": Enter moves: ");
X	bzero(inbuf, numplayers*2);
X	fgets(inbuf, 256, stdin);
X	for (ix=0; ix<numplayers*2; ix++) {
X	    movelist[ix] = cheap_translate(inbuf[ix]);
X	}
X	whowon = RunTurn(gameval, movelist);
X    } while (whowon < 0);
X
X    if (whowon==MAXPLAYERS)
X	printf("+++ the game is a draw +++\n");
X    else
X	printf("+++ %s has won +++\n", namelist[whowon]);
X
X    FreeGame(gameval);
X}
X
END_OF_FILE
if test 12884 -ne `wc -c <'stupid.c'`; then
    echo shar: \"'stupid.c'\" unpacked with wrong size!
fi
# end of 'stupid.c'
fi
echo shar: End of archive 3 \(of 7\).
cp /dev/null ark3isdone
MISSING=""
for I in 1 2 3 4 5 6 7 ; do
    if test ! -f ark${I}isdone ; then
	MISSING="${MISSING} ${I}"
    fi
done
if test "${MISSING}" = "" ; then
    echo You have unpacked all 7 archives.
    rm -f ark[1-9]isdone
else
    echo You still need to unpack the following archives:
    echo "        " ${MISSING}
fi
##  End of shell archive.
exit 0
