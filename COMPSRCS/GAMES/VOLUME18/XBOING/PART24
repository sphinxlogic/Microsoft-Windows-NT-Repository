if test -f 'bitmaps/flag.xpm.uu' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'bitmaps/flag.xpm.uu'\"
else
echo shar: Extracting \"'bitmaps/flag.xpm.uu'\" \(4317 characters\)
sed "s/^X//" >'bitmaps/flag.xpm.uu' <<'END_OF_FILE'
Xbegin 644 flag.xpm
