*** 26,31 ****
--- 58,80 ----
  jgraph: $(OBJS)
  	$(CC) -g $(OBJS) -lm -o jgraph
  
+ sin:	sin.o
+ 	cc -o sin sin.o -lm
+ 
+ bailey.jps: bailey.pts
+ ex2.jps: data.txt
+ mab2.jps: mab2.times
+ sin.jps: sin
+ sin2.jps: sin2.pts
+ sin3.jps: sin3.pts
+ tree1.jps: tree.awk
+ 	( echo "4 3" | nawk -f tree.awk ; echo "xaxis size 5.4" ) | \
+            jgraph -P > tree1.jps
+ tree2.jps: tree.awk
+ 	( echo "5 2" | nawk -f tree.awk ; echo "xaxis size 5.4" ) | \
+            jgraph -P > tree2.jps
+ 
