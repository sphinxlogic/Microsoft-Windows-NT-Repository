Xall: 
X	@echo "Makefile for the TIN v1.1 PL4 Usenet newsreader. Configuration:"
X	@echo " "
X	@echo "    Make=[$(MAKE)]  Compiler=[$(CC)]  Copts=[$(COPTS)]  Debug=[$(DEBUG)]"
X	@echo "    Install Bindir=[$(BINDIR)]  Mandir=[$(MANDIR)]"
X	@echo "    News Libdir=[$(LIBDIR)]  Spooldir=[$(SPOOLDIR)]"
X	@echo "    News Gateway=[$(NNTP_INEWS_GATEWAY)]  Domain=[$(NNTP_INEWS_DOMAIN)]"
X	@echo " "
X	@echo "If the above options are OK make one of the following targets:"
X	@echo " "
X	@echo "    make bsd       [ BSD/Dec/DG/NeXT/Pyramid/Sun ]"
X	@echo "    make sysv      [ SysV ]"
X	@echo "    make sysvr4    [ SysV R4 ]"
X	@echo "    make sco       [ SCO Unix ]"
X	@echo "    make aix       [ IBM AIX ]"
