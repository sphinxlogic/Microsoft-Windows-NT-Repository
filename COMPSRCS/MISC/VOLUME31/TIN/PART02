X# For Xenix 386
X#			 NETLIBS="-lsocket" \
X#
Xxenix:
X	@echo "Compiling for Xenix 386..."
X	@$(MAKE) CC=$(CC) CFLAGS='-c -Zi -DSYSV -DLIBDIR="\\"$(LIBDIR)\\"" -DSPOOLDIR="\\"$(SPOOLDIR)\\""' \
X			 LIBS="-lcurses -ltinfo -lx" \
X			 LFLAGS=-Zi EXE=tin linkit
