X# For IBM AIX
Xaix:
X	@echo "Compiling for IBM AIX..."
X	@$(MAKE) CC=$(CC) CFLAGS='$(COPTS) -DSYSV -DRS6000 -DREAD_CHAR_HACK -DUSE_LONG_FILENAMES -DLIBDIR=\"$(LIBDIR)\" -DSPOOLDIR=\"$(SPOOLDIR)\"' \
X			 LIBS="-lcurses -ltermcap" \
X			 EXE=tin linkit
