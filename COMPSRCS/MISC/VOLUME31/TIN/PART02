X	@$(MAKE) CC=$(CC) CFLAGS='$(COPTS) -DBSD -DLIBDIR=\"$(LIBDIR)\" -DSPOOLDIR=\"$(SPOOLDIR)\"' \
X			 LIBS="-lcurses -ltermcap" \
