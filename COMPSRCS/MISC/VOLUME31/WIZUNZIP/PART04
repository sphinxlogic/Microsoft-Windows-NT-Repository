 if test 34155 -ne `wc -c <'status.c.UU'`; then
    echo shar: \"'status.c.UU'\" unpacked with wrong size!
  else
    echo shar: Uudecoding \"'status.c'\" \(24769 characters\)
    cat status.c.UU | uudecode
    if test 24769 -ne `wc -c <'status.c'`; then
      echo shar: \"'status.c'\" uudecoded with wrong size!
    else
      rm status.c.UU
    fi
  fi
  # end of 'status.c.UU'
fi
echo shar: End of archive 4 \(of 6\).
cp /dev/null ark4isdone
MISSING=""
for I in 1 2 3 4 5 6 ; do
    if test ! -f ark${I}isdone ; then
	MISSING="${MISSING} ${I}"
    fi
done
if test "${MISSING}" = "" ; then
    echo You have unpacked all 6 archives.
    rm -f ark[1-9]isdone
else
    echo You still must unpack the following archives:
    echo "        " ${MISSING}
fi
exit 0
exit 0 # Just in case...
