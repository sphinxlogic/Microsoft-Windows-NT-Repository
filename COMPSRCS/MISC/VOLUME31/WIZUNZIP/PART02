#! /bin/sh
# This is a shell archive.  Remove anything before this line, then feed it
# into a shell via "sh file" or similar.  To overwrite existing files,
# type "sh file -c".
# Contents:  help.cur.UU sizewndw.c.UU wiz.rtf.UU.B
# Wrapped by kent@sparky on Tue Aug 25 20:38:45 1992
PATH=/bin:/usr/bin:/usr/ucb:/usr/local/bin:/usr/lbin ; export PATH
echo If this archive is complete, you will see the following message:
echo '          "shar: End of archive 2 (of 6)."'
if test -f 'help.cur.UU' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'help.cur.UU'\"
else
  echo shar: Extracting \"'help.cur.UU'\" \(477 characters\)
  sed "s/^X//" >'help.cur.UU' <<'END_OF_FILE'
Xbegin 666 help.cur
XM   "  $ ("         P 0  %@   "@    @    0     $  0        $ 
XM                            ____                        !\  
XM  @@   ((   ""    @@   'P        .?   #((  !R"   8@@  .((  #
XM""  1P00 &8"" !^ 00 ?_"" '\$00!_!$$ ?P1! '\$00!^!$$ ?(.! 'A 
XM @!P( 0 8! ( $ ($   !^  _______________________X/___\!____ ?
XM___P'___\!____@___X/___^"#___@ ?__P0'__\$!__.# ?_Q P'_\ > __
XM 'P'_P & _\ !P'_  . _P #@/\  X#_  . _P #@/\   #_ 8 !_P/  _\'
X+X ?_#_ /_Q_X'___
X 
Xend
END_OF_FILE
 if test 477 -ne `wc -c <'help.cur.UU'`; then
    echo shar: \"'help.cur.UU'\" unpacked with wrong size!
  else
    echo shar: Uudecoding \"'help.cur'\" \(326 characters\)
    cat help.cur.UU | uudecode
    if test 326 -ne `wc -c <'help.cur'`; then
      echo shar: \"'help.cur'\" uudecoded with wrong size!
    else
      rm help.cur.UU
    fi
  fi
  # end of 'help.cur.UU'
fi
