 if test 51094 -ne `wc -c <'wiz.rtf.UU.B'`; then
   echo shar: \"'wiz.rtf.UU.B'\" unpacked with wrong size!
 elif test -f 'wiz.rtf.UU.A'; then
   echo shar: Combining  \"'wiz.rtf.UU'\" \(103755 characters\)
   cat 'wiz.rtf.UU.A' 'wiz.rtf.UU.B' > 'wiz.rtf.UU'
   if test 103755 -ne `wc -c <'wiz.rtf.UU'`; then
     echo shar: \"'wiz.rtf.UU'\" combined with wrong size!
   else
     echo shar: Uudecoding \"'wizunzip.rtf'\" \(75283 characters\)
     cat 'wiz.rtf.UU' | uudecode
     if test 75283 -ne `wc -c <'wizunzip.rtf'`; then
       echo shar: \"'wizunzip.rtf'\" uudecoded with wrong size!
     else
       rm 'wiz.rtf.UU.A' 'wiz.rtf.UU.B' 'wiz.rtf.UU'
     fi
   fi
 fi
 # end of 'wiz.rtf.UU.B'
fi
echo shar: End of archive 2 \(of 6\).
cp /dev/null ark2isdone
MISSING=""
for I in 1 2 3 4 5 6 ; do
    if test ! -f ark${I}isdone ; then
	MISSING="${MISSING} ${I}"
    fi
done
if test "${MISSING}" = "" ; then
    echo You have unpacked all 6 archives.
    rm -f ark[1-9]isdone
else
    echo You still must unpack the following archives:
    echo "        " ${MISSING}
fi
exit 0
exit 0 # Just in case...
