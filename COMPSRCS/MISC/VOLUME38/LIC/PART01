    echo shar: Creating directory \"'lic.1.2/lic'\"
    mkdir 'lic.1.2/lic'
fi
if test ! -d 'lic.1.2/test' ; then
    echo shar: Creating directory \"'lic.1.2/test'\"
    mkdir 'lic.1.2/test'
fi
