if test -f 'lude-1.1/run/crim/sun4.1_sparc/include/lude/BldRegexpMinRqr.pl' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'lude-1.1/run/crim/sun4.1_sparc/include/lude/BldRegexpMinRqr.pl'\"
