  if test 56204 -ne `wc -c <'lude-1.1/src/orig/info/fra.texi.UU.B'`; then
    echo shar: \"'lude-1.1/src/orig/info/fra.texi.UU.B'\" unpacked with wrong size!
  elif test -f 'lude-1.1/src/orig/info/fra.texi.UU.A'; then
    echo shar: Combining  \"'lude-1.1/src/orig/info/fra.texi.UU'\" \(111966 characters\)
    cat 'lude-1.1/src/orig/info/fra.texi.UU.A' 'lude-1.1/src/orig/info/fra.texi.UU.B' > 'lude-1.1/src/orig/info/fra.texi.UU'
    if test 111966 -ne `wc -c <'lude-1.1/src/orig/info/fra.texi.UU'`; then
      echo shar: \"'lude-1.1/src/orig/info/fra.texi.UU'\" combined with wrong size!
    else
      rm lude-1.1/src/orig/info/fra.texi.UU.A lude-1.1/src/orig/info/fra.texi.UU.B
      echo shar: Uudecoding \"'lude-1.1/src/orig/info/fra.texi'\" \(81243 characters\)
      cat lude-1.1/src/orig/info/fra.texi.UU | uudecode
      if test 81243 -ne `wc -c <'lude-1.1/src/orig/info/lude_fra.texi'`; then
        echo shar: \"'lude-1.1/src/orig/info/lude_fra.texi'\" uudecoded with wrong size!
      else
        rm lude-1.1/src/orig/info/fra.texi.UU
      fi
      # end of 'lude-1.1/src/orig/info/fra.texi.UU'
    fi
  fi
  # end of 'lude-1.1/src/orig/info/fra.texi.UU.B'
