    echo shar: Combining  \"'lude-1.1/src/orig/info/eng.texi.UU'\" \(96625 characters\)
    cat 'lude-1.1/src/orig/info/eng.texi.UU.A' 'lude-1.1/src/orig/info/eng.texi.UU.B' > 'lude-1.1/src/orig/info/eng.texi.UU'
