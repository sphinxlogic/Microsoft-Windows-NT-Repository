    else
      rm lude-1.1/src/orig/info/eng.texi.UU.A lude-1.1/src/orig/info/eng.texi.UU.B
      echo shar: Uudecoding \"'lude-1.1/src/orig/info/lude_eng.texi'\" \(70092 characters\)
