    echo shar: Creating directory \"'lude-1.1/run/crim'\"
    mkdir 'lude-1.1/run/crim'
fi
if test ! -d 'lude-1.1/run/crim/share' ; then
    echo shar: Creating directory \"'lude-1.1/run/crim/share'\"
    mkdir 'lude-1.1/run/crim/share'
