echo 'File hodge-c-0.98j/magick/bz-009.gif is complete' &&
echo 'uudecoding file hodge-c-0.98j/magick/bz-009.gif' &&
uudecode < _shar_tmp_.tmp && rm -f _shar_tmp_.tmp &&
chmod 0640 hodge-c-0.98j/magick/bz-009.gif ||
echo 'restore of hodge-c-0.98j/magick/bz-009.gif failed'
Wc_c="`wc -c < 'hodge-c-0.98j/magick/bz-009.gif'`"
test 42188 -eq "$Wc_c" ||
	echo 'hodge-c-0.98j/magick/bz-009.gif: original size 42188, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
# ============= hodge-c-0.98j/magick/gif2vif ==============
if test -f 'hodge-c-0.98j/magick/gif2vif' -a X"$1" != X"-c"; then
	echo 'x - skipping hodge-c-0.98j/magick/gif2vif (File already exists)'
	rm -f _shar_wnt_.tmp
else
> _shar_wnt_.tmp
