plot "typ1.plt" using "%*d%*d%*d%*d%f" title "percent infected" with linespoints
pause -1 "[PRESS RETURN]"
X
set output "typ1-plt.ps"
set terminal postscript eps monochrome "Helvetica" 18
replot
X
