sed 's/^X//' << 'SHAR_EOF' > 'hodge-c-0.98j/panic.c' &&
X
/* panic.c -- error handler for noninteractive utilities
X
X   Copyright (C) 1991 Free Software Foundation, Inc.
X   Copyright (C) 1992, 1993 Joerg Heitkoetter
X
X   This program is free software; you can redistribute it and/or modify
X   it under the terms of the GNU General Public License as published by
X   the Free Software Foundation; either version 2, or (at your option)
X   any later version.
X
X   This program is distributed in the hope that it will be useful,
X   but WITHOUT ANY WARRANTY; without even the implied warranty of
X   MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the
X   GNU General Public License for more details.
X
X   You should have received a copy of the GNU General Public License
X   along with this program; if not, write to the Free Software
X   Foundation, Inc., 675 Mass Ave, Cambridge, MA 02139, USA.  */
X
/* based on `error.c' by David MacKenzie */
X
#ifndef lint
static char *rcsid = "$Id: panic.c,v 1.2 1993/03/19 11:28:30 heitkoet Exp heitkoet $";
#endif
X
#include <stdio.h>
#include <errno.h>
#include <time.h>
#include "panic.h"
X
#ifdef EDDIE
static char *adv[] =
{
X    "Sirius Cybernetics(TM)",
X    "charming",
X    "core dumping",
X    "dusty",
X    "extraordinary",
X    "faulty",
X    "funny",
X    "friendly",
X    "humble",
X    "irresistable",
X    "joking",
X    "marvellous",
X    "mostly harmless",
X    "old and tired",
X    "rusty",
X    "stupid",
X    "terminating",
};
#define ADVSIZ	(sizeof(adv)/sizeof(char *))
#endif
X
#ifndef VPRINTF_MISSING
X
#ifdef __STDC__
#include <stdarg.h>
#define VA_START(args, lastarg) va_start(args, lastarg)
#else /* not __STDC__ */
#include <varargs.h>
#define VA_START(args, lastarg) va_start(args)
#endif /* __STDC__ */
X
#else /* not VPRINTF_MISSING */
X
#ifndef DOPRNT_MISSING
#define va_alist args
#define va_dcl int args;
#else /* DOPRNT_MISSING */
#define va_alist a1, a2, a3, a4, a5, a6, a7, a8
#define va_dcl char *a1, *a2, *a3, *a4, *a5, *a6, *a7, *a8;
#endif /* DOPRNT_MISSING */
X
#endif /* VPRINTF_MISSING */
X
#ifdef STDC_HEADERS
#include <stdlib.h>
#include <string.h>
#else /* not STDC_HEADERS */
void exit ();
#endif /* STDC_HEADERS */
X
#ifdef STRERROR_MISSING
static char *
private_strerror (errnum)
X    int errnum;
{
X    extern char *sys_errlist[];
X    extern int sys_nerr;
X
X    if (errnum > 0 && errnum < sys_nerr)
X	return sys_errlist[errnum];
X    return "Unknown system error";
}
X
#define strerror private_strerror
#endif /* STRERROR_MISSING */
X
/* Print the program name and error message MESSAGE, which is a printf-style
X   format string with optional args.
X   If ERRNUM is nonzero, print its corresponding system error message.
X   Exit with status STATUS if it is nonzero. */
/* VARARGS */
int
X
#if !defined (VPRINTF_MISSING) && defined (__STDC__)
panic (int level, char *caller, char *message, ...)
#else /* VPRINTF_MISSING or not __STDC__ */
panic (level, caller, message, va_alist)
X    int level;
X    char *caller;
X    char *message;
X    va_dcl
#endif /* not VPRINTF_MISSING or __STDC__ */
{
X
#ifndef VPRINTF_MISSING
X    va_list args;
#endif /* VPRINTF_MISSING */
X
#ifdef EDDIE
X    srand (time (0));
X    fprintf (stderr, "\n\007*** DON'T PANIC\n");
X    fprintf (stderr, "Hi there, this is Eddie your %s ", adv[rand () % ADVSIZ]);
X    fprintf (stderr, "shipboard computer\n");
X    fprintf (stderr, "I really don't want to bother you, but I received\n");
X    if ((program_name != NULL) && (*program_name != '\0'))
X	fprintf (stderr, "the following error message from level %d while running `%s'...\n", level, program_name);
X    else
X	fprintf (stderr, "the following error message from level %d while running...\n", level);
#endif
X
X    if (*program_name != '\0')
X	fprintf (stderr, "%s: %s: ", program_name, caller);
X    else
X	fprintf (stderr, "%s: ", caller);
X
#ifndef VPRINTF_MISSING
X    VA_START (args, message);
X    vfprintf (stderr, message, args);
X    va_end (args);
#else /* VPRINTF_MISSING */
X
#ifndef DOPRNT_MISSING
X    _doprnt (message, &args, stderr);
#else /* DOPRNT_MISSING */
X    fprintf (stderr, message, a1, a2, a3, a4, a5, a6, a7, a8);
#endif /* DOPRNT_MISSING */
X
#endif /* VPRINTF_MISSING */
X
X    if (errno)
X	fprintf (stderr, ": %s", (char *) strerror (errno));
X    putc ('\n', stderr);
X
#ifdef EDDIE
X    fprintf (stderr, "***\n");
#endif
X
X    fflush (stderr);
X
#ifdef EGREP_COMPAT
X    if (level < 0)
X	exit (2);			/* e?grep fatal return code */
#else
X    if (level < 0)
X	exit (1);
#endif
X
X    return (level);
}
SHAR_EOF
chmod 0640 hodge-c-0.98j/panic.c ||
echo 'restore of hodge-c-0.98j/panic.c failed'
Wc_c="`wc -c < 'hodge-c-0.98j/panic.c'`"
test 4419 -eq "$Wc_c" ||
	echo 'hodge-c-0.98j/panic.c: original size 4419, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
