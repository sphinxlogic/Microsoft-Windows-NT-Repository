set terminal postscript eps monochrome "Helvetica" 18
replot
SHAR_EOF
chmod 0640 hodge-c-0.98j/typ2.gnu ||
echo 'restore of hodge-c-0.98j/typ2.gnu failed'
Wc_c="`wc -c < 'hodge-c-0.98j/typ2.gnu'`"
test 1066 -eq "$Wc_c" ||
	echo 'hodge-c-0.98j/typ2.gnu: original size 1066, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
# ============= hodge-c-0.98j/typ3 ==============
if test -f 'hodge-c-0.98j/typ3' -a X"$1" != X"-c"; then
	echo 'x - skipping hodge-c-0.98j/typ3 (File already exists)'
	rm -f _shar_wnt_.tmp
else
> _shar_wnt_.tmp
echo 'x - extracting hodge-c-0.98j/typ3 (Text)'
sed 's/^X//' << 'SHAR_EOF' > 'hodge-c-0.98j/typ3' &&
#!/bin/csh -f
set size  = 20
set speed = 10
set type  = typ3
X
hodge \
--no-film				\
--color-map-file-name   cmaps/bz.map	\
--number-of-frames	100		\
--groth-rate		$speed		\
--number-of-cell-states	100		\
--plot-file-name	$type.plt	\
--size-of-grid		$size		\
--time-steps		100		\
--moore-neighbourhood			\
--gnuplot				\
--dump-data-file-name	$type.dmp	\
--start-of-dump		100		\
--seed-for-random	12345678
X
#--monitor-process	"pixmon -dx $size -dy $size -scale 10 -cmap cmaps/bz.map"\
#--animation-file-name	<name>
#--color-map-file-name <name>
#--number-of-frames <number>
#--groth-rate <number>
#--monitor-process <name>
#--number-of-cell-states <number>
#--plot-file-name <name>
#--size-of-grid <size>
#--time-steps <number>
#--help
#--moore-neighbourhood
#--no-film
#--on-line-visualization
#--seed-for-random <seed value>
#--torus
#--version
#--warranty
X
set lines = `wc -l $type.plt | awk -e '{print $1}'`
X
if ($lines > 0) then
X    gnuplot $type.gnu
endif
SHAR_EOF
chmod 0750 hodge-c-0.98j/typ3 ||
