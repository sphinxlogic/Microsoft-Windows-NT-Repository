#!/bin/csh -f
X
foreach i ($*)
X  set out = `echo $i | sed -e 's/.gif//'`
X  echo "$i -> $out.vif"
X  giftoppm $i | pnmtorast -standard | rast2viff -i - -o $out.vif
end
SHAR_EOF
