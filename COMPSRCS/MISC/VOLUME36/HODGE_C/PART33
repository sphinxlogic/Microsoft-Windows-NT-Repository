sed 's/^X//' << 'SHAR_EOF' > 'hodge-c-0.98j/typ3.gnu' &&
set terminal x11
set title "Type 3"
set xlabel "Cycles"
set ylabel "Proportion of Infected Cells"
set grid
set format y "%.02f"
set yrange [0:1]
plot "typ3.plt" using "%*d%*d%*d%*d%f" title "percent infected" with linespoints
pause -1 "[PRESS RETURN]"
X
set output "typ3-plt.ps"
set terminal postscript eps monochrome "Helvetica" 18
replot
X
set terminal x11
set nogrid
set format y "%g"
set xlabel "X Cells"
set ylabel "Y Cells"
set zlabel "State"
set title "Hodge-Podge Machine - contour plot"
set contour
set cntrparam levels 5
set cntrparam bspline
set xrange [1:20]
set yrange [1:20]
set zrange [0.0:100]
set data style lines
set parametric
splot "typ3.dmp"  using 1:2:3
pause -1 "[PRESS RETURN]"
X
set output "typ3-cnt.ps"
set terminal postscript eps monochrome "Helvetica" 18
replot
X
set terminal x11
set noparametric
set samples 10000
set isosamples 10000
set hidden3d
set title "Hodge-Podge Machine - hidden line removal"
splot "typ3.dmp" using 3
pause -1 "[PRESS RETURN]"
X
set output "typ3-hid.ps"
set terminal postscript eps monochrome "Helvetica" 18
replot
SHAR_EOF
