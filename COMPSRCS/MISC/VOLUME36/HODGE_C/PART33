set nogrid
set format y "%g"
set xlabel "X Cells"
set ylabel "Y Cells"
set zlabel "State"
set title "Hodge-Podge Machine - contour plot"
set contour
set cntrparam levels 5
set cntrparam bspline
set xrange [1:20]
set yrange [1:20]
set zrange [0.0:100]
set data style lines
set parametric
splot "typ1.dmp"  using 1:2:3
pause -1 "[PRESS RETURN]"
X
set output "typ1-cnt.ps"
set terminal postscript eps monochrome "Helvetica" 18
replot
X
