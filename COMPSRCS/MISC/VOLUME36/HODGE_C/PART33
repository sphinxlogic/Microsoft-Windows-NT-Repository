--no-dump				\
--animation-file-name	/tmp/movie/bz-%003d \
--color-map-file-name   ../cmaps/bz.map	\
--start-of-film		100		\
--number-of-frames	10		\
--groth-rate		$speed		\
--number-of-cell-states	100		\
--plot-file-name	/dev/null	\
--size-of-grid		$size		\
--time-steps		110		\
--moore-neighbourhood			\
--seed-for-random	12345678
X
