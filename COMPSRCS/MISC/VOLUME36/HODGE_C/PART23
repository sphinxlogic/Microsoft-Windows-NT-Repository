rm -f _shar_wnt_.tmp
fi
# ============= hodge-c-0.98j/khoros/movie ==============
if test -f 'hodge-c-0.98j/khoros/movie' -a X"$1" != X"-c"; then
	echo 'x - skipping hodge-c-0.98j/khoros/movie (File already exists)'
	rm -f _shar_wnt_.tmp
else
> _shar_wnt_.tmp
echo 'x - extracting hodge-c-0.98j/khoros/movie (Text)'
sed 's/^X//' << 'SHAR_EOF' > 'hodge-c-0.98j/khoros/movie' &&
#!/bin/csh -f
set size  = 256
set speed = 30
set type  = movie
X
mkdir /tmp/movie
X
../hodge \
--no-dump				\
--animation-file-name	/tmp/movie/bz-%003d \
--color-map-file-name   ../cmaps/bz.map	\
--start-of-film		100		\
--number-of-frames	10		\
--groth-rate		$speed		\
--number-of-cell-states	100		\
--plot-file-name	/dev/null	\
--size-of-grid		$size		\
--time-steps		110		\
--moore-neighbourhood			\
--seed-for-random	12345678
X
foreach i (/tmp/movie/*)
X  ppmtogif $i | giftoppm | pnmtorast -standard | rast2viff -i - -o $i.vif
X  /bin/rm $i
X  mv $i.vif .
end
X
rmdir /tmp/movie
SHAR_EOF
chmod 0750 hodge-c-0.98j/khoros/movie ||
echo 'restore of hodge-c-0.98j/khoros/movie failed'
Wc_c="`wc -c < 'hodge-c-0.98j/khoros/movie'`"
test 575 -eq "$Wc_c" ||
	echo 'hodge-c-0.98j/khoros/movie: original size 575, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
# ============= hodge-c-0.98j/magick/README ==============
if test ! -d 'hodge-c-0.98j/magick'; then
    echo 'x - creating directory hodge-c-0.98j/magick'
    mkdir 'hodge-c-0.98j/magick'
fi
if test -f 'hodge-c-0.98j/magick/README' -a X"$1" != X"-c"; then
	echo 'x - skipping hodge-c-0.98j/magick/README (File already exists)'
	rm -f _shar_wnt_.tmp
else
> _shar_wnt_.tmp
echo 'x - extracting hodge-c-0.98j/magick/README (Text)'
sed 's/^X//' << 'SHAR_EOF' > 'hodge-c-0.98j/magick/README' &&
IMAGEMAGICK:
------------
X
1. generate frames for animation
X
X    example % movie
X
2. run ImageMagick's animate(1)
X
X    example% animate *.gif
SHAR_EOF
chmod 0640 hodge-c-0.98j/magick/README ||
echo 'restore of hodge-c-0.98j/magick/README failed'
Wc_c="`wc -c < 'hodge-c-0.98j/magick/README'`"
test 142 -eq "$Wc_c" ||
	echo 'hodge-c-0.98j/magick/README: original size 142, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
