echo 'restore of hodge-c-0.98j/LICENSE failed'
Wc_c="`wc -c < 'hodge-c-0.98j/LICENSE'`"
test 17982 -eq "$Wc_c" ||
	echo 'hodge-c-0.98j/LICENSE: original size 17982, current size' "$Wc_c"
