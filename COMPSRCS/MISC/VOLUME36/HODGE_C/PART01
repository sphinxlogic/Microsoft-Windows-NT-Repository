X   Foundation, Inc., 675 Mass Ave, Cambridge, MA 02139, USA. */
X
/* $Id: dos.cf,v 1.1 1993/03/16 10:35:58 joke Exp $ */
X
#ifndef __CONFIG_H__
#define __CONFIG_H__
X
#define V_OSTYPE	"DOS 5.x"
X
/*
X   #define V_OSTYPE	"OSTYPE"
SHAR_EOF
true || echo 'restore of hodge-c-0.98j/config/dos.cf failed'
fi
echo 'End of  part 1'
echo 'File hodge-c-0.98j/config/dos.cf is continued in part 2'
echo 2 > _shar_seq_.tmp
exit 0
exit 0 # Just in case...
