# ============= hodge-c-0.98j/Makefile ==============
if test -f 'hodge-c-0.98j/Makefile' -a X"$1" != X"-c"; then
	echo 'x - skipping hodge-c-0.98j/Makefile (File already exists)'
	rm -f _shar_wnt_.tmp
else
> _shar_wnt_.tmp
echo 'x - extracting hodge-c-0.98j/Makefile (Text)'
sed 's/^X//' << 'SHAR_EOF' > 'hodge-c-0.98j/Makefile' &&
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
# 	Makefile for HODGE-C: Gerhard & Schuster's hodge-podge machine
# 	$Id: Makefile,v 1.2 1993/03/19 11:28:34 heitkoet Exp heitkoet $
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
SHELL	= /bin/sh
X
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#	C compiler & tools
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
# define:
#  -DLOCAL_MAINTAINER  if you want to support HODGE-C locally (see version.h)
CC	= cc
CFLAGS	= -O4
X
#CC	= gcc -W
#CFLAGS	= -g -O
X
#CC	= acc
#CFLAGS	= -g
X
X
MKPS	= dvi2ps	#dvips -ta4 -r -f
MKDEP	= makedepend
MKPRO	= mkpro
MKTAGS	= ctags -t
MKPRETTY= tgrind
X
COMPRESS= compress -vf #gzip --best --verbose
X
LATEX	= latex
BIBTEX	= bibtex
X
#DITROFF	= groff -Tps
DITROFF	= psroff
NROFF	= nroff
X
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#	Logical structure
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
TOP	= .
X
HDR	= $(TOP)
HDRS	= $(HDR)/hodge.h	\
X	$(HDR)/version.h	\
X	$(HDR)/pixmon.h		\
X	$(HDR)/proto.h
X
XXHDRS	= $(HDR)/getopt.h	\
X	$(HDR)/panic.h
X
SRC	= $(TOP)
SRCS	= $(SRC)/hodge.c
X
XXSRCS	= $(SRC)/getopt.c	\
X	$(SRC)/getopt1.c	\
X	$(SRC)/panic.c
X
OBJ	= $(TOP)
OBJS	= $(OBJ)/hodge.o
X
XXOBJS	= $(OBJ)/getopt.o	\
X	$(OBJ)/getopt1.o	\
X	$(OBJ)/panic.o
X
CONFIG	= $(TOP)/config
X
CONFIGS	= $(CONFIG)/sunos.cf	\
X	$(CONFIG)/bsd.cf	\
X	$(CONFIG)/sys5.cf	\
X	$(CONFIG)/dos.cf	\
X	$(CONFIG)/tos.cf
X
DOC	= $(TOP)/docs
DOCS	= $(DOC)/*
X
MAN	= $(TOP)
MANS	= $(MAN)/*.1 $(MAN)/*.man
X
DEMO	= $(TOP)
DEMOS	= $(DEMO)/typ1 $(DEMO)/typ1.gnu \
X	$(DEMO)/typ2 $(DEMO)/typ2.gnu \
X	$(DEMO)/typ3 $(DEMO)/typ3.gnu \
X	$(DEMO)/typ4 $(DEMO)/typ4.gnu
X
MAP	= $(TOP)/cmaps
MAPS	= $(MAP)/*.map
X
IDL	= $(TOP)/idl
IDLS	= $(IDL)/*
X
MAGIC	= $(TOP)/magick
MAGICS	= $(MAGIC)/*
X
KHOROS	= $(TOP)/khoros
KHOROSS	= $(KHOROS)/*
X
CONTRIB	= $(TOP)/contrib
CONTRIBS= $(CONTRIB)/infekt2.c $(CONTRIB)/pixmon.1 $(CONTRIB)/pixmon.man
X
MISC	= $(TOP)/Makefile $(TOP)/README $(TOP)/COPYRIGHT $(TOP)/LICENSE \
X	$(TOP)/INSTALL $(TOP)/configure
X
PRG	= hodge
LIBS	= #-lm
X
JUNK	= *~ $(OBJ)/*.o core errs warns a.out *.bak *.ps *.plt *.log *.man *.ppm *.dmp *.shar.* *.tar.*
X
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#	General rules
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
.c.o:
X	$(CC) $(CFLAGS) -I$(SRC) -o $(TOP)/$@  -c $(TOP)/$<
X
$(PRG):	$(OBJS) $(XOBJS)
X	$(CC) $(CFLAGS) -o $@ $(OBJS) $(XOBJS) $(LIBS)
X
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#	Special/extra rules
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
XXFLAGS	= -DSTRERROR_MISSING ##-DEDDIE
$(OBJ)/panic.o: $(HDR)/panic.h $(SRC)/panic.c
X	$(CC) $(CFLAGS) $(XFLAGS) -I$(HDR) -o $(TOP)/$@  -c $(SRC)/panic.c
X
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#	Prototypes
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
pro:
X	$(RM) $(HDR)/proto.h
X	$(MKPRO) $(SRCS) > $(HDR)/proto.h
X
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#	Pretty printing
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
pretty:
X	$(MKPRETTY) $(HDRS) $(SRCS)
X
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#	Animations
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
figs:	$(PRG)
X	(cd film; make)
X
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#	Documenatation
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
man:
X	$(NROFF)   -man $(MAN)/$(PRG).1 >$(PRG).man
psman:
X	$(DITROFF) -man $(MAN)/$(PRG).1 >$(PRG)-man.ps
X
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#	Editor tags
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
tags:
X	$(RM) tags
X	$(MKTAGS) $(HDRS) $(SRCS) $(XHDRS) $(XSRCS)
X
index:
X	$(RM) index
X	$(MKTAGS) -x $(SRCS) $(XSRCS) | sort -f >index
X
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#	Version control
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
RCS	= $(TOP)/RCS
RCSS	= $(RCS)/*,v $(SRC)/RCS/*,v $(CONFIG)/RCS/*,v
RCSALL	= Makefile configure hodge.1 $(HDRS) $(SRCS) $(XHDRS) $(XSRCS) $(CONFIGS)
rcs:
X	$(RM) $(RCSS)
X	rcs -i -U -t-"Initial Version" -q $(RCSALL)
X
up:
X	ci -u -w"joke" -t-"Update" -q $(RCSALL)
X
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#	Distribution
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
VERS	= 0.98j
DIST	= hodge-c-$(VERS)
dist:	man
X	mkdir $(DIST)
X	cp $(HDRS) $(XHDRS) $(SRCS) $(XSRCS) $(DEMOS) $(MANS) $(MISC) $(DIST)
X	mkdir $(DIST)/config
X	cp $(CONFIGS) $(DIST)/config
X	mkdir $(DIST)/cmaps
X	cp $(MAPS) $(DIST)/cmaps
X	mkdir $(DIST)/contrib
X	cp $(CONTRIBS) $(DIST)/contrib
X	mkdir $(DIST)/idl
X	cp $(IDLS) $(DIST)/idl
X	mkdir $(DIST)/magick
X	cp $(MAGICS) $(DIST)/magick
X	mkdir $(DIST)/khoros
X	cp $(KHOROSS) $(DIST)/khoros
X	tar cvf $(DIST).tar $(DIST)
X	$(COMPRESS) $(DIST).tar
X	rm -rf $(DIST)
X
uue:	dist
X	uuencode $(DIST).tar.Z $(DIST).tar.Z > $(DIST).tar.Z.uue
X	rm -f $(DIST).tar.Z
X
shar:	dist
X	shar -c -n "hodge-c-$(VERS)" -a -M \
X		-s joke@ls11.informatik.uni-dortmund.de \
X		-f -L50 -d "*** MAGPIE ***" -o "$(DIST).shar" \
X		$(DIST).tar.Z
X	rm -f $(DIST).tar.Z
X
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#	Cleanup
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
clean:
X	$(RM) $(JUNK)
X
clobber: clean
X	$(RM) $(PRG) $(SRC)/config.h tags index
X
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
#	Dependencies
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
depend:
X	$(MKDEP) $(CFLAGS) $(SRCS) $(XSRCS)
X
# -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-
# DO NOT DELETE THIS LINE -- make depend depends on it.
X
hodge.o: hodge.h config.h /usr/include/stdio.h /usr/include/ctype.h
hodge.o: /usr/include/math.h /usr/include/floatingpoint.h
hodge.o: /usr/include/sys/ieeefp.h /usr/include/memory.h
hodge.o: /usr/include/string.h /usr/include/sys/stdtypes.h panic.h pixmon.h
hodge.o: proto.h getopt.h version.h
getopt.o: /usr/include/stdio.h getopt.h
getopt1.o: getopt.h
panic.o: /usr/include/stdio.h /usr/include/errno.h /usr/include/sys/errno.h
panic.o: /usr/include/time.h /usr/include/sys/stdtypes.h panic.h
panic.o: /usr/include/varargs.h
SHAR_EOF
chmod 0640 hodge-c-0.98j/Makefile ||
echo 'restore of hodge-c-0.98j/Makefile failed'
Wc_c="`wc -c < 'hodge-c-0.98j/Makefile'`"
test 6631 -eq "$Wc_c" ||
	echo 'hodge-c-0.98j/Makefile: original size 6631, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
# ============= hodge-c-0.98j/README ==============
if test -f 'hodge-c-0.98j/README' -a X"$1" != X"-c"; then
	echo 'x - skipping hodge-c-0.98j/README (File already exists)'
	rm -f _shar_wnt_.tmp
else
> _shar_wnt_.tmp
echo 'x - extracting hodge-c-0.98j/README (Text)'
sed 's/^X//' << 'SHAR_EOF' > 'hodge-c-0.98j/README' &&
HODGE-C:
--------
X
HODGE-C is a (`mostly ANSI') C language implemenatation of
Gerhard & Schuster's hodge-podge machine. It implements a class
of cellular automata, that resemble very closely autocatalytic
chemical reactions, like for example, the Belousov-Zhabotinskii (BZ)
reaction.
X
For an introductory article on hodge-podge see:
A.K.Dewdney's Computer Recreations column, in Scientific American,
August 1988.
X
HODGE-C is distributed free of charge and the other terms of the GPL, ie.
the GNU General Public License.
X
HODGE-C v0.98j is available via ftp from lumpi.informatik.uni-dortmund.de
(129.217.36.140).  Log on with user name "ftp" and give your full e-mail
address as password.  The file hodge-c-0.98j.tar.Z in directory /pub/CA/src
contains the complete software and documentation.
X
HODGE-C v0.98j was posted to comp.sources.misc, and thus is available
from every newsarchive server close to you.
In Germany ftp to ftp.germany.eu.net, and see directory
/pub/newsarchive/comp.sources.misc
X
For more information contact:
X
X	Joerg Heitkoetter
X	(joke@ls11.informatik.uni-dortmunde.de)
X
X    c/o Systems Analysis Group, LSXI
X	Department of Computer Science             ////
X	University of Dortmund                UNI DO// 
X	P.O. Box 500 500                     ___ ////
X	4600 Dortmund 50                     \*\\///
X	Germany                               \\\\/
SHAR_EOF
chmod 0640 hodge-c-0.98j/README ||
echo 'restore of hodge-c-0.98j/README failed'
