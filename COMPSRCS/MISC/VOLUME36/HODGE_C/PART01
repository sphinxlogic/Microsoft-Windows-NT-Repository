X   along with this program; if not, write to the Free Software
X   Foundation, Inc., 675 Mass Ave, Cambridge, MA 02139, USA. */
X
/* $Id: bsd.cf,v 1.1 1993/03/16 10:35:58 joke Exp $ */
X
#ifndef __CONFIG_H__
#define __CONFIG_H__
X
#define V_OSTYPE	"generic BSD un*x"
X
/*
X   #define V_OSTYPE	"OSTYPE"
X
X   #include <os-specials.h>
X     ...
X   #ifdef __COMPILER_TYPE__
X     #include <compiler-specials.h>
X     ...
X   #endif __COMPILER_TYPE__
X
X   #define RSC_FILE	"<rsc-file-name>"
X   #define RSC_PATH	"<rsc_path>" */
X
#include <stdio.h>
#include <ctype.h>
#include <math.h>
#include <strings.h>
X
#ifdef __STDC__
#include <stdlib.h>
#include <unistd.h>
#endif /* __STDC__ */
X
#define memcpy(a,b,c)	bcopy(b,a,c)
X
#define KEYPRESSED	(0)
#define GETKEY		(0)	
X
#define RSC_FILE	".hodge"
#define RSC_PATH	"/usr/local/lib:~:."
#endif /* __CONFIG_H__ */
SHAR_EOF
chmod 0640 hodge-c-0.98j/config/bsd.cf ||
echo 'restore of hodge-c-0.98j/config/bsd.cf failed'
Wc_c="`wc -c < 'hodge-c-0.98j/config/bsd.cf'`"
test 1481 -eq "$Wc_c" ||
	echo 'hodge-c-0.98j/config/bsd.cf: original size 1481, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
# ============= hodge-c-0.98j/config/dos.cf ==============
if test -f 'hodge-c-0.98j/config/dos.cf' -a X"$1" != X"-c"; then
	echo 'x - skipping hodge-c-0.98j/config/dos.cf (File already exists)'
	rm -f _shar_wnt_.tmp
else
> _shar_wnt_.tmp
echo 'x - extracting hodge-c-0.98j/config/dos.cf (Text)'
sed 's/^X//' << 'SHAR_EOF' > 'hodge-c-0.98j/config/dos.cf' &&
X
/* dos.cf -- generic PC DOS configurations
X
X   Copyright (C) 1993 Joerg Heitkoetter
