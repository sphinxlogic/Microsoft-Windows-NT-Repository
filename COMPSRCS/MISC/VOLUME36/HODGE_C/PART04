Archive-name: hodge-c/part04
Environment: ImageMagick, KHOROS

#!/bin/sh
# this is Part.04 (part 4 of a multipart archive)
# do not concatenate these parts, unpack them in order with /bin/sh
# file hodge-c-0.98j/hodge.h continued
#
if test ! -r _shar_seq_.tmp; then
	echo 'Please unpack part 1 first!'
	exit 1
fi
(read Scheck
 if test "$Scheck" != 4; then
	echo Please unpack part "$Scheck" next!
	exit 1
 else
	exit 0
 fi
) < _shar_seq_.tmp || exit 1
if test ! -f _shar_wnt_.tmp; then
	echo 'x - still skipping hodge-c-0.98j/hodge.h'
else
echo 'x - continuing file hodge-c-0.98j/hodge.h'
sed 's/^X//' << 'SHAR_EOF' >> 'hodge-c-0.98j/hodge.h' &&
} cell_s;
X
typedef unsigned char cell_v;
X
typedef struct {
X      cell_s state;
X      cell_v value;
} cell_t, CELL;
X
typedef cell_t grid_t[MAXCELLS][MAXCELLS];
X
/* color stuff: RGB 255 colors */
typedef unsigned char color_t[3];
X
typedef struct {
X	unsigned char red;
X	unsigned char green;
X	unsigned char blue;
} rgb_t, RGB;
X
typedef rgb_t cmap_t[MAXCOLORS];
X
/* prototyping */
#include "config.h"
#include "panic.h"
#include "pixmon.h"
#include "proto.h"
X
#endif /* __HODGE_H__ */
SHAR_EOF
echo 'File hodge-c-0.98j/hodge.h is complete' &&
chmod 0640 hodge-c-0.98j/hodge.h ||
echo 'restore of hodge-c-0.98j/hodge.h failed'
Wc_c="`wc -c < 'hodge-c-0.98j/hodge.h'`"
test 1815 -eq "$Wc_c" ||
	echo 'hodge-c-0.98j/hodge.h: original size 1815, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
# ============= hodge-c-0.98j/hodge.man ==============
if test -f 'hodge-c-0.98j/hodge.man' -a X"$1" != X"-c"; then
	echo 'x - skipping hodge-c-0.98j/hodge.man (File already exists)'
	rm -f _shar_wnt_.tmp
else
> _shar_wnt_.tmp
echo 'x - extracting hodge-c-0.98j/hodge.man (Binary)'
sed 's/^X//' << 'SHAR_EOF' > _shar_tmp_.tmp &&
begin 600 hodge-c-0.98j/hodge.man
