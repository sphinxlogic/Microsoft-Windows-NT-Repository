echo 'uudecoding file hodge-c-0.98j/hodge.man' &&
uudecode < _shar_tmp_.tmp && rm -f _shar_tmp_.tmp &&
chmod 0640 hodge-c-0.98j/hodge.man ||
echo 'restore of hodge-c-0.98j/hodge.man failed'
Wc_c="`wc -c < 'hodge-c-0.98j/hodge.man'`"
test 25096 -eq "$Wc_c" ||
	echo 'hodge-c-0.98j/hodge.man: original size 25096, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
# ============= hodge-c-0.98j/idl/README ==============
if test ! -d 'hodge-c-0.98j/idl'; then
    echo 'x - creating directory hodge-c-0.98j/idl'
    mkdir 'hodge-c-0.98j/idl'
fi
if test -f 'hodge-c-0.98j/idl/README' -a X"$1" != X"-c"; then
	echo 'x - skipping hodge-c-0.98j/idl/README (File already exists)'
	rm -f _shar_wnt_.tmp
else
> _shar_wnt_.tmp
echo 'x - extracting hodge-c-0.98j/idl/README (Text)'
sed 's/^X//' << 'SHAR_EOF' > 'hodge-c-0.98j/idl/README' &&
IDEMO:
------
X
1. start IDL
X
X    example % idl
X    IDL. Version 3.0.0 (sunos sparc).
X    Copyright 1989-1992, Research Systems, Inc.
X    All rights reserved.  Unauthorized reproduction prohibited.
X    Site: 1341.
X    Licensed for use by: Universitaet Dortmund
X
X    20 March 1992
X        The file openwin3.doc in the notes directory of the IDL
X        distribution describes how to solve the following problems:
X            - How to prevent IDL from complaining about missing fonts
X              when running an X Server other than OpenWindows.  
X            - How to prevent IDL from complaining about missing bitmap
X              files for widgets.
X            - Running IDL 2.2.3 or newer requires the system manager
X              to install an X Window system applications default file
X              for IDL before widgets will behave properly.
X            - How to prevent "Cannot allocate colormap entry" errors.
X
X    If you are tired of seeing this message, edit or delete the
X    file motd_sunos, in the main IDL directory.
X    IDL>
X
2. compile procedure `hodge'
X
X    IDL> .run hodge
X
3. plot data in mode 0..3
X
X    IDL> pl,3
SHAR_EOF
chmod 0640 hodge-c-0.98j/idl/README ||
echo 'restore of hodge-c-0.98j/idl/README failed'
Wc_c="`wc -c < 'hodge-c-0.98j/idl/README'`"
test 1133 -eq "$Wc_c" ||
	echo 'hodge-c-0.98j/idl/README: original size 1133, current size' "$Wc_c"
rm -f _shar_wnt_.tmp
fi
# ============= hodge-c-0.98j/idl/hermes.tbl ==============
if test -f 'hodge-c-0.98j/idl/hermes.tbl' -a X"$1" != X"-c"; then
	echo 'x - skipping hodge-c-0.98j/idl/hermes.tbl (File already exists)'
	rm -f _shar_wnt_.tmp
else
> _shar_wnt_.tmp
echo 'x - extracting hodge-c-0.98j/idl/hermes.tbl (Binary)'
