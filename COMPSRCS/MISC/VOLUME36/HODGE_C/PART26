#!/bin/sh
# this is Part.26 (part 26 of a multipart archive)
# do not concatenate these parts, unpack them in order with /bin/sh
# file hodge-c-0.98j/magick/bz-002.gif continued
#
if test ! -r _shar_seq_.tmp; then
	echo 'Please unpack part 1 first!'
	exit 1
fi
(read Scheck
 if test "$Scheck" != 26; then
	echo Please unpack part "$Scheck" next!
	exit 1
 else
	exit 0
 fi
) < _shar_seq_.tmp || exit 1
if test ! -f _shar_wnt_.tmp; then
	echo 'x - still skipping hodge-c-0.98j/magick/bz-002.gif'
else
echo 'x - continuing file hodge-c-0.98j/magick/bz-002.gif'
sed 's/^X//' << 'SHAR_EOF' >> '_shar_tmp_.tmp' &&
