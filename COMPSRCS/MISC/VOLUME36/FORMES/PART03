#! /bin/sh
# This is a shell archive.  Remove anything before this line, then unpack
# it by saving it into a file and typing "sh file".  To overwrite existing
# files, type "sh file -c".  You can also feed this as standard input via
# unshar, or by typing "sh <file", e.g..  If this archive is complete, you
# will see the following message at the end:
#		"End of archive 3 (of 3)."
# Contents:  verb.c xobjects
# Wrapped by chiltj@ois on Fri Mar 26 14:21:20 1993
PATH=/bin:/usr/bin:/usr/ucb ; export PATH
if test -f 'verb.c' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'verb.c'\"
else
echo shar: Extracting \"'verb.c'\" \(11946 characters\)
sed "s/^X//" >'verb.c' <<'END_OF_FILE'
X
X/*
X *  Copyright (C) 1992-1993 Jeffrey Chilton
X *
X *  Permission is granted to anyone to make or distribute copies of
X *  this program, in any medium, provided that the copyright notice
X *  and permission notice are preserved, and that the distributor
X *  grants the recipient permission for further redistribution as
X *  permitted by this notice.
X *  
X *  Author's E-mail address:  172-9221@mcimail.com
X *  
X */
X
Xstatic char *whatstring = "@(#)verb.c	2.6 JWC";
X
X#include <stdio.h>
X#include <string.h>
X
X#include "class.h"
X#include "form.h"
X#include "verb.h"
X#include "verbset.h"
X
Xextern Verb *Avoir;
Xextern Verb *Etre;
X
X#define MOD N_OF_PERSON
X
X/* Verb_new - Create an empty verb object */
X
XVerb *
XVerb_new(infinitive)
Xchar *infinitive;
X{
X    int pers;
X    int plur;
X    int gender;
X    Verb *self;
X
X    self = (Verb *)malloc(sizeof (Verb));
X    if (!self)
X    {
X	fprintf(stderr, "Verb_new: malloc fails\n");
X	goto out;
X    }
X
X    self->infinitive = ExtendString_newFromString(infinitive);
X
X    for (pers = 0; pers < N_OF_PERSON; pers++)
X    {
X	for (plur = 0; plur < N_OF_PLURAL; plur++)
X	{
X	    self->presIndEndings[pers][plur] = (ExtendString *)0;
X	    self->presSubjEndings[pers][plur] = (ExtendString *)0;
X	    self->imperfectEndings[pers][plur] = (ExtendString *)0;
X	}
X    }
X
X    self->imperativeEndings[0] = (ExtendString *)0;
X    self->imperativeEndings[1] = (ExtendString *)0;
X    self->imperativeEndings[2] = (ExtendString *)0;
X
X    self->presIndStem = (ExtendString *)0;
X    self->presSubjStem = (ExtendString *)0;
X    self->imperativeStem = (ExtendString *)0;
X    self->imperfectStem = (ExtendString *)0;
X    self->futureStem = (ExtendString *)0;
X    self->presentParticipal = (ExtendString *)0;
X    self->pastParticipal = (ExtendString *)0;
X
X    self->conjugationClass = 0;
X    self->auxiliary = -1;
X
Xout:
X
X    return self;
X
X}
X
X/* Verb_newFromFile - read formatted representation from file stream */
X
X#define FORMAT " %[^:]: %s %s %s %s %s %s %s "
X
X#define ENDS ends[0], ends[1], ends[2], ends[3], ends[4], ends[5]
X
XVerb *
XVerb_newFromFile(infinitive, stream)
Xchar *infinitive;
XFILE *stream;
X{
X    register int i;
X    Verb *self;
X    char oneLine[80];
X    char conjugate[32];
X    char ends[6][32];
X    char stem[32];
X    Form *form;
X    ExtendString *t, u;
X    int count;
X
X    self = Verb_new(infinitive);
X    if (!self)
X    {
X	goto out;
X    }
X
X    fgets(oneLine, 80, stream);
X    while (TRUE)
X    {
X	if (oneLine[0] == '\n' || oneLine[0] == '{')
X	{
X	    fgets(oneLine, 80, stream);
X	    continue;
X	}
X
X	if (0 == strcmp(oneLine, "}\n"))
X	{
X	    break;
X	}
X
X	count = sscanf(oneLine, FORMAT, conjugate, stem, ENDS);
X
X#if XDEBUG
Xprintf("Verb_newFromFile: %s: ", conjugate);
Xprintf("%s ", stem);
Xfor (i = 0; i < count - 2; i++)
X{
X    printf("%s ", ends[i]);
X}
Xprintf("\n");
X#endif
X
X	if (0 == strcmp(conjugate, "PRES-IND"))
X	{
X	    stem[strlen(stem) - 1] = '\0';
X	    self->presIndStem = ExtendString_newFromString(stem);
X	    self->thirdPersonOnly = count < 8;
X	    for (i = 0; i < count - 2; i++)
X	    {
X		t = ExtendString_newFromString(ends[i]);
X		if (self->thirdPersonOnly)
X		{
X		    i = PERSON_THIRD;
X		}
X	        self->presIndEndings[i % MOD][i / MOD] = t;
X	    }
X	}
X	else if (0 == strcmp(conjugate, "PRES-SUB"))
X	{
X	    stem[strlen(stem) - 1] = '\0';
X	    self->presSubjStem = ExtendString_newFromString(stem);
X	    for (i = 0; i < count - 2; i++)
X	    {
X		t = ExtendString_newFromString(ends[i]);
X		if (self->thirdPersonOnly)
X		{
X		    i = PERSON_THIRD;
X		}
X		self->presSubjEndings[i % MOD][i / MOD] = t;
X	    }
X	}
X	else if (0 == strcmp(conjugate, "IMPERFECT"))
X	{
X	    stem[strlen(stem) - 1] = '\0';
X	    self->imperfectStem = ExtendString_newFromString(stem);
X	    for (i = 0; i < count - 2; i++)
X	    {
X		t = ExtendString_newFromString(ends[i]);
X		if (self->thirdPersonOnly)
X		{
X		    i = PERSON_THIRD;
X		}
X		self->imperfectEndings[i % MOD][i / MOD] = t;
X	    }
X	}
X	else if (0 == strcmp(conjugate, "IMPERATIVE"))
X	{
X	    stem[strlen(stem) - 1] = '\0';
X	    self->imperativeStem = ExtendString_newFromString(stem);
X	    for (i = 0; i < count - 2; i++)
X	    {
X		t = ExtendString_newFromString(ends[i]);
X		self->imperativeEndings[i] = t;
X	    }
X	}
X	else if (0 == strcmp(conjugate, "FUTURE"))
X	{
X	    stem[strlen(stem) - 1] = '\0';
X	    self->futureStem = ExtendString_newFromString(stem);
X	}
X	else if (0 == strcmp(conjugate, "PRES-PART"))
X	{
X	    self->presentParticipal = ExtendString_newFromString(stem);
X	}
X	else if (0 == strcmp(conjugate, "PAST-PART"))
X	{
X	    self->pastParticipal = ExtendString_newFromString(stem);
X	}
X	else if (0 == strcmp(conjugate, "AUXILIARY"))
X	{
X	    if (0 == strcmp(stem, "avoir"))
X	    {
X		self->auxiliary = AUX_AVOIR;
X	    }
X	    else if (0 == strcmp(stem, "e^tre"))
X	    {
X		self->auxiliary = AUX_ETRE;
X	    }
X	    else
X	    {
X		fprintf(stderr, "Verb_newFromFile: unknown: %s\n", stem);
X		self = (Verb *)0;
X		goto out;
X	    }
X	}
X	else
X	{
X	    fprintf(stderr, "Verb_newFromFile: unknown: %s\n", conjugate);
X	    self = (Verb *)0;
X	    goto out;
X	}
X
X	fgets(oneLine, 80, stream);
X    }
X
Xout:
X
X    return self;
X
X}
X
X
X/* Verb_conjugate - assemble text of any form of a verb */
X
XExtendString *ImperfectEndings[N_OF_PERSON][N_OF_PLURAL] =
X{
X    (ExtendString *)"ais",
X    (ExtendString *)"ions",
X    (ExtendString *)"ais",
X    (ExtendString *)"iez",
X    (ExtendString *)"ait",
X    (ExtendString *)"aient"
X};
X
XExtendString *FutureEndings[N_OF_PERSON][N_OF_PLURAL] =
X{
X    (ExtendString *)"ai",
X    (ExtendString *)"ons",
X    (ExtendString *)"as",
X    (ExtendString *)"ez",
X    (ExtendString *)"a",
X    (ExtendString *)"ont"
X};
X
XExtendString *AgreementEndings[N_OF_PLURAL][N_OF_GENDER] =
X{
X    (ExtendString *)"",
X    (ExtendString *)"e",
X    (ExtendString *)"s",
X    (ExtendString *)"es"
X};
X
XExtendString *
XVerb_conjugate(self, form, tense)
XVerb *self;
XForm *form;
Xint tense;
X{
X    register int i, j;
X    ExtendString cg[80];
X    ExtendString *e, *f;
X    ExtendString *result;
X
X    i = form->person;
X    j = form->plurality;
X
X    if (self->thirdPersonOnly && (i != PERSON_THIRD || j != PLURAL_SINGULAR))
X    {
X	result = (ExtendString *)0;
X	goto out;
X    }
X
X    switch (tense)
X    {
X	case TENSE_INFINITIVE:
X
X	    ExtendString_copy(cg, self->infinitive);
X	    break;
X
X	case TENSE_PRESENT:
X
X	    ExtendString_copy(cg, self->presIndStem);
X	    ExtendString_cat(cg, self->presIndEndings[i][j]);
X	    break;
X
X	case TENSE_IMPERFECT:
X
X	    ExtendString_copy(cg, self->imperfectStem);
X	    e = self->imperfectEndings[i][j];
X	    f = ImperfectEndings[i][j];
X	    ExtendString_cat(cg, e ? e : f);
X	    break;
X
X	case TENSE_PASTCOMP:
X
X	    if (self->auxiliary == AUX_ETRE)
X	    {
X		e = Verb_conjugate(Etre, form, TENSE_PRESENT);
X		ExtendString_copy(cg, e);
X		ExtendString_cat(cg, " ");
X		ExtendString_cat(cg, self->pastParticipal);
X		ExtendString_cat(cg, AgreementEndings[j][form->gender]);
X	    }
X	    else
X	    {
X		e = Verb_conjugate(Avoir, form, TENSE_PRESENT);
X		ExtendString_copy(cg, e);
X		ExtendString_cat(cg, " ");
X		ExtendString_cat(cg, self->pastParticipal);
X	    }
X	    ExtendString_destroy(e);
X	    break;
X
X	case TENSE_FUTURE:
X
X	    ExtendString_copy(cg, self->futureStem);
X	    ExtendString_cat(cg, FutureEndings[i][j]);
X	    break;
X
X	case TENSE_CONDITION:
X
X	    ExtendString_copy(cg, self->futureStem);
X	    ExtendString_cat(cg, ImperfectEndings[i][j]);
X	    break;
X
X	case TENSE_SUBJUNCT:
X
X	    ExtendString_copy(cg, self->presSubjStem);
X	    ExtendString_cat(cg, self->presSubjEndings[i][j]);
X	    break;
X
X	case TENSE_IMPERATIVE:
X
X	    ExtendString_copy(cg, self->imperativeStem);
X	    ExtendString_cat(cg, self->imperativeEndings[IMPERSUM(i, j)]);
X	    break;
X
X	case TENSE_PLUPERFECT:
X
X	    if (self->auxiliary == AUX_ETRE)
X	    {
X		e = Verb_conjugate(Etre, form, TENSE_IMPERFECT);
X		ExtendString_copy(cg, e);
X		ExtendString_cat(cg, " ");
X		ExtendString_cat(cg, self->pastParticipal);
X		ExtendString_cat(cg, AgreementEndings[j][form->gender]);
X	    }
X	    else
X	    {
X		e = Verb_conjugate(Avoir, form, TENSE_IMPERFECT);
X		ExtendString_copy(cg, e);
X		ExtendString_cat(cg, " ");
X		ExtendString_cat(cg, self->pastParticipal);
X	    }
X	    ExtendString_destroy(e);
X	    break;
X
X	case TENSE_PASTSIMP:
X	case TENSE_IMPSUBJ:
X	case TENSE_PASTANT:
X	case TENSE_FUTUREANT:
X	case TENSE_CONDPERF:
X	case TENSE_PLUPSUBJ:
X
X	    fprintf(stderr, "Verb_conjugate: not implemented: #%d\n", tense);
X	    break;
X
X	default:
X
X	    fprintf(stderr, "Verb_conjugate: unknown tense: %d\n", tense);
X	    break;
X
X    }
X
X    i = ExtendString_len(cg) + 1;
X    result = (ExtendString *)malloc(i);
X    if (!result)
X    {
X	fprintf(stderr, "Verb_conjugate: malloc fails\n");
X	goto out;
X    }
X
X    ExtendString_copy(result, cg);
X
X#if XDEBUG
X    printf("Verb_conjugate: got %s (%s)\n", result, self->infinitive);
X#endif
X
Xout:
X
X    return result;
X
X}
X
Xvoid
XVerb_destroy(self)
XVerb *self;
X{
X
X    ExtendString_destroy(self->presentParticipal);
X    ExtendString_destroy(self->pastParticipal);
X    ExtendString_destroy(self->imperativeStem);
X    ExtendString_destroy(self->presSubjStem);
X    ExtendString_destroy(self->presIndStem);
X    ExtendString_destroy(self->imperfectStem);
X    ExtendString_destroy(self->infinitive);
X
X    free(self);
X
X}
X
X#ifndef PRODUCTION
X
X/* Verb_printOn - write formatted representation to output file stream */
X
Xvoid
XVerb_printOn(self, stream)
XVerb *self;
XFILE *stream;
X{
X    register int i, j;
X    ExtendString *e;
X    int length;
X    char *t, *f;
X
X    t = ExtendString_externalFormat(self->infinitive);
X    fprintf(stream, "VERB: %s (%d)\n", t, self->conjugationClass);
X    fprintf(stream, "{\n");
X    free(t);
X
X    /* Output present indicative line */
X
X    t = ExtendString_externalFormat(self->presIndStem);
X    fprintf(stream, "    PRES-IND: %s- ", t);
X    free(t);
X    for (j = 0; j < N_OF_PLURAL; j++)
X    {
X	for (i = 0; i < N_OF_PERSON; i++)
X	{
X	    e = self->presIndEndings[i][j];
X	    if (e)
X	    {
X		t = ExtendString_externalFormat(e);
X		fprintf(stream, "%s ", t);
X		free(t);
X	    }
X	}
X    }
X    fprintf(stream, "\n");
X
X    /* Output present subjunctive line */
X
X    t = ExtendString_externalFormat(self->presSubjStem);
X    fprintf(stream, "    PRES-SUB: %s- ", t);
X    free(t);
X    for (j = 0; j < N_OF_PLURAL; j++)
X    {
X	for (i = 0; i < N_OF_PERSON; i++)
X	{
X	    e = self->presSubjEndings[i][j];
X	    if (e)
X	    {
X		t = ExtendString_externalFormat(e);
X		fprintf(stream, "%s ", t);
X		free(t);
X	    }
X	}
X    }
X    fprintf(stream, "\n");
X
X    /* Output imperatives */
X
X    e = self->imperativeStem;
X    if (e)
X    {
X	t = ExtendString_externalFormat(e);
X	fprintf(stream, "    IMPERATIVE: %s- ", t);
X	free(t);
X	for (j = 0; j < N_OF_PLURAL + 1; j++)
X	{
X	    e = self->imperativeEndings[j];
X	    t = ExtendString_externalFormat(e);
X	    fprintf(stream, "%s ", t);
X	    free(t);
X	}
X	fprintf(stream, "\n");
X    }
X
X    /* Output imperfect */
X
X    if (self->imperfectStem)
X    {
X	t = ExtendString_externalFormat(self->imperfectStem);
X        fprintf(stream, "    IMPERFECT: %s- ", t);
X	free(t);
X	for (j = 0; j < N_OF_PLURAL; j++)
X	{
X	    for (i = 0; i < N_OF_PERSON; i++)
X	    {
X		e = self->imperfectEndings[i][j];
X		if (e)
X		{
X		    t = ExtendString_externalFormat(e);
X		    fprintf(stream, "%s ", t);
X		    free(t);
X		}
X	    }
X	}
X	fprintf(stream, "\n");
X    }
X
X    /* Output future stem */
X
X    if (self->futureStem)
X    {
X	t = ExtendString_externalFormat(self->futureStem);
X        fprintf(stream, "    FUTURE: %s-\n", t);
X	free(t);
X    }
X
X    /* Output participals */
X
X    if (self->presentParticipal)
X    {
X	t = ExtendString_externalFormat(self->presentParticipal);
X	fprintf(stream, "    PRES-PART: %s\n", t);
X	free(t);
X    }
X
X    if (self->pastParticipal)
X    {
X	t = ExtendString_externalFormat(self->pastParticipal);
X	fprintf(stream, "    PAST-PART: %s\n", t);
X	free(t);
X    }
X
X    switch (self->auxiliary)
X    {
X	case AUX_AVOIR:
X	    fprintf(stream, "    AUXILIARY: avoir\n");
X	    break;
X	case AUX_ETRE:
X	    fprintf(stream, "    AUXILIARY: e^tre\n");
X	    break;
X    }
X
X    fprintf(stream, "}\n\n");
X
X}
X
X#endif
END_OF_FILE
if test 11946 -ne `wc -c <'verb.c'`; then
    echo shar: \"'verb.c'\" unpacked with wrong size!
fi
# end of 'verb.c'
fi
if test -f 'xobjects' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'xobjects'\"
else
echo shar: Extracting \"'xobjects'\" \(31532 characters\)
sed "s/^X//" >'xobjects' <<'END_OF_FILE'
X
X#
X#  Copyright (C) 1992-1993 Jeffrey Chilton
X#
X#  Permission is granted to anyone to make or distribute copies of
X#  this file, in any medium, provided that the copyright notice
X#  and permission notice are preserved, and that the distributor
X#  grants the recipient permission for further redistribution as
X#  permitted by this notice.
X#  
X#  Author's E-mail address:  172-9221@mcimail.com
X#  
X
X# static char *whatstring = "@(#)xobjects	2.8 JWC";
X
X
X#  Subjects
X
XSUBJECT: 1SM je
XSUBJECT: 1SM moi, je
XSUBJECT: 1PM Annick et moi, nous
X
X#  Note:  change "M" to "F" on the previous three lines if you're a gurl
X
XSUBJECT: 1PM nous
XSUBJECT: 2PM vous
XSUBJECT: 2PM vous deux, vous
X
X#  Change these three too if you want...
X
XSUBJECT: 2SM tu
XSUBJECT: 2SM tu
XSUBJECT: 3SM il
XSUBJECT: 3SF Annick
XSUBJECT: 3PM Annick et Guy
XSUBJECT: 3PF elles
X
X
X#  Verbs
X
XVERB: accepter (1)
X{
X    PRES-IND: accept- e es e ons ez ent 
X    PRES-SUB: accept- e es e ions iez ent
X    IMPERATIVE: accept- e ons ez 
X    IMPERFECT: accept-
X    FUTURE: accepter-
X    PRES-PART: acceptant
X    PAST-PART: accepte'
X    AUXILIARY: avoir
X}
X
XVERB: acheter (108)
X{
X    PRES-IND: ach- e`te e`tes e`te etons etez e`tent 
X    PRES-SUB: ach- e`te e`tes e`te etions etiez e`tent
X    IMPERATIVE: ach- e`tes etons etez 
X    IMPERFECT: achet-
X    FUTURE: ache`ter-
X    PRES-PART: achetant
X    PAST-PART: achete'
X    AUXILIARY: avoir
X}
X
XVERB: agacer (5) [V15]
X{
X    PRES-IND: aga- ce ces ce c,ons cez cent 
X    PRES-SUB: agac- e es e ions iez ent
X    IMPERATIVE: aga- ce c,ons cez 
X    IMPERFECT: aga- c,ais c,ais c,ait cions ciez c,aient
X    FUTURE: agacer-
X    PRES-PART: agac,ant
X    PAST-PART: agace'
X    AUXILIARY: avoir
X}
X
XVERB: aimer (1)
X{
X    PRES-IND: aim- e es e ons ez ent 
X    PRES-SUB: aim- e es e ions iez ent
X    IMPERATIVE: aim- e ons ez 
X    IMPERFECT: aim-
X    FUTURE: aimer-
X    PRES-PART: aimant
X    PAST-PART: aime'
X    AUXILIARY: avoir
X}
X
XVERB: aller (15)
X{
X    PRES-IND: - vais vas va allons allez vont 
X    PRES-SUB: a- ille illes ille llions lliez illent
X    IMPERATIVE: - va allons allez 
X    IMPERFECT: all-
X    FUTURE: ir-
X    PRES-PART: allant
X    PAST-PART: alle'
X    AUXILIARY: e^tre
X}
X
XVERB: amener (5)
X{
X    PRES-IND: am- e`ne e`nes e`ne enons enez e`nent
X    PRES-SUB: am- e`ne e`nes e`ne enions eniez e`nent
X    IMPERATIVE: am- e`ne enons enez
X    IMPERFECT: amen-
X    FUTURE: ame`ner-
X    PRES-PART: amenant
X    PAST-PART: amene'
X    AUXILIARY: avoir
X}
X
XVERB: annoncer (6) [V15]
X{
X    PRES-IND: annon- ce ces ce c,ons cez cent 
X    PRES-SUB: annonc- e es e ions iez ent
X    IMPERATIVE: annon- ce c,ons cez 
X    IMPERFECT: annon- c,ais c,ais c,ait cions ciez c,aient
X    FUTURE: annoncer-
X    PRES-PART: annonc,ant
X    PAST-PART: annonce'
X    AUXILIARY: avoir
X}
X
XVERB: appeler (8)
X{
X    PRES-IND: appel- le les le ons ez lent 
X    PRES-SUB: appel- le les le ions iez lent
X    IMPERATIVE: appel- le ons ez 
X    IMPERFECT: appel-
X    FUTURE: appeller-
X    PRES-PART: appelant
X    PAST-PART: appele'
X    AUXILIARY: avoir
X}
X
XVERB: apparai^tre (74) [V15]
X{
X    PRES-IND: appara- is is i^t issons issez issent 
X    PRES-SUB: apparaiss- e es e ions iez ent
