X
X     1 {([Rr])iu([Mm][^A-Za-z])} -1   {\1\0xC9\0xD5\2} # iu
X     1 {([^A-Za-z])i(ud)}        -1   {\1\0xC9\2}
X     1 "dius"                     0   "\0xC4\0xC9\0xD5\0xD3"
X     1 {[iyj]u}                   0   "\0xC0"    #ju
