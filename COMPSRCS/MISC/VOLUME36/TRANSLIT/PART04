  if test 10408 -ne `wc -c <'example.pho.UU'`; then
    echo shar: \"'example.pho.UU'\" unpacked with wrong size!
  else
    echo shar: Uudecoding \"'example.pho'\" \(7531 characters\)
    cat example.pho.UU | uudecode
    if test 7531 -ne `wc -c <'example.pho'`; then
      echo shar: \"'example.pho'\" uudecoded with wrong size!
    else
      rm example.pho.UU
    fi
  fi
  # end of 'example.pho.UU'
fi
if test -f 'translit.ps.A' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'translit.ps.A'\"
else
  echo shar: Extracting \"'translit.ps.A'\" \(50632 characters\)
  sed "s/^X//" >'translit.ps.A' <<'END_OF_FILE'
X%!PS-Adobe-1.0
X%%Creator: krakow:jkl (Jan Labanowski)
X%%Title: stdin
X%%CreationDate: Thu Mar 18 13:26:08 1993
