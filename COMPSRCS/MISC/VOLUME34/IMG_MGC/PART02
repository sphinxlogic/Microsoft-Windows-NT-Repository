# ============= ImageMagick/xtp/xtp.h ==============
if test -f 'ImageMagick/xtp/xtp.h' -a X"$1" != X"-c"; then
	echo 'x - skipping ImageMagick/xtp/xtp.h (File already exists)'
	rm -f _shar_wnt_.tmp
