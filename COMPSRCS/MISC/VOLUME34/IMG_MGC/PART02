echo 'x - extracting ImageMagick/xtp/regular.c (Text)'
sed 's/^X//' << 'SHAR_EOF' > 'ImageMagick/xtp/regular.c' &&
/*
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%                                                                             %
