END_OF_FILE
  if test 52541 -ne `wc -c <'acron.2'`; then
    echo shar: \"'acron.2'\" unpacked with wrong size!
 else
    for J in 1 2 3 4 ; do
       if test ! -f acron.${J} ; then
          TOGO="${TOGO} ${J}"
       fi
    done 
    if test "${TOGO}" = "" ; then
       echo shar: Combining  \"'acron'\" \(203842 characters\)
       cat acron.* > 'acron'
       if test 203842 -ne `wc -c <'acron'`; then
          echo shar: \"'acron'\" combined with wrong size!
       else
          rm acron.[1-4]
       fi
     fi  
  fi
  # end of 'acron.2'
fi
if test -f 'whats' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'whats'\"
else
  echo shar: Extracting \"'whats'\" \(2044 characters\)
  sed "s/^X//" >'whats' <<'END_OF_FILE'
X#!/bin/sh
X#
X# NAME
X#      whats - lookup acronym/abbreviation in database
X#
X# SYNOPSIS
X#      whats [acronym [defn...]]
X#
X# DESCRIPTION
X#      Whats, without any arguments, prompts for an acronym to look up.
X#
X#      With one argument, whats prints all entries from the database
X#      that match the argument.  If no matches are found, whats
X#      prompts for expansion of the acronym or abbreviation.
X#
X#      With more than one argument, whats scans the database for
X#      entries matching the first argument.  If any matches are found,
X#      whats prints the matching entries and asks if the remaining
X#      arguments should be added as a new entry.  If no matches are
X#      found, a new entry is created for the first argument with the
X#      remaining arguments forming the expansion.
X#
X# FILES
X#      acron or $ACRON
X#
X# BUGS/DEFICIENCIES
X#      Not blindingly fast.
X#      Not tested under System V.  (Uses "echo -n", "grep -i")
X#
X# AUTHOR
X#      Dave Sill (dsill@nswc-oas.arpa) (de5@ornl.gov as of 6/90)
X#      Naval Surface Warfare Center (NSWC), Dahlgren, VA
X#
Xn='-n'	# Use '' if your echo uses '\c'
Xc=''	# Use '\c' if your echo uses it
Xq='-i'	# Use '' or your grep's "ignore case" flag
X
Xif [ -z "$1" ]
Xthen
X    echo $n "What is what? $c"
X    read ac
Xelse
X    ac=$1
Xfi
Xac=`echo ${ac} | tr "[a-z]" "[A-Z]"`
X
Xif [ ! -f acron ]
Xthen
X    acron=${ACRON}
Xelse
X    acron=acron
Xfi
Xi=`grep $q -c "^${ac}	" ${acron}`
X
Xif [ -z "$2" -a $i -ne 0 ]
Xthen
X    # No definition passed and is on file, so just do a lookup.
X    grep $q "^${ac}	" ${acron}
X    exit
Xelif [ $i -ne 0 ]
Xthen
X    # Is on file and a definition was passed, check before adding.
X    echo $i occurrences found.
X    grep $q "^${ac}	" ${acron}
X    echo $n "Still want to add ${ac}? $c"
X    read reply
X    if [ "${reply}" = n -o "${reply}" = N ]
X    then
X	exit
X    fi
Xfi
Xif [ -z "$2" ]
Xthen
X    echo $n "What does ${ac} stand for? $c"
X    read def
X    if [ -z "${def}" ]
X    then
X	exit
X    fi
Xelse
X    shift
X    def=$*
Xfi
Xecho "${ac}	- ${def}" >>${acron}
Xecho "${ac}	- ${def}"
END_OF_FILE
  if test 2044 -ne `wc -c <'whats'`; then
    echo shar: \"'whats'\" unpacked with wrong size!
  fi
  chmod +x 'whats'
  # end of 'whats'
fi
echo shar: End of archive 3 \(of 4\).
cp /dev/null ark3isdone
MISSING=""
for I in 1 2 3 4 ; do
    if test ! -f ark${I}isdone ; then
	MISSING="${MISSING} ${I}"
    fi
done
if test "${MISSING}" = "" ; then
    echo You have unpacked all 4 archives.
    rm -f ark[1-9]isdone
else
    echo You still must unpack the following archives:
    echo "        " ${MISSING}
fi
exit 0
exit 0 # Just in case...
