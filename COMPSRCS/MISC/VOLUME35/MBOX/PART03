  if test 24910 -ne `wc -c <'etc/deutsch.h.U'`; then
    echo shar: \"'etc/deutsch.h.U'\" unpacked with wrong size!
  else
    echo shar: Uudecoding \"'etc/deutsch.hilfe'\" \(18054 characters\)
    cat etc/deutsch.h.U | uudecode
    if test 18054 -ne `wc -c <'etc/deutsch.hilfe'`; then
      echo shar: \"'etc/deutsch.hilfe'\" uudecoded with wrong size!
    else
      rm etc/deutsch.h.U
    fi
  fi
  # end of 'etc/deutsch.h.U'
fi
if test -f 'src/mbox.msg' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'src/mbox.msg'\"
else
  echo shar: Extracting \"'src/mbox.msg'\" \(27223 characters\)
