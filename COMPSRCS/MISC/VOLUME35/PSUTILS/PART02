#! /bin/sh
# This is a shell archive.  Remove anything before this line, then feed it
# into a shell via "sh file" or similar.  To overwrite existing files,
# type "sh file -c".
# Contents:  fixwpps md71.0.ps.U psnup
# Wrapped by kent@sparky on Mon Feb  1 23:31:45 1993
PATH=/bin:/usr/bin:/usr/ucb:/usr/local/bin:/usr/lbin ; export PATH
echo If this archive is complete, you will see the following message:
echo '          "shar: End of archive 2 (of 4)."'
if test -f 'fixwpps' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'fixwpps'\"
else
  echo shar: Extracting \"'fixwpps'\" \(525 characters\)
  sed "s/^X//" >'fixwpps' <<'END_OF_FILE'
X#!/usr/local/bin/perl
X# fixwpps: get semi-conforming PostScript out of WordPerfect 5.0 file
X
X$page = 1;
Xwhile (<>) {
X   s/(_[a-zA-Z]+)([0-9]+)/\1 \2/g;
X   if (/^_bp/) {
X      print STDOUT "%%Page: $page $page\n";
X      print STDOUT $_;
X      $page++;
X   } elsif (/^([^\/].*\S.*)(_bp.*)/) {
X      print STDOUT "$1\n";
X      print STDOUT "%%Page: $page $page\n";
X      print STDOUT "$2\n";
X      $page++;
X   } elsif (/^_ed/) {
X      print STDOUT "%%Trailer:\n";
X      print STDOUT $_;
X   } else {
X      print STDOUT $_;
X   }
X}
END_OF_FILE
  if test 525 -ne `wc -c <'fixwpps'`; then
    echo shar: \"'fixwpps'\" unpacked with wrong size!
  fi
  chmod +x 'fixwpps'
  # end of 'fixwpps'
fi
if test -f 'md71.0.ps.U' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'md71.0.ps.U'\"
else
  echo shar: Extracting \"'md71.0.ps.U'\" \(48726 characters\)
  sed "s/^X//" >'md71.0.ps.U' <<'END_OF_FILE'
