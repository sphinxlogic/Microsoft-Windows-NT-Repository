  if test 65106 -ne `wc -c <'libftp/etc/rus.ps.B'`; then
    echo shar: \"'libftp/etc/rus.ps.B'\" unpacked with wrong size!
  elif test -f 'libftp/etc/rus.ps.A'; then
    echo shar: Combining  \"'libftp/etc/rus.ps'\" \(124331 characters\)
    cat 'libftp/etc/rus.ps.A' 'libftp/etc/rus.ps.B' > 'libftp/etc/rus.ps'
    if test 124331 -ne `wc -c <'libftp/etc/rus.ps'`; then
      echo shar: \"'libftp/etc/rus.ps'\" combined with wrong size!
    else
      rm libftp/etc/rus.ps.A libftp/etc/rus.ps.B 
    fi
  fi
  # end of 'libftp/etc/rus.ps.B'
fi
if test ! -d 'libftp/examples' ; then
    echo shar: Creating directory \"'libftp/examples'\"
    mkdir 'libftp/examples'
fi
if test ! -d 'libftp/utils' ; then
    echo shar: Creating directory \"'libftp/utils'\"
    mkdir 'libftp/utils'
fi
echo shar: End of archive 1 \(of 5\).
cp /dev/null ark1isdone
MISSING=""
for I in 1 2 3 4 5 ; do
    if test ! -f ark${I}isdone ; then
	MISSING="${MISSING} ${I}"
    fi
done
if test "${MISSING}" = "" ; then
    echo You have unpacked all 5 archives.
    rm -f ark[1-9]isdone
else
    echo You still must unpack the following archives:
    echo "        " ${MISSING}
fi
exit 0
exit 0 # Just in case...
