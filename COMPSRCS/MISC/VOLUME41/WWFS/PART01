Xdup scale
Xnewpath
X} bind def
X/@pop {
XSpecialSave restore
X} bind def
X/@pn
X/setlinewidth load
Xdef
X/@pt {
Xmoveto currentpoint currentlinewidth 2 div 0 360 arc fill
X} bind def
X/@ln {
Xmoveto lineto stroke
X} bind def
X/@ar {
Xmatrix currentmatrix
X7 -2 roll moveto
Xcurrentpoint translate
Xnewpath
X5 -2 roll scale
X0 0 1 6 -2 roll arc
Xsetmatrix
X} bind def
X/@wh {1 setgray} bind def
X/@sh {0.75 setgray} bind def
X/@bk {0 setgray} bind def
X/@RGB {setrgbcolor} bind def
X/@HSB {sethsbcolor} bind def
X/@CMYK {setcmykcolor} bind def
Xend
X/@endspecial {
Xclear
XTeXDict begin
XSpecialSave restore
X} bind def
Xsystemdict /setpacking known {
Xsavepacking setpacking
X} if
X%%EndFile
X%%EndProlog
X%%BeginSetup
X%
X% PStick for dvi2ps-j, version 1.0 (preamble)
X%
X/ShowDict 8 dict def
XTeXDict begin
X/oldEP /EP load def		% Save old EP command (dvi2ps-j)
XShowDict begin
X%pstick wwfsrotate
X/wwfsrotate {
X  gsave
X    /Helvetica-Bold findfont 10 scalefont setfont
X    deg1 20 add /deg1 exch def
X%    .95 setgray
X    500 50 moveto deg1 rotate (WWFS) shadowshow
X  grestore 
X} bind def
X
X/deg1 0 def
X
X%Title: shadowshow
X%Creator: DEMIZU Noritoshi
X%CreationDate: Mon Dec 22 1992
X%
X% Definition of ``shadowshow''
X%	stack: text -> --
X%	current point moves to after text
X%
X% /shadowshowdict 3 dict def
X/shadowshow {			% strings -> --
X%   shadowshowdict begin
X  gsave
X    /text exch def		% argument string is in ``text''
X    text stringwidth pop	% get text width
X    text length div 5 div	% divide by text length and take 20%
X    /diff exch def		% define it as diff
X    1 -0.1 0 {
X      /degree exch def		% define current degree as ``degree''
X      gsave
X        diff degree mul		% X:diff*degree
X        dup neg rmoveto		% Y:-diff*degree rmoveto
X        degree setgray		% set gray scale
X        text show
X      grestore
X    } for
X    currentpoint		% push current point
X      1 setgray			% white
X      text show
X    moveto			% pop current point
X    0 setgray			% black
X    text true charpath		% draw outline of font
X    diff degree mul setlinewidth
X    stroke
X  grestore
X  text stringwidth pop		% get text width
X  0 rmoveto			% moveto after the text
X%   end
X}def
X
X%
X% END of shadowshow definition
X%
X%
X% PStick for dvi2ps-j, version 1.0 (postamble)
X%
Xend	% ShowDict
Xend	% TeXDict
X% Redefine showpage (using command defined in ShowDict)
XTeXDict begin
X/EP { ShowDict begin oldEP  wwfsrotate end } bind def
Xend
XShowDict begin  wwfsrotate end	% So the first page has the text
X%%PaperSize: A4
X%%Feature: *ManualFeed False
X%%Feature: *Resolution 400
XTeXDict begin
X@large
X@a4
X%%BeginFile: /usr/local/lib/dvi2ps/ASCII-LW-fix.ps
X% This file is included in this dvi2ps (j-version) kit by the courtesy of
X% Matsushita Electric Industrial Co., Ltd. You are free to distribute this
X% file as a part of this dvi2ps kit, but if you use this file as a part of
X% your own program and want to re-distribute the program, you should contact
X%	miyabe@sy6.isl.mei.co.jp, kakiuchi@sy6.isl.mei.co.jp
X%	
X%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
X%%% Following procedures are used to define new japanese font which has 
X%%% correct metrics.
X%%%		Added by T.Kakiuchi (kakiuchi@sy6.isl.mei.co.JP) 89/5/19
X%%%		Copyright (c) Matsushita Electric Industrial Co.,Ltd. 1990
X%%%
X% If the packed-array facility is available, this program sets the
X% array-packing mode.
Xsystemdict /setpacking known {
X    /savepacking currentpacking def
X    true setpacking
X} if
X% Procedure to make a new font which has new metrics.
X
X/Set-New-Metrics 		% [New-FNam][FNam][Metrics-array]
X{/Set-Metrics exch def		% [New-FNam][FNam]  Set-Metrix=[Metrix-Array]
X findfont 			% [New-FNam][FDic]
X /OrigCompFont exch def		% [New-FNam]  OrigCompFont=[FDic]
X /NewCompFontName exch def	% NewCompFontName=[New-FNam]
X
X % Create new composite font.
X OrigCompFont dup		% [FDic][FDic]
X length dict			% [FDic][dict]
X /NewCompFont exch def		% [FDic]   NewCompFont=[dict]
X {1 index			% [FDic][key][value][key]
X   /FID ne			% [FDic][key][value][T/F]
X   2 index /UniqueID ne and     % [FDic][key][value][T/F]
X  {NewCompFont 3 1 roll put}	% if TRUE   NewCompFont <- {key,value}
X  {pop pop}
X  ifelse
X } forall			% 
X
X OrigCompFont /FDepVector get	% [FDepVector]
X dup length array 		% [FDepVector][array]
X copy				% [array] (=[FDepVector])
X /NewFDepVector exch def	% NewFDepVector=[arrray]
X NewCompFont /FDepVector 
X NewFDepVector put		% [NewCompFont] <- {/FDepVector, NewFDepVector}
X NewCompFontName NewCompFont 
X definefont pop			% Define [NewCompFont] as [NewCompFontName]
X
X Set-Metrics	% [code,NewBaseFontName,Metrics-Array]
X {dup 0 get /Code exch def	% set Code
X  dup 1 get 
X  /NewBaseFontName exch def     % set NewBaseFontName
X  2 get /Metric-Array exch def	% set Metrics-Array
X
X  % Extract base font and its index to FDepVector.
X  /Index
X     OrigCompFont 
X     /Encoding get 
X     Code get def
X
X  /OrigBaseFont NewFDepVector Index get def
X
X  % Set Metric-Dictionary
X  Metric-Array length dict
X  /Metric-Dict exch def
X  Metric-Dict begin Metric-Array {exec def} forall end
X
X  % Create new base font.
X  OrigBaseFont dup			% [OrgFont][OrgFont] 
X  length 2 add	 			% [OrgFont][1+current size]
X  dict					% [OrgFont][dict]
X  /NewBaseFont exch def			% [OrgFont]    /NewBaseFont=[dict]
X  {1 index                       	% [OrgFont][key][value][key]
X     /FID ne				% [OrgFont][key][value][T/F]
X     2 index /UniqueID ne and           % [OrgFont][key][value][T/F]
X     {NewBaseFont			% [OrgFont][key][value][NewBaseFont]
X      3 1 roll				% [OrgFont][NewBaseFont][key][value]
X      put}					% [OrgFont]
X     {pop pop}
X     ifelse
X  } forall
X  
X  NewBaseFont /Kusumi-Metrics Metric-Dict put
X
X  NewBaseFont				% [OrgFont][NewBaseFont]
X  /CDevProc
X  {currentfont /Kusumi-Metrics get
X   exch 2 copy known
X    {get	% w0x w0y llx lly urx ury w1x w1y vx vy new-llx
X     dup	% w0x w0y llx lly urx ury w1x w1y vx vy new-llx new-llx
X     8 -1 roll	% w0x w0y llx lly ury w1x w1y vx vy new-llx new-llx urx
X     10 -1 roll	% w0x w0y lly ury w1x w1y vx vy new-llx new-llx urx llx
X     sub add	% w0x w0y lly ury w1x w1y vx vy new-llx new-urx
X     7 1 roll	% w0x w0y lly new-urx ury w1x w1y vx vy new-llx
X     8 1 roll	% w0x w0y new-llx lly new-urx ury w1x w1y vx vy
X    }
X    {pop pop}
X   ifelse
X  } put
X
X  %  Regist to FDepVector
X  NewFDepVector Index NewBaseFontName NewBaseFont definefont put
X
X } forall
