X@hosts = ('sh.wide.ad.jp', 'ftp.iij.ad.jp', 'utsun.s.u-tokyo.ac.jp',
X	  'ftp.center.osaka-u.ac.jp', 'ftp.ics.es.osaka-u.ac.jp');
X
X@sorted = &gtr'sort(@hosts);
X
Xprint join("\t", @sorted), "\n";
X
