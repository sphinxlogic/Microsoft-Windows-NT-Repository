  if test 52856 -ne `wc -c <'lic.1.3/doc/siggraph93/p.ps.Z.UU.B'`; then
    echo shar: \"'lic.1.3/doc/siggraph93/p.ps.Z.UU.B'\" unpacked with wrong size!
  elif test -f 'lic.1.3/doc/siggraph93/p.ps.Z.UU.A'; then
    echo shar: Combining  \"'lic.1.3/doc/siggraph93/p.ps.Z.UU'\" \(102438 characteters\)
    cat 'lic.1.3/doc/siggraph93/p.ps.Z.UU.A' 'lic.1.3/doc/siggraph93/p.ps.Z.UU.B' > 'lic.1.3/doc/siggraph93/p.ps.Z.UU'
    if test 102438 -ne `wc -c <'lic.1.3/doc/siggraph93/p.ps.Z.UU'`; then
      echo shar: \"'lic.1.3/doc/siggraph93/p.ps.Z.UU'\" combined with wrong size!
    else
      rm lic.1.3/doc/siggraph93/p.ps.Z.UU.A lic.1.3/doc/siggraph93/p.ps.Z.UU.B
      echo shar: Uudecoding \"'lic.1.3/doc/siggraph93/p.ps.Z'\" \(74311 characters\)
      cat lic.1.3/doc/siggraph93/p.ps.Z.UU | uudecode
      if test 74311 -ne `wc -c <'lic.1.3/doc/siggraph93/paper.ps.Z'`; then
        echo shar: \"'lic.1.3/doc/siggraph93/p.ps.Z.UU'\" uudecoded with wrong sisize!
      else
        rm lic.1.3/doc/siggraph93/p.ps.Z.UU
      fi 
      # end of 'lic.1.3/doc/siggraph93/p.ps.Z.UU'
    fi
  fi
  # end of 'lic.1.3/doc/siggraph93/p.ps.Z.UU.B'
fi
if test ! -d 'lic.1.3/include' ; then
    echo shar: Creating directory \"'lic.1.3/include'\"
    mkdir 'lic.1.3/include'
fi
if test ! -d 'lic.1.3/liblic' ; then
    echo shar: Creating directory \"'lic.1.3/liblic'\"
    mkdir 'lic.1.3/liblic'
fi
if test ! -d 'lic.1.3/lic' ; then
    echo shar: Creating directory \"'lic.1.3/lic'\"
    mkdir 'lic.1.3/lic'
fi
if test ! -d 'lic.1.3/test' ; then
    echo shar: Creating directory \"'lic.1.3/test'\"
    mkdir 'lic.1.3/test'
fi
echo shar: End of archive 1 \(of 9\).
cp /dev/null ark1isdone
MISSING=""
for I in 1 2 3 4 5 6 7 8 9 ; do
    if test ! -f ark${I}isdone ; then
	MISSING="${MISSING} ${I}"
    fi
done
if test "${MISSING}" = "" ; then
    echo You have unpacked all 9 archives.
    rm -f ark[1-9]isdone ark[1-9][0-9]isdone
else
    echo You still must unpack the following archives:
    echo "        " ${MISSING}
fi
exit 0
exit 0 # Just in case...
