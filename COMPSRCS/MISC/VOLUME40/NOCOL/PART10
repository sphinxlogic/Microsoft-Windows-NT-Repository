if test -f 'nocol-3.0/src/eventselect/eventselect.c' -a "${1}" != "-c" ; then 
  echo shar: Will not clobber existing file \"'nocol-3.0/src/eventselect/eventselect.c'\"
else
  echo shar: Extracting \"'nocol-3.0/src/eventselect/eventselect.c'\" \(4753 characters\)
  sed "s/^X//" >'nocol-3.0/src/eventselect/eventselect.c' <<'END_OF_FILE'
X/* $Header: /home/aggarwal/lsrc/nocol/src/eventselect/RCS/eventselect.c,v 1.4 1993/10/30 03:18:11 aggarwal Exp $ */
X
X/*+ 
X ** Program to parse all NOCOL data files and print out (in LOG format)
X ** all events matching the specified (command line) criterion.
X **
X **	-vikas@jvnc.net		Sep 27, 1993
X **/
X
X/* Copyright 1993 JvNCnet, Global Enterprise Services */
X
X/*
X * $Log: eventselect.c,v $
X * Revision 1.4  1993/10/30  03:18:11  aggarwal
X * Added copyright
X *
X * Revision 1.3  1993/10/22  14:24:07  aggarwal
X * Now also handles numbers as levels.
X *
X * Revision 1.2  1993/10/14  16:59:10  aggarwal
X * Small change in the usage message.
X *
X * Revision 1.1  1993/10/02  05:23:01  aggarwal
X * Initial revision
X *
X */
X
X#ifndef lint
X# define rcsid[] = "$Id: eventselect.c,v 1.4 1993/10/30 03:18:11 aggarwal Exp $" ;
X#endif
X
X#include "nocol.h"
X#include "fcntl.h"		/* for O_RDONLY definition */
X
Xint	logevents, debugflag, minseverity = E_INFO ;
Xstatic char 	*myname ;
Xtime_t	fromtime, totime ;	/* user specified */
X
Xchar *(senderlist[40]);		/* list of command-line sender names */
Xchar *(sitenamelist[40]);
X
Xmain(argc, argv)
X     int argc;
X     char **argv;
X{
X    extern char *optarg;
X    extern int  optind;
X    int c, sitei =0, senderi =0 ;
X
X    myname = argv[0] ;
X
X    while ((c = getopt(argc, argv, "dlv:s:f:t:S:")) != EOF)
X      switch(c)
X      {
X       case 'd':
X          debugflag++;
X	  fprintf(stderr, "%s: DEBUG turned on\n", myname);
X          break;
X       case 'l':
X	  logevents++ ;		/* send to noclogd logging daemon */
X          break ;
X       case 'v':
X	  switch(tolower(optarg[0]))
X	  {
X	   case 'c': case '1': minseverity = E_CRITICAL; break;
X	   case 'e': case '2': minseverity = E_ERROR; break;
X	   case 'w': case '3': minseverity = E_WARNING; break;
X	   case 'i': case '4': minseverity = E_INFO; break;
X	  }
X          break ;
X       case 's':
X	  senderlist[senderi++] = optarg ;
X          break ;
X       case 'S':
X	  sitenamelist[sitei++] = optarg ;
X          break ;
X       case 'f':			/* From time */
X	  if (optarg[0] == '+')		/* time given as delta seconds */
X	  {
X	      time_t  t = time((long *)NULL);
X	      fromtime = t - atoi(++optarg) ;
X	  }
X	  else
X	    fromtime = get_date(optarg, NULL) ;
X	  if (fromtime < 0)
X	    fromtime = 0;
X          break ;
X       case 't':			/* To time */
X	  if (optarg[0] == '+')		/* time given as delta seconds */
X	  {
X	      time_t  t = time((long *)NULL);
X	      totime = t - atoi(++optarg) ;
X	  }
X	  else
X	    totime = get_date(optarg, NULL) ;
X          break ;
X       case '?':
X       default:
X          fprintf(stderr, "%s: Unknown flag: %c\n", myname, optarg);
X          fprintf(stderr, "Usage: %s [-d (debug)] [-l (logevents)]\n", myname);
X          fprintf(stderr, "\t[-v <severity as Critical/Error/Warn/Info>]\n");
X	  fprintf(stderr, "\t[-s <sendername>] [-S <sitename>]\n");
X	  fprintf(stderr, "\t[-f <FromTime>] [-t <ToTime>] ");
X	  fprintf(stderr, "(can specify deltaTimeSecs as +300)\n");
X	  fprintf(stderr, "[data-files if not from stdin]\n");
X          exit (1);
X      }
X
X    /*
X     * Check times
X     */
X
X    if (totime <= 0)
X      totime = time((long *)NULL) + 100000;	/* curtime + large number*/
X    if (fromtime  &&  totime  &&  totime <= fromtime)
X    {
X	fprintf(stderr,	"%s: Ignoring EndTime since specified EndTime %s",
X		myname, ctime(&totime));
X	fprintf(stderr, "\t is less than FromTime %s\n", ctime(&fromtime) );
X	totime = time((long *)NULL) + 100000;
X    }
X
X    if (debugflag)
X    {
X	char **s ;
X
X	fprintf(stderr, "%s (debug):\n", myname);
X	fprintf(stderr, "\tlogevents= %s, MinSeverity= %s\n",
X		logevents ? "YES" : "NO", severity_txt[minseverity]);
X
X	fprintf(stderr, "\tFrom= %s",ctime(&fromtime));
X	fprintf(stderr, "\tTo= %s", ctime(&totime));
X	fprintf(stderr, "\tSenders= ");
X	if ((s = senderlist) && *s)
X	  fprintf(stderr, "%s /", *s++);
X	fprintf(stderr, "\n");
X	fprintf(stderr, "\tSitenames= ");
X	if ((s = sitenamelist) && *s)
X	  fprintf(stderr, "%s /", *s++);
X	fprintf(stderr, "\n");
X    }
X
X    if (optind >= argc)			/* no command line args */
X      proc_datafile (fileno(stdin));	/* process STDIN */
X    else                                /* specified input files  */
X      while ( optind < argc )
X      {
X	  int fd ;
X
X          if (debugflag)
X            fprintf(stderr,"debug (main): processing input file %s\n",
X                    argv[optind]);
X          if ((fd = open(argv[optind], O_RDONLY)) < 0) /* Cannot open */
X          {
X              fprintf(stderr,"%s: Error in read open ", myname);
X              perror(argv[optind]);
X          }
X          else
X          {
X              proc_datafile (fd);
X	      close(fd);
X          }
X          ++optind ;            /* next file    */
X      }                         /* end while()  */
X
X}	/* end:  main()  */
X
END_OF_FILE
