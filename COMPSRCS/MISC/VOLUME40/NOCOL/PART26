X
X#include <sys/types.h>
X#include <stdio.h>
X
Xstatic int kmem;
Xint swap, mem;
X
Xinit_kmem(file)
X    char *file;
X{
X    kmem = open(file, 0);
X    if (kmem < 0){
X	fprintf(stderr, "cannot open ");
X	perror(file);
X	exit(1);
X    }
X#ifdef ibm032
X    mem = open("/dev/mem");    
X    if (mem < 0){
X	fprintf(stderr, "cannot open ");
X	perror(file);
X	exit(1);
X    }
X    swap = open("/dev/drum");
X    if (swap < 0){
X	fprintf(stderr, "cannot open ");
X	perror(file);
X	exit(1);
X    }
X#endif
X}
X
X/*
X * Seek into the kernel for a value.
X */
Xoff_t
Xklseek(base)
X	off_t base;
X{
X	return (lseek(kmem, base, 0));
X}
X
Xklread(buf, buflen)
X    char *buf;
X    int buflen;
X{
X    read(kmem, buf, buflen);
X}
X
END_OF_FILE
