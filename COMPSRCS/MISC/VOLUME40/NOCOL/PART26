X
X${PROG} -v critical -f +10800 -t +7200 ${DATA} > ${TEMP}
X
Xif ( -z ${TEMP} ) then
X  /bin/rm -f ${TEMP}
Xelse
X  cat ${TEMP} | /usr/ucb/mail -s "Sites down for 2-3 hours" ${TO2}
X  /bin/rm -f ${TEMP}
Xendif
X
X##
X##
X${PROG} -v critical -f +14400 -t +10800 ${DATA} > ${TEMP}
X
