    echo shar: \"'IPP/IPP.ps.A'\" unpacked with wrong size!
  elif test -f 'IPP/IPP.ps.B'; then
    echo shar: Combining  \"'IPP/IPP.ps'\" \(125004 characters\)
    cat 'IPP/IPP.ps.A' 'IPP/IPP.ps.B' > 'IPP/IPP.ps'
    if test 125004 -ne `wc -c <'IPP/IPP.ps'`; then
      echo shar: \"'IPP/IPP.ps'\" combined with wrong size!
    else 
      rm IPP/IPP.ps.A IPP/IPP.ps.B
    fi 
  fi
