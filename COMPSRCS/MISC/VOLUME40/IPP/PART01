  sed "s/^X//" >'IPP/router/RouterIo.c' <<'END_OF_FILE'
X/* --------------------------------------------------------------------------
X * Ident: RouterIo.c
X * Author: Damiano Bolla 1993
X * All this project is covered by the GNU Copyright.
X */
X
X#include <stdio.h>
X#include <sys/types.h>
X#include <sys/time.h>
X#include <defs.h>
X#include <debug.h>
X#include <packet.h>
X#include <router.h>
X#include <func_defs.h>
X
X/* This function knows that there is something to process and then will doit
X * This function will either process a packet or accept a ringing
X */
X
Xint RouterIo ( struct Routing *rinfo )
X   {
X   int c;
X   int  Newsocket;
X   char Msg[CONF_LLEN+2];
X
X   for (c=1; c <=IF_MAX; c++ ) 
X       if ( FD_ISSET (rinfo->ifdata[c].Line, &rinfo->readfds) )
X          ProcessPack ( rinfo, c );
X
X   /* The function RoParseCall is in charge of closing the new socket if not needed	*/
X   for (c=1; c <=IF_MAX; c++ ) 
X      if ( FD_ISSET (rinfo->ifdata[c].Ring, &rinfo->readfds) )
X         if ( Accept ( rinfo->ifdata[c].Ring, &Newsocket, Msg, CONF_LLEN ) == DONE )
X            ProcessCall ( rinfo, c, Newsocket, Msg );
X
X
X   return (DONE);
X   }
END_OF_FILE
  if test 1079 -ne `wc -c <'IPP/router/RouterIo.c'`; then
    echo shar: \"'IPP/router/RouterIo.c'\" unpacked with wrong size!
  fi
  # end of 'IPP/router/RouterIo.c'
fi
