  if test 61089 -ne `wc -c <'patch.10.B.UU'`; then
    echo shar: \"'patch.10.B.UU'\" unpacked with wrong size!
  else
    echo shar: Uudecoding \"'patch.10.B.UU'\" \(44318 characters\)
    cat patch.10.B.UU | uudecode
    if test 44318 -ne `wc -c <'patch.10.B'`; then
      echo shar: \"'patch.10.B'\" uudecoded with wrong size!
    else
      rm patch.10.B.UU
      if test -f 'patch.10.A' && test -f patch.10.C ; then
        echo shar: Combining  \"'patch.10'\" \(95017 characters\)
        cat 'patch.10.A' 'patch.10.B' 'patch.10.C' > 'patch.10'
        if test 95017 -ne `wc -c <'patch.10'`; then
          echo shar: \"'patch.10'\" combined with wrong size!
        else
          rm patch.10.A patch.10.B patch.10.C 
        fi
      fi
    fi
  fi
  # end of 'patch.10.B.UU'
fi
echo shar: End of archive 1 \(of 3\).
cp /dev/null ark1isdone
MISSING=""
for I in 1 2 3 ; do
    if test ! -f ark${I}isdone ; then
	MISSING="${MISSING} ${I}"
    fi
done
if test "${MISSING}" = "" ; then
    echo You have unpacked all 3 archives.
    rm -f ark[1-9]isdone
else
    echo You still must unpack the following archives:
    echo "        " ${MISSING}
fi
exit 0
exit 0 # Just in case...
