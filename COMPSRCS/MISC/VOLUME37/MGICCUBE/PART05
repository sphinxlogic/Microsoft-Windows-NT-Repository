XDependSubdirs($(SUBDIRS))
X
X
X
X############################################################################
X#
X#
X#
XAllTarget(magicCube)
XNormalProgramTarget(magicCube,$(OBJS),$(DEPLIBS),$(XSLIB),$(SYS_LIBRARIES))
X
Xmerge_resource:
X	xrdb -merge MagicCube
X   
