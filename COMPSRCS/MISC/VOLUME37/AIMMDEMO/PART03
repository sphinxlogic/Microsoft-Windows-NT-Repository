END_OF_FILE
  if test 21080 -ne `wc -c <'gen_a11.exe.UU'`; then
    echo shar: \"'gen_a11.exe.UU'\" unpacked with wrong size!
  else
    echo shar: Uudecoding \"'gen_a11.exe'\" \(15277 characters\)
    cat gen_a11.exe.UU | uudecode
    if test 15277 -ne `wc -c <'gen_a11.exe'`; then
      echo shar: \"'gen_a11.exe'\" uudecoded with wrong size!
    else
      rm gen_a11.exe.UU
    fi
  fi
  # end of 'gen_a11.exe.UU'
fi
echo shar: End of archive 3 \(of 3\).
cp /dev/null ark3isdone
MISSING=""
for I in 1 2 3 ; do
    if test ! -f ark${I}isdone ; then
	MISSING="${MISSING} ${I}"
    fi
done
if test "${MISSING}" = "" ; then
    echo You have unpacked all 3 archives.
    rm -f ark[1-9]isdone
else
    echo You still must unpack the following archives:
    echo "        " ${MISSING}
fi
exit 0
exit 0 # Just in case...
